      cov_ri5photsx().f[6]++;
      cov_ri5photsx().s[23]++;
      return {
        ...prevState,
        theme: {
          ...preset,
          ...prevState.customizations
        },
        currentPreset: presetName
      };
    });
  }, []));
  // Actualizar tema personalizado
  const updateTheme =
  /* istanbul ignore next */
  (cov_ri5photsx().s[24]++, (0, _react.useCallback)(updates => {
    /* istanbul ignore next */
    cov_ri5photsx().f[7]++;
    cov_ri5photsx().s[25]++;
    setState(prevState => {
      /* istanbul ignore next */
      cov_ri5photsx().f[8]++;
      const newCustomizations =
      /* istanbul ignore next */
      (cov_ri5photsx().s[26]++, {
        ...prevState.customizations,
        ...updates
      });
      /* istanbul ignore next */
      cov_ri5photsx().s[27]++;
      return {
        ...prevState,
        theme: {
          ...presetConfigs[prevState.currentPreset],
          ...newCustomizations
        },
        customizations: newCustomizations
      };
    });
  }, []));
  // Actualizar colores específicos
  const updateColors =
  /* istanbul ignore next */
  (cov_ri5photsx().s[28]++, (0, _react.useCallback)(colors => {
    /* istanbul ignore next */
    cov_ri5photsx().f[9]++;
    cov_ri5photsx().s[29]++;
    updateTheme({
      colors: {
        ...state.theme.colors,
        ...colors
      }
    });
  }, [state.theme.colors, updateTheme]));
  // Actualizar defaults de componentes
  const updateComponentDefaults =
  /* istanbul ignore next */
  (cov_ri5photsx().s[30]++, (0, _react.useCallback)((component, defaults) => {
    /* istanbul ignore next */
    cov_ri5photsx().f[10]++;
    cov_ri5photsx().s[31]++;
    setState(prevState => {
      /* istanbul ignore next */
      cov_ri5photsx().f[11]++;
      cov_ri5photsx().s[32]++;
      return {
        ...prevState,
        componentDefaults: {
          ...prevState.componentDefaults,
          [component]: {
            ...prevState.componentDefaults[component],
            ...defaults
          }
        }
      };
    });
  }, []));
  // Resetear a configuración por defecto
  const resetToDefault =
  /* istanbul ignore next */
  (cov_ri5photsx().s[33]++, (0, _react.useCallback)(() => {
    /* istanbul ignore next */
    cov_ri5photsx().f[12]++;
    cov_ri5photsx().s[34]++;
    setState(initialState);
    /* istanbul ignore next */
    cov_ri5photsx().s[35]++;
    localStorage.removeItem('design-system-config');
  }, []));
  // Exportar configuración actual
  const exportConfig =
  /* istanbul ignore next */
  (cov_ri5photsx().s[36]++, (0, _react.useCallback)(() => {
    /* istanbul ignore next */
    cov_ri5photsx().f[13]++;
    cov_ri5photsx().s[37]++;
    return JSON.stringify(state, null, 2);
  }, [state]));
  // Importar configuración
  const importConfig =
  /* istanbul ignore next */
  (cov_ri5photsx().s[38]++, (0, _react.useCallback)(configJson => {
    /* istanbul ignore next */
    cov_ri5photsx().f[14]++;
    cov_ri5photsx().s[39]++;
    try {
      const config =
      /* istanbul ignore next */
      (cov_ri5photsx().s[40]++, JSON.parse(configJson));
      /* istanbul ignore next */
      cov_ri5photsx().s[41]++;
      setState(config);
      /* istanbul ignore next */
      cov_ri5photsx().s[42]++;
      return true;
    } catch (error) {
      /* istanbul ignore next */
      cov_ri5photsx().s[43]++;
      console.error('Error importing config:', error);
      /* istanbul ignore next */
      cov_ri5photsx().s[44]++;
      return false;
    }
  }, []));
  // Generar CSS variables para el tema actual
  const getCSSVariables =
  /* istanbul ignore next */
  (cov_ri5photsx().s[45]++, (0, _react.useCallback)(() => {
    /* istanbul ignore next */
    cov_ri5photsx().f[15]++;
    const {
      colors,
      fontSize,
      spacing
    } =
    /* istanbul ignore next */
    (cov_ri5photsx().s[46]++, state.theme);
    /* istanbul ignore next */
    cov_ri5photsx().s[47]++;
    return {
      '--color-primary': colors.primary,
      '--color-secondary': colors.secondary,
      '--color-success': colors.success,
      '--color-warning': colors.warning,
      '--color-error': colors.error,
      '--color-info': colors.info,
      '--color-background': colors.background,
      '--color-surface': colors.surface,
      '--color-text': colors.text,
      '--color-text-secondary': colors.textSecondary,
      '--color-border': colors.border,
      '--font-size-xs': fontSize.xs,
      '--font-size-sm': fontSize.sm,
      '--font-size-base': fontSize.base,
      '--font-size-lg': fontSize.lg,
      '--font-size-xl': fontSize.xl,
      '--font-size-2xl': fontSize['2xl'],
      '--font-size-3xl': fontSize['3xl'],
      '--spacing-xs': spacing.xs,
      '--spacing-sm': spacing.sm,
      '--spacing-md': spacing.md,
      '--spacing-lg': spacing.lg,
      '--spacing-xl': spacing.xl,
      '--spacing-2xl': spacing['2xl']
    };
  }, [state.theme]));
  /* istanbul ignore next */
  cov_ri5photsx().s[48]++;
  return {
    // Estado actual
    theme: state.theme,
    currentPreset: state.currentPreset,
    customizations: state.customizations,
    componentDefaults: state.componentDefaults,
    // Presets disponibles
    availablePresets: Object.keys(presetConfigs),
    // Acciones
    applyPreset,
    updateTheme,
    updateColors,
    updateComponentDefaults,
    resetToDefault,
    exportConfig,
    importConfig,
    getCSSVariables,
    // Utilidades
    isCustomized: Object.keys(state.customizations).length > 0,
    isDarkMode: state.currentPreset === 'dark'
  };
}
