    cov_2osvgg30i2().s[33]++;
    for (let attempt =
    /* istanbul ignore next */
    (cov_2osvgg30i2().s[34]++, 1); attempt <= retryConfig.maxRetries + 1; attempt++) {
      /* istanbul ignore next */
      cov_2osvgg30i2().s[35]++;
      try {
        /* istanbul ignore next */
