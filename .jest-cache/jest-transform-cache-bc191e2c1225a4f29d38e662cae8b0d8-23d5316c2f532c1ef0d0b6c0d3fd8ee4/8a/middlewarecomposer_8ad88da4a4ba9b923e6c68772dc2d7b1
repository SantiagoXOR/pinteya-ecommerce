    (cov_130k2fh29r().s[28]++, require('../validation/admin-schemas'));
    /* istanbul ignore next */
    cov_130k2fh29r().s[29]++;
    return composeMiddlewares(withErrorHandler, withApiLogging, withAdminAuth(permissions), withValidation(schema))(handler);
  };
};
