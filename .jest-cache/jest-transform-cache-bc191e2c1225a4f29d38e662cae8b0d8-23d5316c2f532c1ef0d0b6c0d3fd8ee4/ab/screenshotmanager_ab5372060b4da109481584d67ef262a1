  /**
  * Espera a que un elemento sea visible antes de capturar
  */
  async waitAndCapture(selector, filename, description, options) {
    /* istanbul ignore next */
    cov_25myxcx5qh().f[13]++;
    cov_25myxcx5qh().s[96]++;
