    }
  }
  /**
  * Captura múltiples screenshots con delay
  */
  async captureSequence(screenshots) {
    /* istanbul ignore next */
    cov_25myxcx5qh().f[14]++;
    const results =
    /* istanbul ignore next */
    (cov_25myxcx5qh().s[103]++, []);
    /* istanbul ignore next */
    cov_25myxcx5qh().s[104]++;
