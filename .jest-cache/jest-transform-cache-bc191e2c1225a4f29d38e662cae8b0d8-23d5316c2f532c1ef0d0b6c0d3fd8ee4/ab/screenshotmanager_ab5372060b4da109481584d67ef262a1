    cov_25myxcx5qh().s[69]++;
    try {
      const element =
      /* istanbul ignore next */
      (cov_25myxcx5qh().s[70]++, await this.page.locator(selector).first());
      /* istanbul ignore next */
      cov_25myxcx5qh().s[71]++;
      await element.waitFor({
        state: 'visible',
        timeout: 10000
      });
      const boundingBox =
      /* istanbul ignore next */
      (cov_25myxcx5qh().s[72]++, await element.boundingBox());
