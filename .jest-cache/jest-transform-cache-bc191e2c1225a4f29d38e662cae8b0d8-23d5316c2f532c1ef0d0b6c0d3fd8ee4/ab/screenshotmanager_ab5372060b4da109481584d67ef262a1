      const metadata =
      /* istanbul ignore next */
      (cov_25myxcx5qh().s[107]++, await this.captureScreenshot(screenshot.filename, screenshot.description, screenshot.options));
      /* istanbul ignore next */
      cov_25myxcx5qh().s[108]++;
      results.push(metadata);
    }
    /* istanbul ignore next */
    cov_25myxcx5qh().s[109]++;
    return results;
  }
