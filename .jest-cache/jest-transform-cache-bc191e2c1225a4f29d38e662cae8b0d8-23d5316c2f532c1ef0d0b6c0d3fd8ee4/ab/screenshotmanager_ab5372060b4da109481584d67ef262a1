  /**
  * Captura screenshot de un elemento específico
  */
  async captureElementScreenshot(selector, filename, description, options) {
    /* istanbul ignore next */
    cov_25myxcx5qh().f[10]++;
    cov_25myxcx5qh().s[67]++;
    if (!this.page) {
      /* istanbul ignore next */
      cov_25myxcx5qh().b[25][0]++;
      cov_25myxcx5qh().s[68]++;
