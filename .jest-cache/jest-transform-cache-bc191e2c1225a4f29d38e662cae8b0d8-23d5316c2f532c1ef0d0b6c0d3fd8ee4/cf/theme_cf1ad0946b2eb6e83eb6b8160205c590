  // Escuchar cambios en la preferencia del sistema
  watchSystemTheme(callback) {
    /* istanbul ignore next */
    cov_udrdx4wao().f[15]++;
    cov_udrdx4wao().s[53]++;
    if (typeof window === 'undefined') {
