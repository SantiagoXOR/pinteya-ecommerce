    const mediaQuery =
    /* istanbul ignore next */
    (cov_udrdx4wao().s[55]++, window.matchMedia('(prefers-color-scheme: dark)'));
    /* istanbul ignore next */
    cov_udrdx4wao().s[56]++;
    const handler = e => {
      /* istanbul ignore next */
      cov_udrdx4wao().f[17]++;
      cov_udrdx4wao().s[57]++;
