      /* istanbul ignore next */
      (cov_udrdx4wao().b[12][0]++, 'dark') :
      /* istanbul ignore next */
      (cov_udrdx4wao().b[12][1]++, 'light'));
    };
    /* istanbul ignore next */
    cov_udrdx4wao().s[58]++;
    mediaQuery.addEventListener('change', handler);
    // Retornar función de limpieza
    /* istanbul ignore next */
    cov_udrdx4wao().s[59]++;
    return () => {
      /* istanbul ignore next */
      cov_udrdx4wao().f[18]++;
      cov_udrdx4wao().s[60]++;
