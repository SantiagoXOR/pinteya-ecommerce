    /* istanbul ignore next */
    (cov_udrdx4wao().s[91]++, themeManager.getStoredTheme());
    const resolved =
    /* istanbul ignore next */
    (cov_udrdx4wao().s[92]++, themeManager.resolveTheme(storedTheme));
    /* istanbul ignore next */
    cov_udrdx4wao().s[93]++;
    setResolvedTheme(resolved);
    // Escuchar cambios en la preferencia del sistema
