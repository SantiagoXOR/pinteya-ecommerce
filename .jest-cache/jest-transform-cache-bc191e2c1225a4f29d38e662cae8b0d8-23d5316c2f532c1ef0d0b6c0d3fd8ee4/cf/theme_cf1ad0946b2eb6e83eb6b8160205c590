  /* istanbul ignore next */
  (cov_udrdx4wao().s[61]++, (0, _react.useState)('system'));
  const [resolvedTheme, setResolvedTheme] =
  /* istanbul ignore next */
  (cov_udrdx4wao().s[62]++, (0, _react.useState)('light'));
