    return _server.NextResponse.json({
      success: true,
      data: {
        products: transformedProducts,
        total:
        /* istanbul ignore next */
        (cov_20w5jdwup4().b[3][0]++, count) ||
        /* istanbul ignore next */
        (cov_20w5jdwup4().b[3][1]++, 0),
        pagination: {
          limit: 10,
          offset: 0,
          hasMore: (
          /* istanbul ignore next */
          (cov_20w5jdwup4().b[4][0]++, count) ||
          /* istanbul ignore next */
          (cov_20w5jdwup4().b[4][1]++, 0)) > 10
        }
      },
      debug: {
        timestamp: new Date().toISOString(),
        method: 'simple_supabase_query',
        auth: 'service_key'
      }
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_20w5jdwup4().s[24]++;
    console.error('‚ùå Error fatal en debug simple products:', error);
    /* istanbul ignore next */
    cov_20w5jdwup4().s[25]++;
    return _server.NextResponse.json({
      success: false,
      error: error instanceof Error ?
      /* istanbul ignore next */
      (cov_20w5jdwup4().b[5][0]++, error.message) :
      /* istanbul ignore next */
      (cov_20w5jdwup4().b[5][1]++, 'Error desconocido'),
      debug: {
        timestamp: new Date().toISOString(),
        method: 'simple_supabase_query',
        auth: 'service_key'
      }
    }, {
      status: 500
    });
  }
}
