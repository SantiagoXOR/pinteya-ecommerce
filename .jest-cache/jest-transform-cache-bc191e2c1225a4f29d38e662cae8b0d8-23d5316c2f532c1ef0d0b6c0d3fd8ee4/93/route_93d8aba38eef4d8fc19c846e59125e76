async function getProductsForSitemap() {
  /* istanbul ignore next */
  cov_b5tbe6jd3().f[7]++;
  cov_b5tbe6jd3().s[59]++;
  try {
    const supabase =
    /* istanbul ignore next */
    (cov_b5tbe6jd3().s[60]++, (0, _supabase.getSupabaseClient)());
    /* istanbul ignore next */
    cov_b5tbe6jd3().s[61]++;
    if (!supabase) {
      /* istanbul ignore next */
      cov_b5tbe6jd3().b[7][0]++;
      cov_b5tbe6jd3().s[62]++;
      return [];
    } else
    /* istanbul ignore next */
    {
      cov_b5tbe6jd3().b[7][1]++;
    }
    const {
      data: products,
      error
    } =
    /* istanbul ignore next */
    (cov_b5tbe6jd3().s[63]++, await supabase.from('products').select('slug, name, updated_at, images').eq('is_active', true).order('updated_at', {
      ascending: false
    }).limit(1000)); // Limitar para performance
    /* istanbul ignore next */
    cov_b5tbe6jd3().s[64]++;
    if (error) {
      /* istanbul ignore next */
      cov_b5tbe6jd3().b[8][0]++;
      cov_b5tbe6jd3().s[65]++;
      _logger.logger.warn(_logger.LogLevel.WARN, 'Error fetching products for sitemap', {}, _logger.LogCategory.SEO);
      /* istanbul ignore next */
      cov_b5tbe6jd3().s[66]++;
      return [];
    } else
    /* istanbul ignore next */
    {
      cov_b5tbe6jd3().b[8][1]++;
    }
    cov_b5tbe6jd3().s[67]++;
    return /* istanbul ignore next */(cov_b5tbe6jd3().b[9][0]++, products) ||
    /* istanbul ignore next */
    (cov_b5tbe6jd3().b[9][1]++, []);
  } catch (error) {
    /* istanbul ignore next */
    cov_b5tbe6jd3().s[68]++;
    _logger.logger.error(_logger.LogLevel.ERROR, 'Error in getProductsForSitemap', error, _logger.LogCategory.SEO);
    /* istanbul ignore next */
