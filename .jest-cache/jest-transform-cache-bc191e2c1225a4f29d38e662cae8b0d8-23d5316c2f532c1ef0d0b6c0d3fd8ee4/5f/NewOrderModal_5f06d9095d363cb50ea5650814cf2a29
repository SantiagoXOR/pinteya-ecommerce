      }), /*#__PURE__*/(0, _jsxruntime.jsxs)(_dialog.DialogFooter, {
        className: "flex justify-between",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "flex gap-2",
          children:
          /* istanbul ignore next */
          (cov_oyb5qkt53().b[50][0]++, currentStep > 1) &&
          /* istanbul ignore next */
          (cov_oyb5qkt53().b[50][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)(_button.Button, {
            variant: "outline",
            onClick: () => {
              /* istanbul ignore next */
              cov_oyb5qkt53().f[49]++;
              cov_oyb5qkt53().s[196]++;
              return setCurrentStep(prev => {
                /* istanbul ignore next */
                cov_oyb5qkt53().f[50]++;
                cov_oyb5qkt53().s[197]++;
                return prev - 1;
              });
            },
            disabled: isLoading,
            children: "Anterior"
          }))
        }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          className: "flex gap-2",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_button.Button, {
            variant: "outline",
            onClick: handleClose,
            disabled: isLoading,
            children: "Cancelar"
          }), currentStep < 3 ?
          /* istanbul ignore next */
          (/*#__PURE__*/cov_oyb5qkt53().b[51][0]++, (0, _jsxruntime.jsx)(_button.Button, {
            onClick: () => {
              /* istanbul ignore next */
              cov_oyb5qkt53().f[51]++;
              cov_oyb5qkt53().s[198]++;
              return setCurrentStep(prev => {
                /* istanbul ignore next */
                cov_oyb5qkt53().f[52]++;
                cov_oyb5qkt53().s[199]++;
                return prev + 1;
              });
            },
            disabled:
            /* istanbul ignore next */
            (cov_oyb5qkt53().b[52][0]++, isLoading) ||
            /* istanbul ignore next */
            (cov_oyb5qkt53().b[52][1]++, currentStep === 1) &&
            /* istanbul ignore next */
            (cov_oyb5qkt53().b[52][2]++, !orderData.customer) ||
            /* istanbul ignore next */
            (cov_oyb5qkt53().b[52][3]++, currentStep === 2) &&
            /* istanbul ignore next */
            (cov_oyb5qkt53().b[52][4]++, orderData.items.length === 0),
            children: "Siguiente"
          })) :
          /* istanbul ignore next */
          (/*#__PURE__*/cov_oyb5qkt53().b[51][1]++, (0, _jsxruntime.jsx)(_button.Button, {
            onClick: createOrder,
            disabled:
            /* istanbul ignore next */
            (cov_oyb5qkt53().b[53][0]++, isLoading) ||
            /* istanbul ignore next */
            (cov_oyb5qkt53().b[53][1]++, !orderData.customer) ||
            /* istanbul ignore next */
            (cov_oyb5qkt53().b[53][2]++, orderData.items.length === 0),
            children: isLoading ?
            /* istanbul ignore next */
            (cov_oyb5qkt53().b[54][0]++, 'Creando...') :
            /* istanbul ignore next */
            (cov_oyb5qkt53().b[54][1]++, 'Crear Orden')
          }))]
        })]
      })]
    })
  });
};
