  // =====================================================
  // MANEJO DE MENSAJES
  // =====================================================
  handleMessage(message) {
    /* istanbul ignore next */
    cov_io4p4lvpc().f[25]++;
    cov_io4p4lvpc().s[74]++;
    console.log('ğ¨ WebSocket message received:', message.type);
    /* istanbul ignore next */
    cov_io4p4lvpc().s[75]++;
    switch (message.type) {
      case 'tracking_update':
        /* istanbul ignore next */
        cov_io4p4lvpc().b[8][0]++;
        cov_io4p4lvpc().s[76]++;
        this.handleTrackingUpdate(message.data);
        /* istanbul ignore next */
        cov_io4p4lvpc().s[77]++;
        break;
      case 'shipment_status':
        /* istanbul ignore next */
        cov_io4p4lvpc().b[8][1]++;
        cov_io4p4lvpc().s[78]++;
        this.handleShipmentStatus(message.data);
        /* istanbul ignore next */
        cov_io4p4lvpc().s[79]++;
        break;
      case 'alert':
        /* istanbul ignore next */
        cov_io4p4lvpc().b[8][2]++;
        cov_io4p4lvpc().s[80]++;
        this.handleAlert(message.data);
        /* istanbul ignore next */
        cov_io4p4lvpc().s[81]++;
        break;
      case 'geofence_event':
        /* istanbul ignore next */
        cov_io4p4lvpc().b[8][3]++;
        cov_io4p4lvpc().s[82]++;
        this.handleGeofenceEvent(message.data);
        /* istanbul ignore next */
        cov_io4p4lvpc().s[83]++;
        break;
      case 'route_update':
        /* istanbul ignore next */
        cov_io4p4lvpc().b[8][4]++;
        cov_io4p4lvpc().s[84]++;
        this.handleRouteUpdate(message.data);
        /* istanbul ignore next */
        cov_io4p4lvpc().s[85]++;
        break;
      default:
        /* istanbul ignore next */
        cov_io4p4lvpc().b[8][5]++;
        cov_io4p4lvpc().s[86]++;
        console.log('ğ¤· Unknown message type:', message.type);
    }
    // Emitir evento genÃ©rico
    /* istanbul ignore next */
    cov_io4p4lvpc().s[87]++;
    this.emit('message', message);
  }
  handleTrackingUpdate(update) {
    /* istanbul ignore next */
    cov_io4p4lvpc().f[26]++;
    cov_io4p4lvpc().s[88]++;
    console.log('ğ Tracking update:', update);
