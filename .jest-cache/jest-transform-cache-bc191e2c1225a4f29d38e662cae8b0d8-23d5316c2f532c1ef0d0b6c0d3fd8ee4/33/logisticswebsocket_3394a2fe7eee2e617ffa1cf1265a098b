let logisticsWS =
/* istanbul ignore next */
(cov_io4p4lvpc().s[109]++, null);
function getLogisticsWebSocket() {
  /* istanbul ignore next */
  cov_io4p4lvpc().f[34]++;
  cov_io4p4lvpc().s[110]++;
  if (!logisticsWS) {
    /* istanbul ignore next */
    cov_io4p4lvpc().b[14][0]++;
    // En desarrollo usar WebSocket local, en producciÃ³n usar WSS
    const wsUrl =
    /* istanbul ignore next */
    (cov_io4p4lvpc().s[111]++, process.env.NODE_ENV === 'development' ?
    /* istanbul ignore next */
    (cov_io4p4lvpc().b[15][0]++, 'ws://localhost:3001/logistics') :
    /* istanbul ignore next */
    (cov_io4p4lvpc().b[15][1]++, 'wss://api.pinteya.com/logistics'));
    /* istanbul ignore next */
    cov_io4p4lvpc().s[112]++;
    logisticsWS = new LogisticsWebSocketClient(wsUrl);
  } else
  /* istanbul ignore next */
  {
    cov_io4p4lvpc().b[14][1]++;
  }
  cov_io4p4lvpc().s[113]++;
  return logisticsWS;
}
class LogisticsWebSocketSimulator extends
/* istanbul ignore next */
(_events.EventEmitter) {
  start() {
    /* istanbul ignore next */
    cov_io4p4lvpc().f[35]++;
    cov_io4p4lvpc().s[114]++;
    if (this.isRunning) {
      /* istanbul ignore next */
      cov_io4p4lvpc().b[16][0]++;
      cov_io4p4lvpc().s[115]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_io4p4lvpc().b[16][1]++;
    }
    cov_io4p4lvpc().s[116]++;
    this.isRunning = true;
    /* istanbul ignore next */
    cov_io4p4lvpc().s[117]++;
    console.log('ğ­ Iniciando simulador WebSocket');
    // Simular actualizaciones de tracking cada 10 segundos
    const trackingInterval =
    /* istanbul ignore next */
    (cov_io4p4lvpc().s[118]++, setInterval(() => {
      /* istanbul ignore next */
      cov_io4p4lvpc().f[36]++;
      cov_io4p4lvpc().s[119]++;
      this.simulateTrackingUpdate();
    }, 10000));
    // Simular alertas cada 30 segundos
    const alertInterval =
    /* istanbul ignore next */
    (cov_io4p4lvpc().s[120]++, setInterval(() => {
      /* istanbul ignore next */
      cov_io4p4lvpc().f[37]++;
      cov_io4p4lvpc().s[121]++;
      this.simulateAlert();
    }, 30000));
    // Simular eventos de geofence cada 45 segundos
    const geofenceInterval =
    /* istanbul ignore next */
    (cov_io4p4lvpc().s[122]++, setInterval(() => {
      /* istanbul ignore next */
      cov_io4p4lvpc().f[38]++;
      cov_io4p4lvpc().s[123]++;
      this.simulateGeofenceEvent();
    }, 45000));
    /* istanbul ignore next */
    cov_io4p4lvpc().s[124]++;
    this.intervals.push(trackingInterval, alertInterval, geofenceInterval);
  }
  stop() {
    /* istanbul ignore next */
    cov_io4p4lvpc().f[39]++;
    cov_io4p4lvpc().s[125]++;
    this.intervals.forEach(interval => {
      /* istanbul ignore next */
      cov_io4p4lvpc().f[40]++;
      cov_io4p4lvpc().s[126]++;
      return clearInterval(interval);
    });
    /* istanbul ignore next */
    cov_io4p4lvpc().s[127]++;
    this.intervals = [];
    /* istanbul ignore next */
    cov_io4p4lvpc().s[128]++;
    this.isRunning = false;
    /* istanbul ignore next */
    cov_io4p4lvpc().s[129]++;
    console.log('ğ Simulador WebSocket detenido');
  }
  simulateTrackingUpdate() {
    /* istanbul ignore next */
    cov_io4p4lvpc().f[41]++;
    const update =
    /* istanbul ignore next */
    (cov_io4p4lvpc().s[130]++, {
      shipment_id: Math.floor(Math.random() * 100) + 1,
      location: {
        latitude: -34.6037 + (Math.random() - 0.5) * 0.1,
        longitude: -58.3816 + (Math.random() - 0.5) * 0.1,
        address: 'Av. Corrientes 1234, CABA'
      },
      status: ['in_transit', 'out_for_delivery'][Math.floor(Math.random() * 2)],
      timestamp: new Date().toISOString(),
