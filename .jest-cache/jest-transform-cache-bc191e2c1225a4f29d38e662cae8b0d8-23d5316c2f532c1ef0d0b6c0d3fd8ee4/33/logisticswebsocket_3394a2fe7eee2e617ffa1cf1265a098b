  }
  // =====================================================
  // UTILIDADES
  // =====================================================
  getConnectionState() {
    /* istanbul ignore next */
    cov_io4p4lvpc().f[31]++;
    cov_io4p4lvpc().s[100]++;
    if (!this.ws) {
      /* istanbul ignore next */
      cov_io4p4lvpc().b[11][0]++;
      cov_io4p4lvpc().s[101]++;
      return 'disconnected';
    } else
    /* istanbul ignore next */
    {
      cov_io4p4lvpc().b[11][1]++;
    }
    cov_io4p4lvpc().s[102]++;
    switch (this.ws.readyState) {
      case WebSocket.CONNECTING:
        /* istanbul ignore next */
        cov_io4p4lvpc().b[12][0]++;
        cov_io4p4lvpc().s[103]++;
        return 'connecting';
      case WebSocket.OPEN:
        /* istanbul ignore next */
        cov_io4p4lvpc().b[12][1]++;
        cov_io4p4lvpc().s[104]++;
        return 'connected';
      case WebSocket.CLOSING:
        /* istanbul ignore next */
        cov_io4p4lvpc().b[12][2]++;
      case WebSocket.CLOSED:
        /* istanbul ignore next */
        cov_io4p4lvpc().b[12][3]++;
        cov_io4p4lvpc().s[105]++;
        return 'disconnected';
      default:
        /* istanbul ignore next */
        cov_io4p4lvpc().b[12][4]++;
        cov_io4p4lvpc().s[106]++;
        return 'error';
    }
  }
  isReady() {
    /* istanbul ignore next */
    cov_io4p4lvpc().f[32]++;
    cov_io4p4lvpc().s[107]++;
    return /* istanbul ignore next */(cov_io4p4lvpc().b[13][0]++, this.isConnected) &&
    /* istanbul ignore next */
    (cov_io4p4lvpc().b[13][1]++, this.ws?.readyState === WebSocket.OPEN);
  }
  getSubscriptions() {
    /* istanbul ignore next */
    cov_io4p4lvpc().f[33]++;
    cov_io4p4lvpc().s[108]++;
    return Array.from(this.subscriptions);
  }
}
