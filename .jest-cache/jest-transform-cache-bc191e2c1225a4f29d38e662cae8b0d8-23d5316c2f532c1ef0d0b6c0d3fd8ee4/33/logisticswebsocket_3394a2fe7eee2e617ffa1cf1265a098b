  handleAlert(alert) {
    /* istanbul ignore next */
    cov_io4p4lvpc().f[28]++;
    cov_io4p4lvpc().s[92]++;
    console.log('ğ¨ Alert received:', alert);
    /* istanbul ignore next */
    cov_io4p4lvpc().s[93]++;
    this.emit('alert', alert);
    // Mostrar notificaciÃ³n del navegador si estÃ¡ permitido (DESHABILITADO EN DESARROLLO)
    /* istanbul ignore next */
    cov_io4p4lvpc().s[94]++;
    if (
    /* istanbul ignore next */
    (cov_io4p4lvpc().b[10][0]++, 'Notification' in window) &&
    /* istanbul ignore next */
    (cov_io4p4lvpc().b[10][1]++, Notification.permission === 'granted') &&
    /* istanbul ignore next */
    (cov_io4p4lvpc().b[10][2]++, process.env.NODE_ENV === 'production')) {
      /* istanbul ignore next */
      cov_io4p4lvpc().b[9][0]++;
      cov_io4p4lvpc().s[95]++;
      new Notification(`Alerta de LogÃ­stica - ${alert.type}`, {
        body: alert.message,
        icon: '/favicon.ico',
        tag: alert.id
      });
    } else
    /* istanbul ignore next */
    {
      cov_io4p4lvpc().b[9][1]++;
    }
  }
  handleGeofenceEvent(event) {
    /* istanbul ignore next */
    cov_io4p4lvpc().f[29]++;
    cov_io4p4lvpc().s[96]++;
    console.log('ğºï¸ Geofence event:', event);
