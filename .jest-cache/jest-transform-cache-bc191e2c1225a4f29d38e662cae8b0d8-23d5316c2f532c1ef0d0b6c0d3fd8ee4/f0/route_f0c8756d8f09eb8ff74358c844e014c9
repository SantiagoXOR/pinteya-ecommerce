    // Calcular estadÃ­sticas para operaciones masivas
    const stats =
    /* istanbul ignore next */
    (cov_2om3073nwv().s[254]++, {
      total_selected: promotionIds.length,
      found: promotions.length,
      not_found: promotionIds.length - promotions.length,
      by_status: {
        active: 0,
        inactive: 0,
        paused: 0,
        scheduled: 0,
        expired: 0
      },
      by_type: {},
      has_usage: 0,
      can_delete: 0
    });
    const now =
    /* istanbul ignore next */
