async function executeBulkUpdate(promotionIds, updates, userId) {
  /* istanbul ignore next */
  cov_2om3073nwv().f[11]++;
  const results =
  /* istanbul ignore next */
  (cov_2om3073nwv().s[121]++, []);
  // Validar fechas si se proporcionan
  /* istanbul ignore next */
  cov_2om3073nwv().s[122]++;
  if (
  /* istanbul ignore next */
  (cov_2om3073nwv().b[34][0]++, updates.starts_at) &&
  /* istanbul ignore next */
  (cov_2om3073nwv().b[34][1]++, updates.ends_at)) {
    /* istanbul ignore next */
    cov_2om3073nwv().b[33][0]++;
    const startsAt =
    /* istanbul ignore next */
    (cov_2om3073nwv().s[123]++, new Date(updates.starts_at));
    const endsAt =
    /* istanbul ignore next */
    (cov_2om3073nwv().s[124]++, new Date(updates.ends_at));
    /* istanbul ignore next */
    cov_2om3073nwv().s[125]++;
    if (startsAt >= endsAt) {
      /* istanbul ignore next */
      cov_2om3073nwv().b[35][0]++;
      cov_2om3073nwv().s[126]++;
      throw new Error('La fecha de inicio debe ser anterior a la fecha de finalizaciÃ³n');
    } else
    /* istanbul ignore next */
    {
      cov_2om3073nwv().b[35][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_2om3073nwv().b[33][1]++;
  }
  // Obtener promociones existentes
  const existingPromotions =
  /* istanbul ignore next */
  (cov_2om3073nwv().s[127]++, await getPromotionsForBulkOperation(promotionIds));
  const existingIds =
  /* istanbul ignore next */
  (cov_2om3073nwv().s[128]++, new Set(existingPromotions.map(p => {
    /* istanbul ignore next */
    cov_2om3073nwv().f[12]++;
    cov_2om3073nwv().s[129]++;
    return p.id;
  })));
  /* istanbul ignore next */
  cov_2om3073nwv().s[130]++;
  for (const promotionId of promotionIds) {
    /* istanbul ignore next */
    cov_2om3073nwv().s[131]++;
    try {
      /* istanbul ignore next */
      cov_2om3073nwv().s[132]++;
      if (!existingIds.has(promotionId)) {
        /* istanbul ignore next */
        cov_2om3073nwv().b[36][0]++;
        cov_2om3073nwv().s[133]++;
        results.push({
          promotion_id: promotionId,
          success: false,
          error: 'PromociÃ³n no encontrada'
        });
        /* istanbul ignore next */
        cov_2om3073nwv().s[134]++;
        continue;
      } else
      /* istanbul ignore next */
      {
        cov_2om3073nwv().b[36][1]++;
      }
      const updateData =
      /* istanbul ignore next */
      (cov_2om3073nwv().s[135]++, {
        ...updates,
        updated_at: new Date().toISOString()
      });
      const {
        data: updatedPromotion,
        error: updateError
      } =
      /* istanbul ignore next */
      (cov_2om3073nwv().s[136]++, await supabase.from('promotions').update(updateData).eq('id', promotionId).select().single());
      /* istanbul ignore next */
      cov_2om3073nwv().s[137]++;
      if (updateError) {
        /* istanbul ignore next */
        cov_2om3073nwv().b[37][0]++;
        cov_2om3073nwv().s[138]++;
        throw new Error(`Error al actualizar: ${updateError.message}`);
      } else
      /* istanbul ignore next */
      {
        cov_2om3073nwv().b[37][1]++;
      }
      cov_2om3073nwv().s[139]++;
      results.push({
        promotion_id: promotionId,
        success: true,
        data: updatedPromotion
      });
      // Registrar auditorÃ­a
      /* istanbul ignore next */
      cov_2om3073nwv().s[140]++;
      await logAuditAction('bulk_update', promotionId, userId, {
        updates
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_2om3073nwv().s[141]++;
      results.push({
        promotion_id: promotionId,
        success: false,
        error: error instanceof Error ?
        /* istanbul ignore next */
        (cov_2om3073nwv().b[38][0]++, error.message) :
        /* istanbul ignore next */
        (cov_2om3073nwv().b[38][1]++, 'Error desconocido')
      });
    }
  }
  const successful =
  /* istanbul ignore next */
  (cov_2om3073nwv().s[142]++, results.filter(r => {
    /* istanbul ignore next */
    cov_2om3073nwv().f[13]++;
    cov_2om3073nwv().s[143]++;
    return r.success;
  }).length);
  const failed =
  /* istanbul ignore next */
  (cov_2om3073nwv().s[144]++, results.filter(r => {
    /* istanbul ignore next */
    cov_2om3073nwv().f[14]++;
    cov_2om3073nwv().s[145]++;
    return !r.success;
  }).length);
  /* istanbul ignore next */
  cov_2om3073nwv().s[146]++;
  return {
    total_requested: promotionIds.length,
    successful,
    failed,
    results,
    summary: {
      updated: successful
    }
  };
}
async function exportPromotions(promotionIds, filters, format =
/* istanbul ignore next */
(cov_2om3073nwv().b[39][0]++, 'csv'), includeStats =
/* istanbul ignore next */
(cov_2om3073nwv().b[40][0]++, false)) {
  /* istanbul ignore next */
  cov_2om3073nwv().f[15]++;
  let query =
  /* istanbul ignore next */
  (cov_2om3073nwv().s[147]++, supabase.from('promotions').select(`
      id,
      name,
      description,
      type,
      priority,
      discount_percentage,
      discount_amount,
      minimum_order_amount,
      usage_limit,
      usage_count,
      starts_at,
      ends_at,
      is_active,
      is_paused,
      created_at,
      created_by,
      creator:profiles!promotions_created_by_fkey(
        full_name
      )
    `));
  // Aplicar filtros
  /* istanbul ignore next */
  cov_2om3073nwv().s[148]++;
  if (
  /* istanbul ignore next */
  (cov_2om3073nwv().b[42][0]++, promotionIds) &&
  /* istanbul ignore next */
  (cov_2om3073nwv().b[42][1]++, promotionIds.length > 0)) {
    /* istanbul ignore next */
    cov_2om3073nwv().b[41][0]++;
    cov_2om3073nwv().s[149]++;
    query = query.in('id', promotionIds);
  } else
  /* istanbul ignore next */
  {
    cov_2om3073nwv().b[41][1]++;
  }
  cov_2om3073nwv().s[150]++;
  if (filters) {
    /* istanbul ignore next */
    cov_2om3073nwv().b[43][0]++;
    cov_2om3073nwv().s[151]++;
    if (filters.type) {
      /* istanbul ignore next */
      cov_2om3073nwv().b[44][0]++;
      cov_2om3073nwv().s[152]++;
      query = query.eq('type', filters.type);
    } else
    /* istanbul ignore next */
    {
      cov_2om3073nwv().b[44][1]++;
    }
    cov_2om3073nwv().s[153]++;
    if (filters.date_from) {
      /* istanbul ignore next */
      cov_2om3073nwv().b[45][0]++;
      cov_2om3073nwv().s[154]++;
      query = query.gte('created_at', filters.date_from);
    } else
    /* istanbul ignore next */
    {
      cov_2om3073nwv().b[45][1]++;
    }
    cov_2om3073nwv().s[155]++;
    if (filters.date_to) {
      /* istanbul ignore next */
      cov_2om3073nwv().b[46][0]++;
      cov_2om3073nwv().s[156]++;
      query = query.lte('created_at', filters.date_to);
    } else
    /* istanbul ignore next */
    {
      cov_2om3073nwv().b[46][1]++;
    }
    cov_2om3073nwv().s[157]++;
    if (filters.created_by) {
      /* istanbul ignore next */
      cov_2om3073nwv().b[47][0]++;
      cov_2om3073nwv().s[158]++;
      query = query.eq('created_by', filters.created_by);
    } else
    /* istanbul ignore next */
    {
      cov_2om3073nwv().b[47][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_2om3073nwv().b[43][1]++;
  }
  const {
    data: promotions,
    error
  } =
  /* istanbul ignore next */
  (cov_2om3073nwv().s[159]++, await query.order('created_at', {
    ascending: false
  }));
  /* istanbul ignore next */
  cov_2om3073nwv().s[160]++;
  if (error) {
    /* istanbul ignore next */
    cov_2om3073nwv().b[48][0]++;
    cov_2om3073nwv().s[161]++;
    throw new Error(`Error al exportar promociones: ${error.message}`);
  } else
  /* istanbul ignore next */
  {
    cov_2om3073nwv().b[48][1]++;
  }
  // Procesar datos para exportaciÃ³n
  const exportData =
  /* istanbul ignore next */
  (cov_2om3073nwv().s[162]++, []);
  /* istanbul ignore next */
  cov_2om3073nwv().s[163]++;
  for (const promotion of
  /* istanbul ignore next */
  (cov_2om3073nwv().b[49][0]++, promotions) ||
  /* istanbul ignore next */
  (cov_2om3073nwv().b[49][1]++, [])) {
    // Calcular estado
    const now =
    /* istanbul ignore next */
    (cov_2om3073nwv().s[164]++, new Date());
    const startsAt =
    /* istanbul ignore next */
    (cov_2om3073nwv().s[165]++, new Date(promotion.starts_at));
    const endsAt =
    /* istanbul ignore next */
    (cov_2om3073nwv().s[166]++, promotion.ends_at ?
    /* istanbul ignore next */
    (cov_2om3073nwv().b[50][0]++, new Date(promotion.ends_at)) :
    /* istanbul ignore next */
    (cov_2om3073nwv().b[50][1]++, null));
    let status;
    /* istanbul ignore next */
    cov_2om3073nwv().s[167]++;
    if (promotion.is_paused) {
      /* istanbul ignore next */
      cov_2om3073nwv().b[51][0]++;
      cov_2om3073nwv().s[168]++;
      status = 'paused';
    } else {
      /* istanbul ignore next */
      cov_2om3073nwv().b[51][1]++;
      cov_2om3073nwv().s[169]++;
      if (!promotion.is_active) {
        /* istanbul ignore next */
        cov_2om3073nwv().b[52][0]++;
        cov_2om3073nwv().s[170]++;
        status = 'inactive';
      } else {
        /* istanbul ignore next */
        cov_2om3073nwv().b[52][1]++;
        cov_2om3073nwv().s[171]++;
        if (now < startsAt) {
          /* istanbul ignore next */
          cov_2om3073nwv().b[53][0]++;
          cov_2om3073nwv().s[172]++;
          status = 'scheduled';
        } else {
          /* istanbul ignore next */
          cov_2om3073nwv().b[53][1]++;
          cov_2om3073nwv().s[173]++;
          if (
          /* istanbul ignore next */
          (cov_2om3073nwv().b[55][0]++, endsAt) &&
          /* istanbul ignore next */
          (cov_2om3073nwv().b[55][1]++, now > endsAt)) {
            /* istanbul ignore next */
            cov_2om3073nwv().b[54][0]++;
            cov_2om3073nwv().s[174]++;
            status = 'expired';
          } else {
            /* istanbul ignore next */
            cov_2om3073nwv().b[54][1]++;
            cov_2om3073nwv().s[175]++;
            status = 'active';
          }
        }
      }
    }
    const exportItem =
    /* istanbul ignore next */
    (cov_2om3073nwv().s[176]++, {
      id: promotion.id,
      name: promotion.name,
      description: promotion.description,
      type: promotion.type,
      priority: promotion.priority,
      discount_percentage: promotion.discount_percentage,
      discount_amount: promotion.discount_amount,
      minimum_order_amount: promotion.minimum_order_amount,
      usage_limit: promotion.usage_limit,
      usage_count: promotion.usage_count,
      starts_at: promotion.starts_at,
      ends_at: promotion.ends_at,
      is_active: promotion.is_active,
      is_paused: promotion.is_paused,
      status,
      created_at: promotion.created_at,
      created_by: promotion.created_by,
      creator_name: promotion.creator?.full_name
    });
    // Incluir estadÃ­sticas si se solicita
    /* istanbul ignore next */
    cov_2om3073nwv().s[177]++;
    if (includeStats) {
      /* istanbul ignore next */
      cov_2om3073nwv().b[56][0]++;
      cov_2om3073nwv().s[178]++;
      try {
        const stats =
        /* istanbul ignore next */
        (cov_2om3073nwv().s[179]++, await getPromotionUsageStats(promotion.id));
        /* istanbul ignore next */
        cov_2om3073nwv().s[180]++;
        exportItem.total_usage = stats.total_usage;
        /* istanbul ignore next */
        cov_2om3073nwv().s[181]++;
        exportItem.unique_users = stats.unique_users;
        /* istanbul ignore next */
        cov_2om3073nwv().s[182]++;
        exportItem.total_discount_given = stats.total_discount_given;
        /* istanbul ignore next */
        cov_2om3073nwv().s[183]++;
        exportItem.conversion_rate = stats.conversion_rate;
      } catch (error) {
        // Continuar sin estadÃ­sticas si hay error
      }
    } else
    /* istanbul ignore next */
    {
      cov_2om3073nwv().b[56][1]++;
    }
    cov_2om3073nwv().s[184]++;
    exportData.push(exportItem);
  }
  // Generar nombre de archivo
  const timestamp =
  /* istanbul ignore next */
  (cov_2om3073nwv().s[185]++, new Date().toISOString().split('T')[0]);
  const filename =
  /* istanbul ignore next */
  (cov_2om3073nwv().s[186]++, `promotions_export_${timestamp}.${format}`);
  /* istanbul ignore next */
  cov_2om3073nwv().s[187]++;
  return {
    data: exportData,
    filename
  };
}
async function getPromotionUsageStats(promotionId) {
  /* istanbul ignore next */
  cov_2om3073nwv().f[16]++;
  const {
    data: usage,
    error
  } =
  /* istanbul ignore next */
  (cov_2om3073nwv().s[188]++, await supabase.from('promotion_usage').select('*').eq('promotion_id', promotionId));
  /* istanbul ignore next */
  cov_2om3073nwv().s[189]++;
  if (error) {
    /* istanbul ignore next */
    cov_2om3073nwv().b[57][0]++;
    cov_2om3073nwv().s[190]++;
    throw new Error(`Error al obtener estadÃ­sticas: ${error.message}`);
  } else
  /* istanbul ignore next */
  {
    cov_2om3073nwv().b[57][1]++;
  }
  const totalUsage =
  /* istanbul ignore next */
  (cov_2om3073nwv().s[191]++,
  /* istanbul ignore next */
  (cov_2om3073nwv().b[58][0]++, usage?.length) ||
  /* istanbul ignore next */
  (cov_2om3073nwv().b[58][1]++, 0));
  const uniqueUsers =
  /* istanbul ignore next */
  (cov_2om3073nwv().s[192]++, new Set(
  /* istanbul ignore next */
  (cov_2om3073nwv().b[59][0]++, usage?.map(u => {
    /* istanbul ignore next */
    cov_2om3073nwv().f[17]++;
    cov_2om3073nwv().s[193]++;
    return u.user_id;
  })) ||
  /* istanbul ignore next */
  (cov_2om3073nwv().b[59][1]++, [])).size);
  const totalDiscountGiven =
  /* istanbul ignore next */
  (cov_2om3073nwv().s[194]++, (
  /* istanbul ignore next */
  (cov_2om3073nwv().b[60][0]++, usage) ||
  /* istanbul ignore next */
  (cov_2om3073nwv().b[60][1]++, [])).reduce((sum, u) => {
    /* istanbul ignore next */
    cov_2om3073nwv().f[18]++;
    cov_2om3073nwv().s[195]++;
    return sum + (
    /* istanbul ignore next */
    (cov_2om3073nwv().b[61][0]++, u.discount_amount) ||
    /* istanbul ignore next */
    (cov_2om3073nwv().b[61][1]++, 0));
  }, 0));
  // Calcular tasa de conversiÃ³n simplificada
  const orderIds =
  /* istanbul ignore next */
  (cov_2om3073nwv().s[196]++, [...new Set(
  /* istanbul ignore next */
  (cov_2om3073nwv().b[62][0]++, usage?.map(u => {
    /* istanbul ignore next */
    cov_2om3073nwv().f[19]++;
    cov_2om3073nwv().s[197]++;
    return u.order_id;
  })) ||
  /* istanbul ignore next */
  (cov_2om3073nwv().b[62][1]++, []))]);
  const conversionRate =
  /* istanbul ignore next */
  (cov_2om3073nwv().s[198]++, uniqueUsers > 0 ?
  /* istanbul ignore next */
  (cov_2om3073nwv().b[63][0]++, orderIds.length / uniqueUsers * 100) :
  /* istanbul ignore next */
  (cov_2om3073nwv().b[63][1]++, 0));
  /* istanbul ignore next */
  cov_2om3073nwv().s[199]++;
  return {
    total_usage: totalUsage,
    unique_users: uniqueUsers,
    total_discount_given: totalDiscountGiven,
    conversion_rate: conversionRate
  };
}
