    /* istanbul ignore next */
    cov_2om3073nwv().s[202]++;
    _logger.logger.log(_logger.LogLevel.WARN, _logger.LogCategory.AUDIT, 'Error al registrar auditorÃ­a', {
      error
    });
  }
}
async function POST(request) {
  /* istanbul ignore next */
  cov_2om3073nwv().f[21]++;
  const startTime =
  /* istanbul ignore next */
  (cov_2om3073nwv().s[203]++, Date.now());
  /* istanbul ignore next */
  cov_2om3073nwv().s[204]++;
  try {
    // Rate limiting
    const rateLimitResult =
    /* istanbul ignore next */
    (cov_2om3073nwv().s[205]++, await (0, _ratelimiter.checkRateLimit)(request, {
      windowMs: RATE_LIMIT_CONFIGS.admin.windowMs,
      maxRequests: RATE_LIMIT_CONFIGS.admin.maxRequests,
      message: RATE_LIMIT_CONFIGS.admin.message
    }, 'admin-promotions-bulk'));
    /* istanbul ignore next */
    cov_2om3073nwv().s[206]++;
    if (!rateLimitResult.success) {
      /* istanbul ignore next */
      cov_2om3073nwv().b[64][0]++;
      const response =
      /* istanbul ignore next */
      (cov_2om3073nwv().s[207]++, _server.NextResponse.json({
        error: rateLimitResult.message
      }, {
        status: 429
      }));
      /* istanbul ignore next */
      cov_2om3073nwv().s[208]++;
      (0, _ratelimiter.addRateLimitHeaders)(response, rateLimitResult);
      /* istanbul ignore next */
      cov_2om3073nwv().s[209]++;
      return response;
    } else
    /* istanbul ignore next */
    {
      cov_2om3073nwv().b[64][1]++;
    }
    // Validar autenticaciÃ³n admin
    const authResult =
    /* istanbul ignore next */
    (cov_2om3073nwv().s[210]++, await validateAdminAuth());
    /* istanbul ignore next */
    cov_2om3073nwv().s[211]++;
    if (authResult.error) {
      /* istanbul ignore next */
      cov_2om3073nwv().b[65][0]++;
      const errorResponse =
      /* istanbul ignore next */
      (cov_2om3073nwv().s[212]++, {
        data: null,
        success: false,
        error: authResult.error
      });
      /* istanbul ignore next */
      cov_2om3073nwv().s[213]++;
      return _server.NextResponse.json(errorResponse, {
        status: authResult.status
      });
    } else
    /* istanbul ignore next */
    {
      cov_2om3073nwv().b[65][1]++;
    }
    const body =
    /* istanbul ignore next */
    (cov_2om3073nwv().s[214]++, await request.json());
    const {
      operation
    } =
    /* istanbul ignore next */
    (cov_2om3073nwv().s[215]++, body);
    let result;
    /* istanbul ignore next */
    cov_2om3073nwv().s[216]++;
    switch (operation) {
      case 'action':
        /* istanbul ignore next */
        cov_2om3073nwv().b[66][0]++;
        {
          const actionData =
          /* istanbul ignore next */
          (cov_2om3073nwv().s[217]++, BulkPromotionActionSchema.parse(body));
          /* istanbul ignore next */
          cov_2om3073nwv().s[218]++;
          result = await executeBulkAction(actionData.action, actionData.promotion_ids, authResult.userId, {
            extend_days: actionData.extend_days,
            priority: actionData.priority,
            force: actionData.force
          });
          /* istanbul ignore next */
          cov_2om3073nwv().s[219]++;
          break;
        }
      case 'update':
        /* istanbul ignore next */
        cov_2om3073nwv().b[66][1]++;
        {
          const updateData =
          /* istanbul ignore next */
          (cov_2om3073nwv().s[220]++, BulkPromotionUpdateSchema.parse(body));
          /* istanbul ignore next */
          cov_2om3073nwv().s[221]++;
          result = await executeBulkUpdate(updateData.promotion_ids, updateData.updates, authResult.userId);
          /* istanbul ignore next */
          cov_2om3073nwv().s[222]++;
          break;
        }
      case 'export':
        /* istanbul ignore next */
        cov_2om3073nwv().b[66][2]++;
        {
          const exportData =
          /* istanbul ignore next */
          (cov_2om3073nwv().s[223]++, BulkPromotionExportSchema.parse(body));
          const exportResult =
          /* istanbul ignore next */
          (cov_2om3073nwv().s[224]++, await exportPromotions(exportData.promotion_ids, exportData.filters, exportData.format, exportData.include_stats));
          /* istanbul ignore next */
          cov_2om3073nwv().s[225]++;
          result = {
            filename: exportResult.filename,
            total_records: exportResult.data.length,
            data: exportResult.data
          };
          /* istanbul ignore next */
          cov_2om3073nwv().s[226]++;
          break;
        }
      default:
        /* istanbul ignore next */
        cov_2om3073nwv().b[66][3]++;
        cov_2om3073nwv().s[227]++;
        throw new Error('OperaciÃ³n no vÃ¡lida');
    }
    // Registrar mÃ©tricas
    /* istanbul ignore next */
    cov_2om3073nwv().s[228]++;
    _metrics.metricsCollector.recordApiCall({
      endpoint: '/api/admin/promotions/bulk',
      method: 'POST',
      statusCode: 200,
      responseTime: Date.now() - startTime,
      userId: authResult.userId
    });
    const response =
    /* istanbul ignore next */
    (cov_2om3073nwv().s[229]++, {
      data: result,
      success: true,
      message: `OperaciÃ³n '${operation}' ejecutada exitosamente`
    });
    const nextResponse =
    /* istanbul ignore next */
    (cov_2om3073nwv().s[230]++, _server.NextResponse.json(response));
    /* istanbul ignore next */
    cov_2om3073nwv().s[231]++;
    (0, _ratelimiter.addRateLimitHeaders)(nextResponse, rateLimitResult);
    /* istanbul ignore next */
    cov_2om3073nwv().s[232]++;
    return nextResponse;
  } catch (error) {
    /* istanbul ignore next */
    cov_2om3073nwv().s[233]++;
    _logger.logger.log(_logger.LogLevel.ERROR, _logger.LogCategory.API, 'Error en POST /api/admin/promotions/bulk', {
      error
    });
    // Registrar mÃ©tricas de error
    /* istanbul ignore next */
    cov_2om3073nwv().s[234]++;
    _metrics.metricsCollector.recordApiCall({
      endpoint: '/api/admin/promotions/bulk',
      method: 'POST',
      statusCode: 500,
      responseTime: Date.now() - startTime,
      error: error instanceof Error ?
      /* istanbul ignore next */
      (cov_2om3073nwv().b[67][0]++, error.message) :
      /* istanbul ignore next */
      (cov_2om3073nwv().b[67][1]++, 'Unknown error')
    });
    const errorResponse =
    /* istanbul ignore next */
    (cov_2om3073nwv().s[235]++, {
      data: null,
      success: false,
      error: error instanceof Error ?
      /* istanbul ignore next */
      (cov_2om3073nwv().b[68][0]++, error.message) :
      /* istanbul ignore next */
      (cov_2om3073nwv().b[68][1]++, 'Error interno del servidor')
    });
    /* istanbul ignore next */
    cov_2om3073nwv().s[236]++;
    return _server.NextResponse.json(errorResponse, {
      status: 500
    });
  }
}
async function GET(request) {
  /* istanbul ignore next */
  cov_2om3073nwv().f[22]++;
  const startTime =
  /* istanbul ignore next */
