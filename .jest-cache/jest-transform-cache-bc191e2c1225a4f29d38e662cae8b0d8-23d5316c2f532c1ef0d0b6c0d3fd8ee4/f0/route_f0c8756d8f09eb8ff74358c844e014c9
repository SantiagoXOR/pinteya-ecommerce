    for (const promotion of promotions) {
      // Calcular estado
      const startsAt =
      /* istanbul ignore next */
      (cov_2om3073nwv().s[257]++, new Date(promotion.starts_at));
      const endsAt =
      /* istanbul ignore next */
      (cov_2om3073nwv().s[258]++, promotion.ends_at ?
      /* istanbul ignore next */
      (cov_2om3073nwv().b[73][0]++, new Date(promotion.ends_at)) :
      /* istanbul ignore next */
      (cov_2om3073nwv().b[73][1]++, null));
      let status;
      /* istanbul ignore next */
      cov_2om3073nwv().s[259]++;
      if (promotion.is_paused) {
        /* istanbul ignore next */
        cov_2om3073nwv().b[74][0]++;
        cov_2om3073nwv().s[260]++;
        status = 'paused';
      } else {
        /* istanbul ignore next */
        cov_2om3073nwv().b[74][1]++;
        cov_2om3073nwv().s[261]++;
        if (!promotion.is_active) {
          /* istanbul ignore next */
          cov_2om3073nwv().b[75][0]++;
          cov_2om3073nwv().s[262]++;
          status = 'inactive';
        } else {
          /* istanbul ignore next */
          cov_2om3073nwv().b[75][1]++;
          cov_2om3073nwv().s[263]++;
          if (now < startsAt) {
            /* istanbul ignore next */
            cov_2om3073nwv().b[76][0]++;
            cov_2om3073nwv().s[264]++;
            status = 'scheduled';
          } else {
            /* istanbul ignore next */
            cov_2om3073nwv().b[76][1]++;
            cov_2om3073nwv().s[265]++;
            if (
            /* istanbul ignore next */
            (cov_2om3073nwv().b[78][0]++, endsAt) &&
            /* istanbul ignore next */
            (cov_2om3073nwv().b[78][1]++, now > endsAt)) {
              /* istanbul ignore next */
              cov_2om3073nwv().b[77][0]++;
              cov_2om3073nwv().s[266]++;
              status = 'expired';
            } else {
              /* istanbul ignore next */
              cov_2om3073nwv().b[77][1]++;
              cov_2om3073nwv().s[267]++;
              status = 'active';
            }
          }
        }
      }
      /* istanbul ignore next */
      cov_2om3073nwv().s[268]++;
      stats.by_status[status]++;
      /* istanbul ignore next */
      cov_2om3073nwv().s[269]++;
      stats.by_type[promotion.type] = (
      /* istanbul ignore next */
      (cov_2om3073nwv().b[79][0]++, stats.by_type[promotion.type]) ||
      /* istanbul ignore next */
      (cov_2om3073nwv().b[79][1]++, 0)) + 1;
      /* istanbul ignore next */
      cov_2om3073nwv().s[270]++;
      if (promotion.usage_count > 0) {
        /* istanbul ignore next */
        cov_2om3073nwv().b[80][0]++;
        cov_2om3073nwv().s[271]++;
        stats.has_usage++;
      } else {
        /* istanbul ignore next */
        cov_2om3073nwv().b[80][1]++;
        cov_2om3073nwv().s[272]++;
        stats.can_delete++;
      }
    }
    // Registrar mÃ©tricas
    /* istanbul ignore next */
    cov_2om3073nwv().s[273]++;
    _metrics.metricsCollector.recordApiCall({
      endpoint: '/api/admin/promotions/bulk',
      method: 'GET',
      statusCode: 200,
      responseTime: Date.now() - startTime,
      userId: authResult.userId
    });
    const response =
    /* istanbul ignore next */
    (cov_2om3073nwv().s[274]++, {
      data: {
        promotions,
        stats
      },
      success: true,
      message: 'InformaciÃ³n de operaciones masivas obtenida exitosamente'
    });
    const nextResponse =
    /* istanbul ignore next */
    (cov_2om3073nwv().s[275]++, _server.NextResponse.json(response));
    /* istanbul ignore next */
    cov_2om3073nwv().s[276]++;
    (0, _ratelimiter.addRateLimitHeaders)(nextResponse, rateLimitResult);
    /* istanbul ignore next */
    cov_2om3073nwv().s[277]++;
    return nextResponse;
  } catch (error) {
    /* istanbul ignore next */
