"use strict";
Object.defineProperty(exports, "__esModule", {
    value: true
});
const _helpers = require("../../utils/helpers");
describe('Utility Helpers', ()=>{
    describe('formatPrice', ()=>{
        it('formats prices correctly for Argentine pesos', ()=>{
            expect((0, _helpers.formatPrice)(1000)).toBe('$1.000');
            expect((0, _helpers.formatPrice)(1500.5)).toBe('$1.501'); // Rounded
            expect((0, _helpers.formatPrice)(10000)).toBe('$10.000');
            expect((0, _helpers.formatPrice)(0)).toBe('$0');
        });
        it('handles negative prices', ()=>{
            expect((0, _helpers.formatPrice)(-1000)).toBe('$-1.000');
        });
        it('handles very large numbers', ()=>{
            expect((0, _helpers.formatPrice)(1000000)).toBe('$1.000.000');
        });
    });
    describe('calculateDiscount', ()=>{
        it('calculates discount percentage correctly', ()=>{
            expect((0, _helpers.calculateDiscount)(1000, 800)).toBe(20);
            expect((0, _helpers.calculateDiscount)(2000, 1500)).toBe(25);
            expect((0, _helpers.calculateDiscount)(100, 90)).toBe(10);
        });
        it('returns 0 when no discount', ()=>{
            expect((0, _helpers.calculateDiscount)(1000, 1000)).toBe(0);
            expect((0, _helpers.calculateDiscount)(1000, 1100)).toBe(0); // Higher discounted price
        });
        it('handles edge cases', ()=>{
            expect((0, _helpers.calculateDiscount)(0, 0)).toBe(0);
            expect((0, _helpers.calculateDiscount)(100, 0)).toBe(100);
            expect((0, _helpers.calculateDiscount)(null, 50)).toBe(0);
            expect((0, _helpers.calculateDiscount)(100, null)).toBe(0);
        });
        it('rounds to nearest integer', ()=>{
            expect((0, _helpers.calculateDiscount)(1000, 666)).toBe(33); // 33.4% rounded
            expect((0, _helpers.calculateDiscount)(1000, 667)).toBe(33); // 33.3% rounded
        });
    });
    describe('validateEmail', ()=>{
        it('validates correct email formats', ()=>{
            expect((0, _helpers.validateEmail)('test@example.com')).toBe(true);
            expect((0, _helpers.validateEmail)('user.name@domain.co.ar')).toBe(true);
            expect((0, _helpers.validateEmail)('test+tag@gmail.com')).toBe(true);
        });
        it('rejects invalid email formats', ()=>{
            expect((0, _helpers.validateEmail)('invalid-email')).toBe(false);
            expect((0, _helpers.validateEmail)('test@')).toBe(false);
            expect((0, _helpers.validateEmail)('@domain.com')).toBe(false);
            expect((0, _helpers.validateEmail)('test..test@domain.com')).toBe(false);
            expect((0, _helpers.validateEmail)('')).toBe(false);
            expect((0, _helpers.validateEmail)('test@domain')).toBe(false); // No TLD
            expect((0, _helpers.validateEmail)('test@.com')).toBe(false); // Invalid domain
        });
        it('handles edge cases', ()=>{
            expect((0, _helpers.validateEmail)('a@bb.co')).toBe(true); // Minimal valid email with 2-char domain
        });
    });
    describe('generateSlug', ()=>{
        it('generates slugs from product names', ()=>{
            expect((0, _helpers.generateSlug)('Sherwin Williams ProClassic Blanco 4L')).toBe('sherwin-williams-proclassic-blanco-4l');
            expect((0, _helpers.generateSlug)('Pintura Látex Interior')).toBe('pintura-latex-interior');
            expect((0, _helpers.generateSlug)('Set 3 Pinceles Profesionales')).toBe('set-3-pinceles-profesionales');
        });
        it('handles special characters', ()=>{
            expect((0, _helpers.generateSlug)('Esmalte Sintético Ñandú')).toBe('esmalte-sintetico-nandu');
            expect((0, _helpers.generateSlug)('Antióxido Rojo')).toBe('antioxido-rojo');
            expect((0, _helpers.generateSlug)('Rodillo 23cm (Pack x2)')).toBe('rodillo-23cm-pack-x2');
        });
        it('handles multiple spaces and special cases', ()=>{
            expect((0, _helpers.generateSlug)('  Multiple   Spaces  ')).toBe('multiple-spaces');
            expect((0, _helpers.generateSlug)('UPPERCASE TEXT')).toBe('uppercase-text');
            expect((0, _helpers.generateSlug)('123 Numbers 456')).toBe('123-numbers-456');
        });
        it('handles empty or invalid input', ()=>{
            expect((0, _helpers.generateSlug)('')).toBe('');
            expect((0, _helpers.generateSlug)('   ')).toBe('');
            expect((0, _helpers.generateSlug)('!!!')).toBe('');
        });
    });
    describe('formatPhoneNumber', ()=>{
        it('formats Argentine phone numbers', ()=>{
            expect((0, _helpers.formatPhoneNumber)('1234567890')).toBe('(123) 456-7890');
            expect((0, _helpers.formatPhoneNumber)('01112345678')).toBe('(011) 1234-5678');
        });
        it('handles different input formats', ()=>{
            expect((0, _helpers.formatPhoneNumber)('123-456-7890')).toBe('(123) 456-7890');
            expect((0, _helpers.formatPhoneNumber)('(123) 456-7890')).toBe('(123) 456-7890');
            expect((0, _helpers.formatPhoneNumber)('+54 11 1234-5678')).toBe('+54 11 1234-5678'); // Keep original if not standard format
        });
        it('handles invalid phone numbers', ()=>{
            expect((0, _helpers.formatPhoneNumber)('123')).toBe('123'); // Too short
            expect((0, _helpers.formatPhoneNumber)('')).toBe('');
            expect((0, _helpers.formatPhoneNumber)('abc')).toBe('abc'); // Non-numeric
        });
    });
    describe('calculateShipping', ()=>{
        it('calculates shipping based on weight and distance', ()=>{
            expect((0, _helpers.calculateShipping)(1, 'CABA')).toBe(500); // Base rate
            expect((0, _helpers.calculateShipping)(5, 'Buenos Aires')).toBe(1200); // Higher weight (800 * 1.5)
            expect((0, _helpers.calculateShipping)(1, 'Córdoba')).toBe(1200); // Different province
        });
        it('applies free shipping threshold', ()=>{
            expect((0, _helpers.calculateShipping)(1, 'CABA', 15000)).toBe(0); // Free shipping over threshold
            expect((0, _helpers.calculateShipping)(1, 'CABA', 10000)).toBe(500); // Below threshold
        });
        it('handles express shipping', ()=>{
            expect((0, _helpers.calculateShipping)(1, 'CABA', 0, true)).toBe(1000); // Express shipping
            expect((0, _helpers.calculateShipping)(1, 'Buenos Aires', 0, true)).toBe(1600); // Express + distance
        });
    });
    describe('validateCartItem', ()=>{
        const validItem = {
            id: 1,
            name: 'Test Product',
            price: 1000,
            quantity: 2,
            stock: 10
        };
        it('validates correct cart items', ()=>{
            expect((0, _helpers.validateCartItem)(validItem)).toBe(true);
        });
        it('rejects items with missing required fields', ()=>{
            expect((0, _helpers.validateCartItem)({
                ...validItem,
                id: undefined
            })).toBe(false);
            expect((0, _helpers.validateCartItem)({
                ...validItem,
                name: ''
            })).toBe(false);
            expect((0, _helpers.validateCartItem)({
                ...validItem,
                price: 0
            })).toBe(false);
        });
        it('rejects items with invalid quantities', ()=>{
            expect((0, _helpers.validateCartItem)({
                ...validItem,
                quantity: 0
            })).toBe(false);
            expect((0, _helpers.validateCartItem)({
                ...validItem,
                quantity: -1
            })).toBe(false);
            expect((0, _helpers.validateCartItem)({
                ...validItem,
                quantity: 11
            })).toBe(false); // Exceeds stock
        });
        it('rejects items with invalid prices', ()=>{
            expect((0, _helpers.validateCartItem)({
                ...validItem,
                price: -100
            })).toBe(false);
            expect((0, _helpers.validateCartItem)({
                ...validItem,
                price: 'invalid'
            })).toBe(false);
        });
    });
    describe('sanitizeInput', ()=>{
        it('removes potentially dangerous HTML', ()=>{
            expect((0, _helpers.sanitizeInput)('<script>alert("xss")</script>')).toBe('');
            expect((0, _helpers.sanitizeInput)('<img src="x" onerror="alert(1)">')).toBe('');
            expect((0, _helpers.sanitizeInput)('Hello <b>World</b>')).toBe('Hello World');
        });
        it('preserves safe text', ()=>{
            expect((0, _helpers.sanitizeInput)('Normal text')).toBe('Normal text');
            expect((0, _helpers.sanitizeInput)('Text with números 123')).toBe('Text with números 123');
            expect((0, _helpers.sanitizeInput)('Email: test@example.com')).toBe('Email: test@example.com');
        });
        it('handles special characters safely', ()=>{
            expect((0, _helpers.sanitizeInput)('Price: $1,000 & more')).toBe('Price: $1,000 & more');
            expect((0, _helpers.sanitizeInput)('Ñandú & Cóndor')).toBe('Ñandú & Cóndor');
        });
        it('handles empty and null inputs', ()=>{
            expect((0, _helpers.sanitizeInput)('')).toBe('');
            expect((0, _helpers.sanitizeInput)(null)).toBe('');
            expect((0, _helpers.sanitizeInput)(undefined)).toBe('');
        });
    });
    describe('Edge Cases and Error Handling', ()=>{
        it('handles null and undefined inputs gracefully', ()=>{
            expect((0, _helpers.formatPrice)(null)).toBe('$0');
            expect((0, _helpers.formatPrice)(undefined)).toBe('$0');
            expect((0, _helpers.calculateDiscount)(null, 100)).toBe(0);
            expect((0, _helpers.validateEmail)(null)).toBe(false);
            expect((0, _helpers.generateSlug)(null)).toBe('');
        });
        it('handles extreme values', ()=>{
            expect((0, _helpers.formatPrice)(Number.MAX_SAFE_INTEGER)).toBeDefined();
            expect((0, _helpers.calculateDiscount)(Number.MAX_SAFE_INTEGER, 1)).toBe(100);
        });
        it('handles non-string inputs for string functions', ()=>{
            expect((0, _helpers.generateSlug)(123)).toBe('123');
            expect((0, _helpers.validateEmail)(123)).toBe(false);
            expect((0, _helpers.sanitizeInput)(123)).toBe('123');
        });
    });
});

