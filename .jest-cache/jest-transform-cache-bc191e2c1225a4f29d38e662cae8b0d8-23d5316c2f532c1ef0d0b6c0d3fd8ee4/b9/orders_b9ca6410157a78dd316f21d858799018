  } else
  /* istanbul ignore next */
  {
    cov_1xr2f5y9r6().b[34][1]++;
  }
  // Validar shipping si est presente
  cov_1xr2f5y9r6().s[89]++;
  if (orderData.shipping) {
    /* istanbul ignore next */
    cov_1xr2f5y9r6().b[36][0]++;
    cov_1xr2f5y9r6().s[90]++;
    if (
    /* istanbul ignore next */
    (cov_1xr2f5y9r6().b[38][0]++, !orderData.shipping.address.street_name) ||
    /* istanbul ignore next */
