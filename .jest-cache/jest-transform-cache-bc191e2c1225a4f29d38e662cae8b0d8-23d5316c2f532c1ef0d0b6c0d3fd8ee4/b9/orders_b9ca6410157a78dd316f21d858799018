      /* istanbul ignore next */
      cov_1xr2f5y9r6().b[37][0]++;
      cov_1xr2f5y9r6().s[91]++;
      errors.push('Direccin de envo incompleta');
    } else
    /* istanbul ignore next */
    {
      cov_1xr2f5y9r6().b[37][1]++;
    }
    cov_1xr2f5y9r6().s[92]++;
    if (!orderData.shipping.address.zip_code) {
      /* istanbul ignore next */
      cov_1xr2f5y9r6().b[39][0]++;
