        (cov_1xr2f5y9r6().b[15][1]++, 'Error obteniendo rdenes')
      };
    } else
    /* istanbul ignore next */
    {
      cov_1xr2f5y9r6().b[14][1]++;
    }
    cov_1xr2f5y9r6().s[52]++;
