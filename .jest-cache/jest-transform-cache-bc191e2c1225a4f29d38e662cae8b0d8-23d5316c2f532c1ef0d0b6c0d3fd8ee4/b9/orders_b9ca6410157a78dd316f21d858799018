  } else
  /* istanbul ignore next */
  {
    cov_1xr2f5y9r6().b[33][1]++;
  }
  // Validar email format
  const emailRegex =
  /* istanbul ignore next */
  (cov_1xr2f5y9r6().s[86]++, /^[^\s@]+@[^\s@]+\.[^\s@]+$/);
  /* istanbul ignore next */
  cov_1xr2f5y9r6().s[87]++;
