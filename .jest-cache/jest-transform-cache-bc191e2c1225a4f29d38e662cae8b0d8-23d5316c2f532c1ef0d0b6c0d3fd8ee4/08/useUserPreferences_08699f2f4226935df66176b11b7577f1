function usePreferencesValidation() {
  /* istanbul ignore next */
  cov_2lrxkb0480().f[17]++;
  const validateLanguage =
  /* istanbul ignore next */
  (cov_2lrxkb0480().s[87]++, (0, _react.useCallback)(language => {
