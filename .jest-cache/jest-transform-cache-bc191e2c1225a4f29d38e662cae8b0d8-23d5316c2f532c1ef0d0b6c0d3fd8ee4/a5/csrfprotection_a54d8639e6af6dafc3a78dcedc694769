function validateCSRFToken(providedToken, expectedToken) {
  /* istanbul ignore next */
  cov_26s1i6cm7j().f[18]++;
  cov_26s1i6cm7j().s[66]++;
  if (
  /* istanbul ignore next */
  (cov_26s1i6cm7j().b[27][0]++, !providedToken) ||
  /* istanbul ignore next */
  (cov_26s1i6cm7j().b[27][1]++, !expectedToken)) {
    /* istanbul ignore next */
    cov_26s1i6cm7j().b[26][0]++;
    cov_26s1i6cm7j().s[67]++;
    return false;
  } else
  /* istanbul ignore next */
  {
    cov_26s1i6cm7j().b[26][1]++;
