        /* istanbul ignore next */
        cov_1mlpxp5z2p().b[27][0]++;
        cov_1mlpxp5z2p().s[46]++;
        newFilters.page = 1;
      } else
      /* istanbul ignore next */
      {
        cov_1mlpxp5z2p().b[27][1]++;
      }
      // Actualizar URL de forma asncrona
      cov_1mlpxp5z2p().s[47]++;
      setTimeout(() => {
        /* istanbul ignore next */
        cov_1mlpxp5z2p().f[7]++;
        cov_1mlpxp5z2p().s[48]++;
        return updateUrl(newFilters);
      }, 0);
      // Callback opcional
      /* istanbul ignore next */
      cov_1mlpxp5z2p().s[49]++;
      onFiltersChange?.(newFilters);
      /* istanbul ignore next */
      cov_1mlpxp5z2p().s[50]++;
      return newFilters;
    });
  }, [updateUrl, onFiltersChange]));
  // Handlers memoizados para evitar re-renders
  const updateCategories =
  /* istanbul ignore next */
  (cov_1mlpxp5z2p().s[51]++, (0, _react.useCallback)(categories => {
    /* istanbul ignore next */
    cov_1mlpxp5z2p().f[8]++;
    cov_1mlpxp5z2p().s[52]++;
    updateFilters({
      categories
