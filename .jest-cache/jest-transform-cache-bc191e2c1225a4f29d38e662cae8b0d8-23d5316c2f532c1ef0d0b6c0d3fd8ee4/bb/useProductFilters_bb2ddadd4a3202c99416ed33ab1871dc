    /* istanbul ignore next */
    {
      cov_1mlpxp5z2p().b[17][1]++;
    }
    cov_1mlpxp5z2p().s[25]++;
    if (newFilters.priceMin !== undefined) {
      /* istanbul ignore next */
      cov_1mlpxp5z2p().b[18][0]++;
      cov_1mlpxp5z2p().s[26]++;
      params.set('priceMin', newFilters.priceMin.toString());
    } else
    /* istanbul ignore next */
    {
      cov_1mlpxp5z2p().b[18][1]++;
    }
    cov_1mlpxp5z2p().s[27]++;
    if (newFilters.priceMax !== undefined) {
      /* istanbul ignore next */
      cov_1mlpxp5z2p().b[19][0]++;
      cov_1mlpxp5z2p().s[28]++;
      params.set('priceMax', newFilters.priceMax.toString());
    } else
    /* istanbul ignore next */
    {
      cov_1mlpxp5z2p().b[19][1]++;
    }
    cov_1mlpxp5z2p().s[29]++;
    if (
    /* istanbul ignore next */
    (cov_1mlpxp5z2p().b[21][0]++, newFilters.search) &&
    /* istanbul ignore next */
    (cov_1mlpxp5z2p().b[21][1]++, newFilters.search.trim())) {
      /* istanbul ignore next */
      cov_1mlpxp5z2p().b[20][0]++;
      cov_1mlpxp5z2p().s[30]++;
      params.set('search', newFilters.search.trim());
    } else
    /* istanbul ignore next */
    {
      cov_1mlpxp5z2p().b[20][1]++;
    }
    cov_1mlpxp5z2p().s[31]++;
    if (newFilters.sortBy !== defaultSort) {
      /* istanbul ignore next */
      cov_1mlpxp5z2p().b[22][0]++;
      cov_1mlpxp5z2p().s[32]++;
      params.set('sortBy', newFilters.sortBy);
    } else
    /* istanbul ignore next */
    {
      cov_1mlpxp5z2p().b[22][1]++;
    }
    cov_1mlpxp5z2p().s[33]++;
    if (newFilters.page > 1) {
      /* istanbul ignore next */
      cov_1mlpxp5z2p().b[23][0]++;
      cov_1mlpxp5z2p().s[34]++;
      params.set('page', newFilters.page.toString());
    } else
    /* istanbul ignore next */
    {
      cov_1mlpxp5z2p().b[23][1]++;
    }
    cov_1mlpxp5z2p().s[35]++;
    if (newFilters.limit !== defaultLimit) {
      /* istanbul ignore next */
      cov_1mlpxp5z2p().b[24][0]++;
      cov_1mlpxp5z2p().s[36]++;
      params.set('limit', newFilters.limit.toString());
    } else
    /* istanbul ignore next */
    {
      cov_1mlpxp5z2p().b[24][1]++;
    }
    const newQuery =
    /* istanbul ignore next */
    (cov_1mlpxp5z2p().s[37]++, params.toString());
    const currentQuery =
    /* istanbul ignore next */
    (cov_1mlpxp5z2p().s[38]++, window.location.search.slice(1));
    /* istanbul ignore next */
    cov_1mlpxp5z2p().s[39]++;
    if (newQuery !== currentQuery) {
      /* istanbul ignore next */
      cov_1mlpxp5z2p().b[25][0]++;
      const newUrl =
      /* istanbul ignore next */
      (cov_1mlpxp5z2p().s[40]++, newQuery ?
      /* istanbul ignore next */
      (cov_1mlpxp5z2p().b[26][0]++, `${window.location.pathname}?${newQuery}`) :
      /* istanbul ignore next */
      (cov_1mlpxp5z2p().b[26][1]++, window.location.pathname));
      /* istanbul ignore next */
      cov_1mlpxp5z2p().s[41]++;
      router.push(newUrl, {
        scroll: false
      });
    } else
    /* istanbul ignore next */
    {
      cov_1mlpxp5z2p().b[25][1]++;
    }
  }, [syncWithUrl, router, defaultSort, defaultLimit]));
  // Funcin optimizada para actualizar filtros
  const updateFilters =
  /* istanbul ignore next */
  (cov_1mlpxp5z2p().s[42]++, (0, _react.useCallback)(updates => {
    /* istanbul ignore next */
    cov_1mlpxp5z2p().f[5]++;
    cov_1mlpxp5z2p().s[43]++;
    setFilters(prev => {
      /* istanbul ignore next */
      cov_1mlpxp5z2p().f[6]++;
      const newFilters =
      /* istanbul ignore next */
      (cov_1mlpxp5z2p().s[44]++, {
        ...prev,
        ...updates
      });
      // Reset page cuando cambian otros filtros (excepto page)
      /* istanbul ignore next */
      cov_1mlpxp5z2p().s[45]++;
      if (
      /* istanbul ignore next */
      (cov_1mlpxp5z2p().b[28][0]++, !updates.hasOwnProperty('page')) &&
      /* istanbul ignore next */
