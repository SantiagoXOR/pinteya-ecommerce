      /* istanbul ignore next */
      (cov_1mlpxp5z2p().b[9][0]++, Number(searchParams.get('priceMin'))) :
      /* istanbul ignore next */
      (cov_1mlpxp5z2p().b[9][1]++, undefined),
      priceMax: searchParams.get('priceMax') ?
      /* istanbul ignore next */
      (cov_1mlpxp5z2p().b[10][0]++, Number(searchParams.get('priceMax'))) :
      /* istanbul ignore next */
      (cov_1mlpxp5z2p().b[10][1]++, undefined),
      search:
      /* istanbul ignore next */
      (cov_1mlpxp5z2p().b[11][0]++, searchParams.get('search')) ||
      /* istanbul ignore next */
      (cov_1mlpxp5z2p().b[11][1]++, ''),
      sortBy:
      /* istanbul ignore next */
      (cov_1mlpxp5z2p().b[12][0]++, searchParams.get('sortBy')) ||
      /* istanbul ignore next */
      (cov_1mlpxp5z2p().b[12][1]++, defaultSort),
      page:
      /* istanbul ignore next */
      (cov_1mlpxp5z2p().b[13][0]++, Number(searchParams.get('page'))) ||
      /* istanbul ignore next */
      (cov_1mlpxp5z2p().b[13][1]++, 1),
      limit:
      /* istanbul ignore next */
      (cov_1mlpxp5z2p().b[14][0]++, Number(searchParams.get('limit'))) ||
      /* istanbul ignore next */
      (cov_1mlpxp5z2p().b[14][1]++, defaultLimit)
    });
    /* istanbul ignore next */
    cov_1mlpxp5z2p().s[16]++;
    setFilters(urlFilters);
  }, [searchParams, syncWithUrl, defaultSort, defaultLimit]); // Actualizar cuando cambien los searchParams
  // Funcin optimizada para actualizar URL
  const updateUrl =
  /* istanbul ignore next */
  (cov_1mlpxp5z2p().s[17]++, (0, _react.useCallback)(newFilters => {
    /* istanbul ignore next */
    cov_1mlpxp5z2p().f[4]++;
    cov_1mlpxp5z2p().s[18]++;
    if (!syncWithUrl) {
      /* istanbul ignore next */
      cov_1mlpxp5z2p().b[15][0]++;
      cov_1mlpxp5z2p().s[19]++;
