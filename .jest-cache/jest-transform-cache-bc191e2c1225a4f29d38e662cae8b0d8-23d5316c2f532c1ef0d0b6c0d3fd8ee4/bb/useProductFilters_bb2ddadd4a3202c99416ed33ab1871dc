    /* istanbul ignore next */
    cov_1mlpxp5z2p().s[21]++;
    if (newFilters.categories.length > 0) {
      /* istanbul ignore next */
      cov_1mlpxp5z2p().b[16][0]++;
      cov_1mlpxp5z2p().s[22]++;
