      cov_1m69zw6ysz().s[84]++;
      console.error('Error al obtener drivers disponibles:', driversError);
      /* istanbul ignore next */
      cov_1m69zw6ysz().s[85]++;
      return _server.NextResponse.json({
        error: 'Error al obtener drivers disponibles'
