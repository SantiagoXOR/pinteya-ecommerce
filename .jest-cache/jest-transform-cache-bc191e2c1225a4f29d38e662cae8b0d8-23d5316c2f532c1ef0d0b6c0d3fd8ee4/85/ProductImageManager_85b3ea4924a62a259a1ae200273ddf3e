  /* istanbul ignore next */
  (cov_8hlx3pglg().b[10][0]++, 10),
  maxFileSize =
  /* istanbul ignore next */
  (cov_8hlx3pglg().b[11][0]++, 5),
  allowedTypes =
  /* istanbul ignore next */
  (cov_8hlx3pglg().b[12][0]++, ['image/jpeg', 'image/jpg', 'image/png', 'image/webp', 'image/gif']),
