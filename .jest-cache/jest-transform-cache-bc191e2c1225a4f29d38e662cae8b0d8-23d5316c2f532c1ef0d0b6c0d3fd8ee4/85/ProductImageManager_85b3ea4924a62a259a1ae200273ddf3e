const optimizeImage = async (file, maxWidth =
/* istanbul ignore next */
(cov_8hlx3pglg().b[5][0]++, 1200), quality =
/* istanbul ignore next */
(cov_8hlx3pglg().b[6][0]++, 0.8)) => {
  /* istanbul ignore next */
  cov_8hlx3pglg().f[3]++;
  cov_8hlx3pglg().s[18]++;
  return new Promise(resolve => {
    /* istanbul ignore next */
    cov_8hlx3pglg().f[4]++;
    const canvas =
    /* istanbul ignore next */
    (cov_8hlx3pglg().s[19]++, document.createElement('canvas'));
    const ctx =
    /* istanbul ignore next */
    (cov_8hlx3pglg().s[20]++, canvas.getContext('2d'));
    const img =
    /* istanbul ignore next */
    (cov_8hlx3pglg().s[21]++, new window.Image());
    /* istanbul ignore next */
    cov_8hlx3pglg().s[22]++;
    img.onload = () => {
      /* istanbul ignore next */
      cov_8hlx3pglg().f[5]++;
      // Calculate new dimensions
      let {
        width,
        height
      } =
      /* istanbul ignore next */
      (cov_8hlx3pglg().s[23]++, img);
      /* istanbul ignore next */
      cov_8hlx3pglg().s[24]++;
      if (width > maxWidth) {
        /* istanbul ignore next */
        cov_8hlx3pglg().b[7][0]++;
        cov_8hlx3pglg().s[25]++;
        height = height * maxWidth / width;
        /* istanbul ignore next */
        cov_8hlx3pglg().s[26]++;
        width = maxWidth;
      } else
      /* istanbul ignore next */
      {
        cov_8hlx3pglg().b[7][1]++;
      }
      cov_8hlx3pglg().s[27]++;
      canvas.width = width;
      /* istanbul ignore next */
      cov_8hlx3pglg().s[28]++;
      canvas.height = height;
      // Draw and compress
      /* istanbul ignore next */
      cov_8hlx3pglg().s[29]++;
      ctx.drawImage(img, 0, 0, width, height);
      /* istanbul ignore next */
      cov_8hlx3pglg().s[30]++;
      canvas.toBlob(blob => {
        /* istanbul ignore next */
        cov_8hlx3pglg().f[6]++;
        cov_8hlx3pglg().s[31]++;
        if (blob) {
          /* istanbul ignore next */
          cov_8hlx3pglg().b[8][0]++;
          const optimizedFile =
          /* istanbul ignore next */
          (cov_8hlx3pglg().s[32]++, new File([blob], file.name, {
            type: file.type,
            lastModified: Date.now()
          }));
          /* istanbul ignore next */
          cov_8hlx3pglg().s[33]++;
          resolve(optimizedFile);
        } else {
          /* istanbul ignore next */
          cov_8hlx3pglg().b[8][1]++;
          cov_8hlx3pglg().s[34]++;
          resolve(file);
        }
      }, file.type, quality);
    };
    /* istanbul ignore next */
    cov_8hlx3pglg().s[35]++;
    img.src = URL.createObjectURL(file);
  });
};
/* istanbul ignore next */
