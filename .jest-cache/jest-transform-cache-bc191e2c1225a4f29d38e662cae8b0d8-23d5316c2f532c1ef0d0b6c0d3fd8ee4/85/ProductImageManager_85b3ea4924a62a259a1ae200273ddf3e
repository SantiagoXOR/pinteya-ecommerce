const getImageDimensions = file => {
  /* istanbul ignore next */
  cov_8hlx3pglg().f[7]++;
  cov_8hlx3pglg().s[37]++;
  return new Promise(resolve => {
    /* istanbul ignore next */
    cov_8hlx3pglg().f[8]++;
    const img =
    /* istanbul ignore next */
    (cov_8hlx3pglg().s[38]++, new window.Image());
