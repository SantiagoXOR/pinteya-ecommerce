const validateImageFile = (file, maxSize =
/* istanbul ignore next */
(cov_8hlx3pglg().b[2][0]++, 5)) => {
  /* istanbul ignore next */
  cov_8hlx3pglg().f[2]++;
  const allowedTypes =
  /* istanbul ignore next */
  (cov_8hlx3pglg().s[11]++, ['image/jpeg', 'image/jpg', 'image/png', 'image/webp', 'image/gif']);
  /* istanbul ignore next */
  cov_8hlx3pglg().s[12]++;
  if (!allowedTypes.includes(file.type)) {
    /* istanbul ignore next */
    cov_8hlx3pglg().b[3][0]++;
    cov_8hlx3pglg().s[13]++;
    return 'Tipo de archivo no permitido. Use JPG, PNG, WebP o GIF.';
  } else
  /* istanbul ignore next */
  {
    cov_8hlx3pglg().b[3][1]++;
  }
  cov_8hlx3pglg().s[14]++;
  if (file.size > maxSize * 1024 * 1024) {
    /* istanbul ignore next */
    cov_8hlx3pglg().b[4][0]++;
    cov_8hlx3pglg().s[15]++;
    return `El archivo es muy grande. Maximo ${maxSize}MB permitido.`;
  } else
  /* istanbul ignore next */
  {
    cov_8hlx3pglg().b[4][1]++;
  }
  cov_8hlx3pglg().s[16]++;
  return null;
};
/* istanbul ignore next */
