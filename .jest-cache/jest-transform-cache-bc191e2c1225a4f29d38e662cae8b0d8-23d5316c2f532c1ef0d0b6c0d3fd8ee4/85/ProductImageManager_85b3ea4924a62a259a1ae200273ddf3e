  /* istanbul ignore next */
  cov_8hlx3pglg().f[10]++;
  const [draggedIndex, setDraggedIndex] =
  /* istanbul ignore next */
  (cov_8hlx3pglg().s[42]++, (0, _react.useState)(null));
  const [editingIndex, setEditingIndex] =
  /* istanbul ignore next */
