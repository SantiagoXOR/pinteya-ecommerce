                                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("input", {
                                                                type: "text",
                                                                value: editingAlt,
                                                                onChange: (e)=>setEditingAlt(e.target.value),
                                                                className: "w-full text-xs border border-gray-300 rounded px-2 py-1 focus:outline-none focus:ring-1 focus:ring-blaze-orange-500",
                                                                placeholder: "Texto alternativo"
                                                            }),
                                                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                                className: "flex space-x-1",
                                                                children: [
                                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                                                                        type: "button",
                                                                        onClick: saveAltText,
                                                                        className: "flex-1 text-xs bg-blaze-orange-600 text-white px-2 py-1 rounded hover:bg-blaze-orange-700",
                                                                        children: "Guardar"
                                                                    }),
                                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                                                                        type: "button",
                                                                        onClick: cancelEditingAlt,
                                                                        className: "flex-1 text-xs bg-gray-300 text-gray-700 px-2 py-1 rounded hover:bg-gray-400",
                                                                        children: "Cancelar"
                                                                    })
                                                                ]
                                                            })
                                                        ]
                                                    }) : /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                                        className: "text-xs text-gray-600 truncate",
                                                        children: image.alt || 'Sin texto alternativo'
                                                    })
                                                })
                                            ]
                                        }, image.id || index)),
                                    images.length < maxImages && /*#__PURE__*/ (0, _jsxruntime.jsxs)("button", {
                                        type: "button",
                                        onClick: ()=>fileInputRef.current?.click(),
                                        className: "aspect-square border-2 border-dashed border-gray-300 rounded-lg flex flex-col items-center justify-center hover:border-blaze-orange-400 hover:bg-blaze-orange-50 transition-colors",
                                        children: [
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.Plus, {
                                                className: "w-8 h-8 text-gray-400"
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                className: "text-xs text-gray-500 mt-2",
                                                children: "Agregar mas"
                                            })
                                        ]
                                    })
                                ]
                            })
                        ]
                    }),
                    uploading && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "flex items-center space-x-2 text-sm text-gray-600",
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                className: "animate-spin rounded-full h-4 w-4 border-b-2 border-blaze-orange-600"
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                children: "Subiendo imagenes..."
                            })
                        ]
                    }),
                    images.length > 0 && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "bg-gray-50 border border-gray-200 rounded-lg p-4",
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("h5", {
                                className: "text-sm font-medium text-gray-800 mb-3",
                                children: "Resumen de Imagenes"
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                className: "grid grid-cols-2 md:grid-cols-4 gap-4 text-center",
                                children: [
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                        className: "bg-white rounded-lg p-3",
                                        children: [
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                className: "text-lg font-semibold text-gray-900",
                                                children: images.length
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                className: "text-xs text-gray-500",
                                                children: "Total"
                                            })
                                        ]
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                        className: "bg-white rounded-lg p-3",
                                        children: [
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                className: "text-lg font-semibold text-green-600",
                                                children: images.filter((img)=>img.upload_status === 'success').length
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                className: "text-xs text-gray-500",
                                                children: "Exitosas"
                                            })
                                        ]
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                        className: "bg-white rounded-lg p-3",
                                        children: [
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                className: "text-lg font-semibold text-blaze-orange-600",
                                                children: images.filter((img)=>img.upload_status === 'uploading').length
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                className: "text-xs text-gray-500",
                                                children: "Subiendo"
                                            })
                                        ]
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                        className: "bg-white rounded-lg p-3",
                                        children: [
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                className: "text-lg font-semibold text-red-600",
                                                children: images.filter((img)=>img.upload_status === 'error').length
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                className: "text-xs text-gray-500",
                                                children: "Con Error"
                                            })
                                        ]
                                    })
                                ]
                            }),
                            images.some((img)=>img.file_size) && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                className: "mt-3 pt-3 border-t border-gray-200",
                                children: [
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                        className: "flex justify-between items-center text-sm",
                                        children: [
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                className: "text-gray-600",
                                                children: "Tamano total:"
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                                className: "font-medium text-gray-900",
                                                children: [
                                                    (images.filter((img)=>img.file_size).reduce((total, img)=>total + (img.file_size || 0), 0) / (1024 * 1024)).toFixed(1),
                                                    ' ',
                                                    "MB"
                                                ]
                                            })
                                        ]
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                        className: "flex justify-between items-center text-sm mt-1",
                                        children: [
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                className: "text-gray-600",
                                                children: "Con texto alternativo:"
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                                className: "font-medium text-gray-900",
                                                children: [
                                                    images.filter((img)=>img.alt && img.alt.trim()).length,
                                                    " de ",
                                                    images.length
                                                ]
                                            })
                                        ]
                                    })
                                ]
                            })
                        ]
                    }),
                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "bg-blue-50 border border-blue-200 rounded-lg p-4",
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("h5", {
                                className: "text-sm font-medium text-blue-800 mb-2",
                                children: "Consejos para mejores imagenes:"
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("ul", {
                                className: "text-xs text-blue-700 space-y-1",
                                children: [
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("li", {
                                        children: "• Usa imagenes de alta calidad (minimo 800x800px)"
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("li", {
                                        children: "• La primera imagen sera la imagen principal del producto"
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("li", {
                                        children: "• Incluye diferentes angulos y detalles del producto"
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("li", {
                                        children: "• Agrega texto alternativo para mejorar la accesibilidad"
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("li", {
                                        children: "• Arrastra las imagenes para cambiar el orden"
                                    }),
                                    enableOptimization && /*#__PURE__*/ (0, _jsxruntime.jsx)("li", {
                                        children: "• Las imagenes se optimizan automaticamente para mejor rendimiento"
                                    })
                                ]
                            })
                        ]
                    })
                ]
            })
        })
    });
}

