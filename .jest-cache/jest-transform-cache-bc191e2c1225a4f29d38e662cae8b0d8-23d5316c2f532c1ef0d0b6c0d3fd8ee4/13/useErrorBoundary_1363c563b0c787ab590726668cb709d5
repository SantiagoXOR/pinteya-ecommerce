const useCriticalErrorBoundary = component => {
  /* istanbul ignore next */
  cov_n6u8pe1zh().f[23]++;
  cov_n6u8pe1zh().s[94]++;
  return useErrorBoundary({
    component,
    level: 'page',
    enableRetry: true,
    maxRetries: 2,
    retryDelay: 2000,
    onError: (error, errorInfo) => {
      /* istanbul ignore next */
      cov_n6u8pe1zh().f[24]++;
      cov_n6u8pe1zh().s[95]++;
      // Reportar errores crticos inmediatamente
      console.error(` CRITICAL ERROR in ${component}:`, error);
      // Enviar notificacin urgente
      /* istanbul ignore next */
      cov_n6u8pe1zh().s[96]++;
      if (typeof window !== 'undefined') {
        /* istanbul ignore next */
        cov_n6u8pe1zh().b[19][0]++;
        cov_n6u8pe1zh().s[97]++;
        fetch('/api/monitoring/critical-error', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify({
            component,
            error: {
              name: error.name,
              message: error.message,
              stack: error.stack
            },
            errorInfo,
            timestamp: Date.now(),
            url: window.location.href
          })
        }).catch(console.error);
      } else
      /* istanbul ignore next */
      {
        cov_n6u8pe1zh().b[19][1]++;
      }
    }
  });
};
