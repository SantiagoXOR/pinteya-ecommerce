    };
  }, []);
  // Auto-retry para ciertos tipos de errores
  /* istanbul ignore next */
  cov_n6u8pe1zh().s[57]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_n6u8pe1zh().f[15]++;
    cov_n6u8pe1zh().s[58]++;
    if (
    /* istanbul ignore next */
    (cov_n6u8pe1zh().b[11][0]++, hasError) &&
    /* istanbul ignore next */
    (cov_n6u8pe1zh().b[11][1]++, error) &&
    /* istanbul ignore next */
