  }, [component]));
  // ===================================
  // EFECTOS
  // ===================================
  // Listener para mtricas de errores
  /* istanbul ignore next */
  cov_n6u8pe1zh().s[49]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_n6u8pe1zh().f[11]++;
    cov_n6u8pe1zh().s[50]++;
    const handleErrorUpdate = errorMetric => {
      /* istanbul ignore next */
      cov_n6u8pe1zh().f[12]++;
      cov_n6u8pe1zh().s[51]++;
      setErrorMetrics(prev => {
        /* istanbul ignore next */
        cov_n6u8pe1zh().f[13]++;
        cov_n6u8pe1zh().s[52]++;
