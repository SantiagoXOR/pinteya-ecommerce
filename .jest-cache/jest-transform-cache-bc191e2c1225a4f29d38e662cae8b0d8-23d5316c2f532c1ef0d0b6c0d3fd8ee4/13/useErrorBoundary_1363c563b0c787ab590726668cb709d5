const useAsyncErrorBoundary = (asyncFn, dependencies =
/* istanbul ignore next */
(cov_n6u8pe1zh().b[14][0]++, []), options =
/* istanbul ignore next */
(cov_n6u8pe1zh().b[15][0]++, {})) => {
  /* istanbul ignore next */
  cov_n6u8pe1zh().f[20]++;
