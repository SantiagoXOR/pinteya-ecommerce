        /* istanbul ignore next */
        cov_n6u8pe1zh().b[18][0]++;
        cov_n6u8pe1zh().s[89]++;
        options.onFinally();
      } else
      /* istanbul ignore next */
      {
        cov_n6u8pe1zh().b[18][1]++;
      }
    }
  }, [asyncFn, errorBoundary, options]));
