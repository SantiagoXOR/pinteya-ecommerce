    } catch (error) {
      /* istanbul ignore next */
      cov_n6u8pe1zh().s[85]++;
      errorBoundary.captureError(error, {
        context: 'async_operation',
        operation:
        /* istanbul ignore next */
        (cov_n6u8pe1zh().b[17][0]++, asyncFn.name) ||
        /* istanbul ignore next */
        (cov_n6u8pe1zh().b[17][1]++, 'anonymous')
      });
      /* istanbul ignore next */
      cov_n6u8pe1zh().s[86]++;
      throw error;
    } finally {
      /* istanbul ignore next */
      cov_n6u8pe1zh().s[87]++;
      setIsLoading(false);
      /* istanbul ignore next */
      cov_n6u8pe1zh().s[88]++;
