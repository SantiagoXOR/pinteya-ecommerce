                children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.Trash2, {
                  className: "w-4 h-4"
                }), /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
                  children: "Eliminar Producto"
                })]
              })]
            })
          })]
        })]
      })]
    })
  });
}
