      cov_qxyiuxsma().s[158]++;
      if (
      /* istanbul ignore next */
      (cov_qxyiuxsma().b[60][0]++, sanitizedPhone.startsWith('54')) &&
      /* istanbul ignore next */
      (cov_qxyiuxsma().b[60][1]++, sanitizedPhone.length > 11)) {
        /* istanbul ignore next */
        cov_qxyiuxsma().b[59][0]++;
        cov_qxyiuxsma().s[159]++;
        sanitizedPhone = sanitizedPhone.substring(2); // Remover "54"
      } else
      /* istanbul ignore next */
      {
        cov_qxyiuxsma().b[59][1]++;
      }
      // Si empieza con 9 (código de celular), mantenerlo para que sea 11 dígitos
      // Si no empieza con 9, debería ser 10 dígitos (teléfono fijo)
      cov_qxyiuxsma().s[160]++;
      console.log('🔍 processExpressCheckout - Teléfono original:', billing.phone);
      /* istanbul ignore next */
      cov_qxyiuxsma().s[161]++;
      console.log('🔍 processExpressCheckout - Teléfono sanitizado:', sanitizedPhone);
      /* istanbul ignore next */
      cov_qxyiuxsma().s[162]++;
      console.log('🔍 processExpressCheckout - Longitud del teléfono:', sanitizedPhone.length);
      // Preparar datos para la API (Express Checkout - campos simplificados)
      const payload =
      /* istanbul ignore next */
      (cov_qxyiuxsma().s[163]++, {
        items: cartItems.map(item => {
          /* istanbul ignore next */
          cov_qxyiuxsma().f[24]++;
          cov_qxyiuxsma().s[164]++;
          return {
            id: item.id.toString(),
            name: item.title,
            price: item.discountedPrice,
            quantity: item.quantity,
            image:
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[61][0]++, item.imgs?.previews?.[0]) ||
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[61][1]++, '')
          };
        }),
        payer: {
          name:
          /* istanbul ignore next */
          (cov_qxyiuxsma().b[62][0]++, billing.firstName) ||
          /* istanbul ignore next */
          (cov_qxyiuxsma().b[62][1]++, 'Cliente'),
          surname:
          /* istanbul ignore next */
          (cov_qxyiuxsma().b[63][0]++, billing.lastName) ||
          /* istanbul ignore next */
          (cov_qxyiuxsma().b[63][1]++, 'Express'),
          email: billing.email,
          phone: sanitizedPhone
        },
        shipping: shippingCost > 0 ?
        /* istanbul ignore next */
        (cov_qxyiuxsma().b[64][0]++, {
          cost: shippingCost,
          address: {
            street_name: billing.streetAddress,
            street_number: "123",
            zip_code:
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[65][0]++, billing.zipCode) ||
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[65][1]++, "5000"),
            city_name:
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[66][0]++, billing.city) ||
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[66][1]++, "Córdoba"),
            state_name:
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[67][0]++, billing.state) ||
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[67][1]++, "Córdoba")
          }
        }) :
        /* istanbul ignore next */
        (cov_qxyiuxsma().b[64][1]++, undefined),
        external_reference: `express_checkout_${Date.now()}`
      });
      /* istanbul ignore next */
      cov_qxyiuxsma().s[165]++;
      console.log('🔍 processExpressCheckout - Payload enviado:', payload);
      // Llamar a la API
      const response =
      /* istanbul ignore next */
      (cov_qxyiuxsma().s[166]++, await fetch('/api/payments/create-preference', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json'
        },
        body: JSON.stringify(payload)
      }));
      const result =
      /* istanbul ignore next */
      (cov_qxyiuxsma().s[167]++, await response.json());
      /* istanbul ignore next */
      cov_qxyiuxsma().s[168]++;
      if (!result.success) {
        /* istanbul ignore next */
        cov_qxyiuxsma().b[68][0]++;
        cov_qxyiuxsma().s[169]++;
        throw new Error(
        /* istanbul ignore next */
        (cov_qxyiuxsma().b[69][0]++, result.error) ||
        /* istanbul ignore next */
        (cov_qxyiuxsma().b[69][1]++, 'Error procesando el pago'));
      } else
      /* istanbul ignore next */
      {
        cov_qxyiuxsma().b[68][1]++;
      }
      // ✅ SOLUCIÓN FINAL: Redirección directa a MercadoPago
      // Esto evita el problema de hooks con el Wallet Brick embebido
      cov_qxyiuxsma().s[170]++;
      console.log('🔄 Redirigiendo directamente a MercadoPago:', result.data.init_point);
      // Cambiar a estado de redirección inmediatamente
      /* istanbul ignore next */
      cov_qxyiuxsma().s[171]++;
      setCheckoutState(prev => {
        /* istanbul ignore next */
        cov_qxyiuxsma().f[25]++;
        cov_qxyiuxsma().s[172]++;
        return {
          ...prev,
          step: 'redirect',
          preferenceId: result.data.preference_id,
          initPoint: result.data.init_point,
          isLoading: false
        };
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_qxyiuxsma().s[173]++;
      console.error('Error en express checkout:', error);
      /* istanbul ignore next */
      cov_qxyiuxsma().s[174]++;
      setCheckoutState(prev => {
        /* istanbul ignore next */
        cov_qxyiuxsma().f[26]++;
        cov_qxyiuxsma().s[175]++;
        return {
          ...prev,
          isLoading: false,
          step: 'form',
          errors: {
            general:
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[70][0]++, error.message) ||
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[70][1]++, 'Error procesando el pago')
          }
        };
      });
    }
  }, [checkoutState.formData, cartItems, validateExpressForm, calculateShippingCost, dispatch]));
  // Procesar checkout completo
  const processCheckout =
  /* istanbul ignore next */
  (cov_qxyiuxsma().s[176]++, (0, _react.useCallback)(async () => {
    /* istanbul ignore next */
    cov_qxyiuxsma().f[27]++;
    cov_qxyiuxsma().s[177]++;
    if (!validateForm()) {
      /* istanbul ignore next */
      cov_qxyiuxsma().b[71][0]++;
      cov_qxyiuxsma().s[178]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_qxyiuxsma().b[71][1]++;
    }
    // Marcar que el checkout está en progreso
    cov_qxyiuxsma().s[179]++;
    sessionStorage.setItem('checkout-in-progress', 'true');
    /* istanbul ignore next */
    cov_qxyiuxsma().s[180]++;
    setCheckoutState(prev => {
      /* istanbul ignore next */
      cov_qxyiuxsma().f[28]++;
      cov_qxyiuxsma().s[181]++;
      return {
        ...prev,
        isLoading: true,
        step: 'processing'
      };
    });
    /* istanbul ignore next */
    cov_qxyiuxsma().s[182]++;
    try {
      const {
        billing,
        shipping
      } =
      /* istanbul ignore next */
      (cov_qxyiuxsma().s[183]++, checkoutState.formData);
      const shippingCost =
      /* istanbul ignore next */
      (cov_qxyiuxsma().s[184]++, calculateShippingCost());
      // Preparar datos para la API
      const payload =
      /* istanbul ignore next */
      (cov_qxyiuxsma().s[185]++, {
        items: cartItems.map(item => {
          /* istanbul ignore next */
          cov_qxyiuxsma().f[29]++;
          cov_qxyiuxsma().s[186]++;
          return {
            id: item.id.toString(),
            name: item.title,
            price: item.discountedPrice,
            quantity: item.quantity,
            image:
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[72][0]++, item.imgs?.previews?.[0]) ||
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[72][1]++, '')
          };
        }),
        payer: {
          name: billing.firstName,
          surname: billing.lastName,
          email: billing.email,
          phone: billing.phone
        },
        shipping: shippingCost > 0 ?
        /* istanbul ignore next */
        (cov_qxyiuxsma().b[73][0]++, {
          cost: shippingCost,
          address: {
            street_name: shipping.differentAddress ?
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[74][0]++, shipping.streetAddress) :
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[74][1]++, billing.streetAddress),
            street_number: "123",
            zip_code: shipping.differentAddress ?
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[75][0]++, shipping.zipCode) :
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[75][1]++, billing.zipCode),
            city_name: shipping.differentAddress ?
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[76][0]++, shipping.city) :
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[76][1]++, billing.city),
            state_name: shipping.differentAddress ?
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[77][0]++, shipping.state) :
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[77][1]++, billing.state)
          }
        }) :
        /* istanbul ignore next */
        (cov_qxyiuxsma().b[73][1]++, undefined),
        external_reference: `checkout_${Date.now()}`
      });
      // Llamar a la API
      const response =
      /* istanbul ignore next */
      (cov_qxyiuxsma().s[187]++, await fetch('/api/payments/create-preference', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json'
        },
        body: JSON.stringify(payload)
      }));
      const result =
      /* istanbul ignore next */
      (cov_qxyiuxsma().s[188]++, await response.json());
      /* istanbul ignore next */
      cov_qxyiuxsma().s[189]++;
      if (!result.success) {
        /* istanbul ignore next */
        cov_qxyiuxsma().b[78][0]++;
        cov_qxyiuxsma().s[190]++;
        throw new Error(
        /* istanbul ignore next */
        (cov_qxyiuxsma().b[79][0]++, result.error) ||
        /* istanbul ignore next */
        (cov_qxyiuxsma().b[79][1]++, 'Error procesando el pago'));
      } else
      /* istanbul ignore next */
      {
        cov_qxyiuxsma().b[78][1]++;
      }
      // Procesar resultado exitoso - mostrar paso de pago manual
      // NOTA: No limpiar el carrito aquí, se limpiará cuando el pago se complete exitosamente
      // ✅ CORREGIDO: Ir directamente a redirect para evitar error de hooks con MercadoPagoWallet
      cov_qxyiuxsma().s[191]++;
      console.log('🔄 Redirigiendo directamente a MercadoPago:', result.data.init_point);
      /* istanbul ignore next */
      cov_qxyiuxsma().s[192]++;
      setCheckoutState(prev => {
        /* istanbul ignore next */
        cov_qxyiuxsma().f[30]++;
        cov_qxyiuxsma().s[193]++;
        return {
          ...prev,
          step: 'redirect',
          preferenceId: result.data.preference_id,
          initPoint: result.data.init_point,
          isLoading: false
        };
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_qxyiuxsma().s[194]++;
      console.error('Error en checkout:', error);
      /* istanbul ignore next */
      cov_qxyiuxsma().s[195]++;
      setCheckoutState(prev => {
        /* istanbul ignore next */
        cov_qxyiuxsma().f[31]++;
        cov_qxyiuxsma().s[196]++;
        return {
          ...prev,
          isLoading: false,
          step: 'form',
          errors: {
            general:
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[80][0]++, error.message) ||
            /* istanbul ignore next */
            (cov_qxyiuxsma().b[80][1]++, 'Error procesando el pago')
          }
        };
      });
    }
  }, [checkoutState.formData, cartItems, validateForm, calculateShippingCost, dispatch]));
  // ===================================
  // CALLBACKS PARA WALLET BRICK
  // ===================================
  const handleWalletReady =
  /* istanbul ignore next */
  (cov_qxyiuxsma().s[197]++, (0, _react.useCallback)(() => {
    /* istanbul ignore next */
    cov_qxyiuxsma().f[32]++;
    cov_qxyiuxsma().s[198]++;
    console.log('Wallet Brick está listo');
  }, []));
  const handleWalletError =
  /* istanbul ignore next */
  (cov_qxyiuxsma().s[199]++, (0, _react.useCallback)(error => {
    /* istanbul ignore next */
    cov_qxyiuxsma().f[33]++;
    cov_qxyiuxsma().s[200]++;
    console.error('Error en Wallet Brick:', error);
    /* istanbul ignore next */
    cov_qxyiuxsma().s[201]++;
    setCheckoutState(prev => {
      /* istanbul ignore next */
      cov_qxyiuxsma().f[34]++;
      cov_qxyiuxsma().s[202]++;
      return {
        ...prev,
        errors: {
          payment:
          /* istanbul ignore next */
          (cov_qxyiuxsma().b[81][0]++, error.message) ||
          /* istanbul ignore next */
          (cov_qxyiuxsma().b[81][1]++, 'Error en el sistema de pagos')
        },
        isLoading: false
      };
    });
  }, []));
  const handleWalletSubmit =
  /* istanbul ignore next */
  (cov_qxyiuxsma().s[203]++, (0, _react.useCallback)(data => {
    /* istanbul ignore next */
    cov_qxyiuxsma().f[35]++;
    cov_qxyiuxsma().s[204]++;
    console.log('💳 Wallet Submit - Pago enviado desde Wallet Brick:', data);
    /* istanbul ignore next */
    cov_qxyiuxsma().s[205]++;
    console.log('💳 Wallet Submit - Cambiando a step redirect');
    // IMPORTANTE: NO limpiar el carrito aquí
    // El carrito se limpiará solo cuando el pago sea confirmado como exitoso
    /* istanbul ignore next */
    cov_qxyiuxsma().s[206]++;
    setCheckoutState(prev => {
      /* istanbul ignore next */
      cov_qxyiuxsma().f[36]++;
      cov_qxyiuxsma().s[207]++;
      return {
        ...prev,
        step: 'redirect',
        isLoading: false
      };
    });
  }, []));
  // Memorizar valores calculados para evitar recursión infinita
  const memoizedShippingCost =
  /* istanbul ignore next */
  (cov_qxyiuxsma().s[208]++, (0, _react.useMemo)(() => {
    /* istanbul ignore next */
    cov_qxyiuxsma().f[37]++;
    cov_qxyiuxsma().s[209]++;
    try {
      const cost =
      /* istanbul ignore next */
      (cov_qxyiuxsma().s[210]++, calculateShippingCost());
      /* istanbul ignore next */
      cov_qxyiuxsma().s[211]++;
      return /* istanbul ignore next */(cov_qxyiuxsma().b[83][0]++, typeof cost === 'number') &&
      /* istanbul ignore next */
      (cov_qxyiuxsma().b[83][1]++, !isNaN(cost)) ?
      /* istanbul ignore next */
      (cov_qxyiuxsma().b[82][0]++, cost) :
      /* istanbul ignore next */
      (cov_qxyiuxsma().b[82][1]++, 0);
    } catch (error) {
      /* istanbul ignore next */
      cov_qxyiuxsma().s[212]++;
      console.error('❌ Error calculating shippingCost:', error);
      /* istanbul ignore next */
      cov_qxyiuxsma().s[213]++;
      return 0;
    }
  }, [calculateShippingCost]));
  const memoizedDiscount =
  /* istanbul ignore next */
  (cov_qxyiuxsma().s[214]++, (0, _react.useMemo)(() => {
    /* istanbul ignore next */
    cov_qxyiuxsma().f[38]++;
    cov_qxyiuxsma().s[215]++;
    try {
      const disc =
      /* istanbul ignore next */
      (cov_qxyiuxsma().s[216]++, calculateDiscount());
      /* istanbul ignore next */
      cov_qxyiuxsma().s[217]++;
      return /* istanbul ignore next */(cov_qxyiuxsma().b[85][0]++, typeof disc === 'number') &&
      /* istanbul ignore next */
      (cov_qxyiuxsma().b[85][1]++, !isNaN(disc)) ?
      /* istanbul ignore next */
      (cov_qxyiuxsma().b[84][0]++, disc) :
      /* istanbul ignore next */
      (cov_qxyiuxsma().b[84][1]++, 0);
    } catch (error) {
      /* istanbul ignore next */
      cov_qxyiuxsma().s[218]++;
      console.error('❌ Error calculating discount:', error);
      /* istanbul ignore next */
      cov_qxyiuxsma().s[219]++;
      return 0;
    }
  }, [calculateDiscount]));
  const memoizedFinalTotal =
  /* istanbul ignore next */
  (cov_qxyiuxsma().s[220]++, (0, _react.useMemo)(() => {
    /* istanbul ignore next */
    cov_qxyiuxsma().f[39]++;
    cov_qxyiuxsma().s[221]++;
    try {
      const total =
      /* istanbul ignore next */
      (cov_qxyiuxsma().s[222]++, calculateTotal());
      /* istanbul ignore next */
      cov_qxyiuxsma().s[223]++;
      return /* istanbul ignore next */(cov_qxyiuxsma().b[87][0]++, typeof total === 'number') &&
      /* istanbul ignore next */
      (cov_qxyiuxsma().b[87][1]++, !isNaN(total)) ?
      /* istanbul ignore next */
      (cov_qxyiuxsma().b[86][0]++, total) :
      /* istanbul ignore next */
      (cov_qxyiuxsma().b[86][1]++, 0);
    } catch (error) {
      /* istanbul ignore next */
      cov_qxyiuxsma().s[224]++;
      console.error('❌ Error calculating finalTotal:', error);
      /* istanbul ignore next */
      cov_qxyiuxsma().s[225]++;
      return 0;
    }
  }, [calculateTotal]));
  /* istanbul ignore next */
  cov_qxyiuxsma().s[226]++;
  return {
    // Estado
    formData: checkoutState.formData,
    isLoading: checkoutState.isLoading,
    errors: checkoutState.errors,
    step: checkoutState.step,
    // Datos para Wallet Brick
    preferenceId: checkoutState.preferenceId,
    initPoint: checkoutState.initPoint,
    // Datos calculados
    cartItems,
    totalPrice,
    shippingCost: memoizedShippingCost,
    discount: memoizedDiscount,
    finalTotal: memoizedFinalTotal,
    // Cupones
    appliedCoupon,
    applyCoupon,
    // Acciones
    updateFormData,
    updateBillingData,
    updateShippingData,
    validateForm,
    validateExpressForm,
    processCheckout,
    processExpressCheckout,
    // Callbacks para Wallet Brick
    handleWalletReady,
    handleWalletError,
    handleWalletSubmit
  };
};
