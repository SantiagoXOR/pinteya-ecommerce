      /* istanbul ignore next */
      cov_uvfls65f5().s[25]++;
      setState(prev => {
        /* istanbul ignore next */
        cov_uvfls65f5().f[6]++;
        cov_uvfls65f5().s[26]++;
        return {
          ...prev,
          loading: false,
          error: errorMessage
        };
      });
    }
  }, [filters]));
  /**
  * Actualiza los filtros y obtiene productos
  */
  const updateFilters =
  /* istanbul ignore next */
  (cov_uvfls65f5().s[27]++, (0, _react.useCallback)(newFilters => {
    /* istanbul ignore next */
    cov_uvfls65f5().f[7]++;
    const updatedFilters =
    /* istanbul ignore next */
    (cov_uvfls65f5().s[28]++, {
      ...filters,
      ...newFilters
    });
    /* istanbul ignore next */
    cov_uvfls65f5().s[29]++;
    setFilters(updatedFilters);
    /* istanbul ignore next */
    cov_uvfls65f5().s[30]++;
    fetchProducts(updatedFilters);
  }, [filters, fetchProducts]));
  /**
  * Cambia la página
  */
  const changePage =
  /* istanbul ignore next */
  (cov_uvfls65f5().s[31]++, (0, _react.useCallback)(page => {
    /* istanbul ignore next */
    cov_uvfls65f5().f[8]++;
    cov_uvfls65f5().s[32]++;
    updateFilters({
      page
    });
  }, [updateFilters]));
  /**
  * Cambia el límite de productos por página
  */
  const changeLimit =
  /* istanbul ignore next */
  (cov_uvfls65f5().s[33]++, (0, _react.useCallback)(limit => {
    /* istanbul ignore next */
    cov_uvfls65f5().f[9]++;
    cov_uvfls65f5().s[34]++;
    updateFilters({
      limit,
      page: 1
    });
  }, [updateFilters]));
  /**
  * Cambia el ordenamiento
  */
  const changeSorting =
  /* istanbul ignore next */
  (cov_uvfls65f5().s[35]++, (0, _react.useCallback)((sortBy, sortOrder) => {
    /* istanbul ignore next */
    cov_uvfls65f5().f[10]++;
    cov_uvfls65f5().s[36]++;
    updateFilters({
      sortBy,
      sortOrder,
      page: 1
    });
  }, [updateFilters]));
  /**
  * Busca productos por término
  */
  const searchProducts =
  /* istanbul ignore next */
  (cov_uvfls65f5().s[37]++, (0, _react.useCallback)(searchTerm => {
    /* istanbul ignore next */
    cov_uvfls65f5().f[11]++;
    cov_uvfls65f5().s[38]++;
    updateFilters({
      search: searchTerm,
      page: 1
    });
  }, [updateFilters]));
  /**
  * Filtra por categoría
  */
  const filterByCategory =
  /* istanbul ignore next */
  (cov_uvfls65f5().s[39]++, (0, _react.useCallback)(category => {
    /* istanbul ignore next */
    cov_uvfls65f5().f[12]++;
    cov_uvfls65f5().s[40]++;
    updateFilters({
      category,
      page: 1
    });
  }, [updateFilters]));
  /**
  * Filtra por rango de precios
  */
  const filterByPriceRange =
  /* istanbul ignore next */
  (cov_uvfls65f5().s[41]++, (0, _react.useCallback)((priceMin, priceMax) => {
    /* istanbul ignore next */
    cov_uvfls65f5().f[13]++;
    cov_uvfls65f5().s[42]++;
    updateFilters({
      priceMin,
      priceMax,
      page: 1
    });
  }, [updateFilters]));
  /**
  * Limpia todos los filtros
  */
  const clearFilters =
  /* istanbul ignore next */
  (cov_uvfls65f5().s[43]++, (0, _react.useCallback)(() => {
    /* istanbul ignore next */
    cov_uvfls65f5().f[14]++;
    const clearedFilters =
    /* istanbul ignore next */
    (cov_uvfls65f5().s[44]++, {
      page: 1,
      limit:
      /* istanbul ignore next */
      (cov_uvfls65f5().b[8][0]++, filters.limit) ||
      /* istanbul ignore next */
      (cov_uvfls65f5().b[8][1]++, 12),
      sortBy: 'created_at',
      sortOrder: 'desc'
    });
    /* istanbul ignore next */
    cov_uvfls65f5().s[45]++;
    setFilters(clearedFilters);
    /* istanbul ignore next */
    cov_uvfls65f5().s[46]++;
    fetchProducts(clearedFilters);
  }, [filters.limit, fetchProducts]));
  /**
  * Refresca los productos
  */
  const refresh =
  /* istanbul ignore next */
  (cov_uvfls65f5().s[47]++, (0, _react.useCallback)(() => {
    /* istanbul ignore next */
    cov_uvfls65f5().f[15]++;
    cov_uvfls65f5().s[48]++;
    fetchProducts();
  }, [fetchProducts]));
  // Auto-fetch al montar el componente
  /* istanbul ignore next */
  cov_uvfls65f5().s[49]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_uvfls65f5().f[16]++;
    cov_uvfls65f5().s[50]++;
    if (autoFetch) {
      /* istanbul ignore next */
      cov_uvfls65f5().b[9][0]++;
      cov_uvfls65f5().s[51]++;
      fetchProducts();
    } else
    /* istanbul ignore next */
    {
      cov_uvfls65f5().b[9][1]++;
    }
  }, [autoFetch]); // Removido fetchProducts de las dependencias para evitar bucles
  // Cleanup effect para cancelar requests pendientes
  /* istanbul ignore next */
  cov_uvfls65f5().s[52]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_uvfls65f5().f[17]++;
    cov_uvfls65f5().s[53]++;
    return () => {
      /* istanbul ignore next */
      cov_uvfls65f5().f[18]++;
      cov_uvfls65f5().s[54]++;
      if (abortControllerRef.current) {
        /* istanbul ignore next */
        cov_uvfls65f5().b[10][0]++;
        cov_uvfls65f5().s[55]++;
        abortControllerRef.current.abort();
      } else
      /* istanbul ignore next */
      {
        cov_uvfls65f5().b[10][1]++;
      }
    };
  }, []);
  // Memoized helpers para optimizar renders
  const helpers =
  /* istanbul ignore next */
  (cov_uvfls65f5().s[56]++, (0, _react.useMemo)(() => {
    /* istanbul ignore next */
    cov_uvfls65f5().f[19]++;
    cov_uvfls65f5().s[57]++;
    return {
      hasProducts: state.products.length > 0,
      isEmpty:
      /* istanbul ignore next */
      (cov_uvfls65f5().b[11][0]++, !state.loading) &&
      /* istanbul ignore next */
      (cov_uvfls65f5().b[11][1]++, state.products.length === 0),
      hasError: !!state.error,
      hasNextPage: state.pagination.page < state.pagination.totalPages,
      hasPrevPage: state.pagination.page > 1
    };
  }, [state.products.length, state.loading, state.error, state.pagination.page, state.pagination.totalPages]));
  /* istanbul ignore next */
  cov_uvfls65f5().s[58]++;
  return {
    // Estado
    products: state.products,
    loading: state.loading,
    error: state.error,
    pagination: state.pagination,
    filters,
    // Acciones
    fetchProducts,
    updateFilters,
    changePage,
    changeLimit,
    changeSorting,
    searchProducts,
    filterByCategory,
    filterByPriceRange,
    clearFilters,
    refresh,
    // Helpers memoizados
    ...helpers
  };
}
