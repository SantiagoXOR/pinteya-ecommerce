  /* istanbul ignore next */
  {
    cov_2a4mqo9dj1().b[0][1]++;
  }
  cov_2a4mqo9dj1().s[24]++;
  if (file.size > maxSize) {
    /* istanbul ignore next */
    cov_2a4mqo9dj1().b[1][0]++;
    cov_2a4mqo9dj1().s[25]++;
    throw new _errorhandler.ValidationError('El archivo es demasiado grande. M√°ximo 5MB');
