        (cov_25is6muth0().s[21]++, defaultAddresses.slice(1).map(addr => {
          /* istanbul ignore next */
          cov_25is6muth0().f[2]++;
          cov_25is6muth0().s[22]++;
          return addr.id;
        }));
        /* istanbul ignore next */
        cov_25is6muth0().s[23]++;
        console.log(`üîÑ Desmarcando ${idsToUpdate.length} direcciones predeterminadas duplicadas`);
        /* istanbul ignore next */
        cov_25is6muth0().s[24]++;
        console.log('üîÑ Manteniendo como predeterminada:', keepDefaultId);
        /* istanbul ignore next */
        cov_25is6muth0().s[25]++;
        await _supabase.supabaseAdmin.from('user_addresses').update({
          is_default: false
        }).in('id', idsToUpdate);
      } else {
        /* istanbul ignore next */
        cov_25is6muth0().b[4][1]++;
        cov_25is6muth0().s[26]++;
        console.log('‚úÖ Usuario tiene exactamente una direcci√≥n predeterminada');
      }
    }
  } catch (error) {
    /* istanbul ignore next */
    cov_25is6muth0().s[27]++;
    console.error('‚ùå Error en ensureOneDefaultAddress:', error);
  }
}
async function POST(request, context) {
  /* istanbul ignore next */
  cov_25is6muth0().f[3]++;
  cov_25is6muth0().s[28]++;
  try {
    /* istanbul ignore next */
    cov_25is6muth0().s[29]++;
