    /* istanbul ignore next */
    (cov_25is6muth0().s[33]++, await (0, _auth.auth)());
    /* istanbul ignore next */
    cov_25is6muth0().s[34]++;
    if (!session?.user?.id) {
      /* istanbul ignore next */
