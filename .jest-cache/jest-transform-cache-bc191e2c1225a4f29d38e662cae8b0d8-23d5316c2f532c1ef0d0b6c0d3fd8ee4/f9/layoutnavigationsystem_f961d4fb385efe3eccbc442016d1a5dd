    /* istanbul ignore next */
    cov_bhm5hb1cz().f[11]++;
    const classes =
    /* istanbul ignore next */
    (cov_bhm5hb1cz().s[23]++, []);
    /* istanbul ignore next */
    cov_bhm5hb1cz().s[24]++;
    if (columns.mobile) {
      /* istanbul ignore next */
      cov_bhm5hb1cz().b[0][0]++;
      cov_bhm5hb1cz().s[25]++;
      classes.push(`grid-cols-${columns.mobile}`);
    } else
    /* istanbul ignore next */
    {
      cov_bhm5hb1cz().b[0][1]++;
    }
    cov_bhm5hb1cz().s[26]++;
    if (columns.tablet) {
      /* istanbul ignore next */
      cov_bhm5hb1cz().b[1][0]++;
      cov_bhm5hb1cz().s[27]++;
      classes.push(`md:grid-cols-${columns.tablet}`);
    } else
    /* istanbul ignore next */
    {
      cov_bhm5hb1cz().b[1][1]++;
    }
    cov_bhm5hb1cz().s[28]++;
    if (columns.desktop) {
      /* istanbul ignore next */
      cov_bhm5hb1cz().b[2][0]++;
      cov_bhm5hb1cz().s[29]++;
      classes.push(`lg:grid-cols-${columns.desktop}`);
    } else
    /* istanbul ignore next */
    {
      cov_bhm5hb1cz().b[2][1]++;
    }
    cov_bhm5hb1cz().s[30]++;
    return classes.join(' ');
  },
  // Generar espaciado responsivo
  generateSpacing: spacing => {
    /* istanbul ignore next */
    cov_bhm5hb1cz().f[12]++;
    const classes =
    /* istanbul ignore next */
    (cov_bhm5hb1cz().s[31]++, []);
    /* istanbul ignore next */
    cov_bhm5hb1cz().s[32]++;
    if (spacing.mobile) {
      /* istanbul ignore next */
      cov_bhm5hb1cz().b[3][0]++;
      cov_bhm5hb1cz().s[33]++;
      classes.push(`p-${spacing.mobile}`);
    } else
    /* istanbul ignore next */
    {
      cov_bhm5hb1cz().b[3][1]++;
    }
    cov_bhm5hb1cz().s[34]++;
    if (spacing.tablet) {
      /* istanbul ignore next */
      cov_bhm5hb1cz().b[4][0]++;
      cov_bhm5hb1cz().s[35]++;
      classes.push(`md:p-${spacing.tablet}`);
    } else
    /* istanbul ignore next */
    {
      cov_bhm5hb1cz().b[4][1]++;
    }
    cov_bhm5hb1cz().s[36]++;
    if (spacing.desktop) {
      /* istanbul ignore next */
      cov_bhm5hb1cz().b[5][0]++;
      cov_bhm5hb1cz().s[37]++;
      classes.push(`lg:p-${spacing.desktop}`);
    } else
    /* istanbul ignore next */
    {
      cov_bhm5hb1cz().b[5][1]++;
    }
    cov_bhm5hb1cz().s[38]++;
    return classes.join(' ');
  },
  // Calcular ancho de contenedor
  getContainerWidth: breakpoint => {
    /* istanbul ignore next */
    cov_bhm5hb1cz().f[13]++;
    cov_bhm5hb1cz().s[39]++;
    return /* istanbul ignore next */(cov_bhm5hb1cz().b[6][0]++, gridSystem.container.maxWidth[breakpoint]) ||
    /* istanbul ignore next */
    (cov_bhm5hb1cz().b[6][1]++, '100%');
  }
});
