        }, {
          status: 500
        });
      } else
      /* istanbul ignore next */
      {
        cov_1317zrw0e7().b[18][1]++;
      }
      cov_1317zrw0e7().s[92]++;
      console.log(`â Cart API: Producto ${product.name} agregado al carrito exitosamente`);
      /* istanbul ignore next */
      cov_1317zrw0e7().s[93]++;
      return _server.NextResponse.json({
