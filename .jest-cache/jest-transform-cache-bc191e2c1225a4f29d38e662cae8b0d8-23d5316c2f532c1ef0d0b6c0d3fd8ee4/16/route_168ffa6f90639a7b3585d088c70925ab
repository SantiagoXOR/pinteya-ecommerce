    const message =
    /* istanbul ignore next */
    (cov_23akv3pmqi().s[282]++, deleteResult.deleted ?
    /* istanbul ignore next */
    (cov_23akv3pmqi().b[86][0]++, 'Promoción eliminada exitosamente') :
    /* istanbul ignore next */
