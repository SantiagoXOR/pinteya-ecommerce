  (cov_23akv3pmqi().s[21]++, await supabase.from('profiles').select('role').eq('id', session.user.id).single());
  /* istanbul ignore next */
  cov_23akv3pmqi().s[22]++;
  if (!['admin', 'manager'].includes(profile?.role)) {
