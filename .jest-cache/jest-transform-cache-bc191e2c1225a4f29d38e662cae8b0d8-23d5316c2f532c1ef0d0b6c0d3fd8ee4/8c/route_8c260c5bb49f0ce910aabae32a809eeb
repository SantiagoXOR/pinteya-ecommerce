      cov_107lepzjbt().s[30]++;
      return _server.NextResponse.json({
        error: 'No autorizado'
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_107lepzjbt().b[11][1]++;
    }
    // Obtener parÃ¡metros de consulta
    const {
      searchParams
    } =
    /* istanbul ignore next */
    (cov_107lepzjbt().s[31]++, new URL(request.url));
    const filters =
    /* istanbul ignore next */
    (cov_107lepzjbt().s[32]++, Object.fromEntries(searchParams.entries()));
    // Validar filtros
    const validationResult =
    /* istanbul ignore next */
    (cov_107lepzjbt().s[33]++, ExportFiltersSchema.safeParse(filters));
    /* istanbul ignore next */
    cov_107lepzjbt().s[34]++;
    if (!validationResult.success) {
      /* istanbul ignore next */
      cov_107lepzjbt().b[12][0]++;
      cov_107lepzjbt().s[35]++;
      return _server.NextResponse.json({
        error: 'Filtros invÃ¡lidos',
        details: validationResult.error.errors
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_107lepzjbt().b[12][1]++;
    }
    const validatedFilters =
    /* istanbul ignore next */
    (cov_107lepzjbt().s[36]++, validationResult.data);
    // Construir query base
    let query =
    /* istanbul ignore next */
    (cov_107lepzjbt().s[37]++, supabase.from('products').select(`
        id,
        name,
        slug,
        description,
        price,
        discounted_price,
        stock,
        sku,
        brand,
        is_active,
        is_featured,
        created_at,
        updated_at,
        categories (
          id,
          name
        )
      `));
    // Aplicar filtros
    /* istanbul ignore next */
    cov_107lepzjbt().s[38]++;
    if (validatedFilters.category_id) {
      /* istanbul ignore next */
      cov_107lepzjbt().b[13][0]++;
      cov_107lepzjbt().s[39]++;
      query = query.eq('category_id', parseInt(validatedFilters.category_id));
    } else
    /* istanbul ignore next */
    {
      cov_107lepzjbt().b[13][1]++;
    }
    cov_107lepzjbt().s[40]++;
    if (validatedFilters.brand) {
      /* istanbul ignore next */
      cov_107lepzjbt().b[14][0]++;
      cov_107lepzjbt().s[41]++;
      query = query.ilike('brand', `%${validatedFilters.brand}%`);
    } else
    /* istanbul ignore next */
    {
      cov_107lepzjbt().b[14][1]++;
    }
    cov_107lepzjbt().s[42]++;
    if (
    /* istanbul ignore next */
    (cov_107lepzjbt().b[16][0]++, validatedFilters.status) &&
    /* istanbul ignore next */
    (cov_107lepzjbt().b[16][1]++, validatedFilters.status !== 'all')) {
      /* istanbul ignore next */
      cov_107lepzjbt().b[15][0]++;
      cov_107lepzjbt().s[43]++;
      query = query.eq('is_active', validatedFilters.status === 'active');
    } else
    /* istanbul ignore next */
    {
      cov_107lepzjbt().b[15][1]++;
    }
    cov_107lepzjbt().s[44]++;
    if (
    /* istanbul ignore next */
    (cov_107lepzjbt().b[18][0]++, validatedFilters.stock_status) &&
    /* istanbul ignore next */
    (cov_107lepzjbt().b[18][1]++, validatedFilters.stock_status !== 'all')) {
      /* istanbul ignore next */
      cov_107lepzjbt().b[17][0]++;
      cov_107lepzjbt().s[45]++;
      switch (validatedFilters.stock_status) {
        case 'out_of_stock':
          /* istanbul ignore next */
          cov_107lepzjbt().b[19][0]++;
          cov_107lepzjbt().s[46]++;
          query = query.eq('stock', 0);
          /* istanbul ignore next */
          cov_107lepzjbt().s[47]++;
          break;
        case 'low_stock':
          /* istanbul ignore next */
          cov_107lepzjbt().b[19][1]++;
          cov_107lepzjbt().s[48]++;
          query = query.gt('stock', 0).lte('stock', 10);
          /* istanbul ignore next */
          cov_107lepzjbt().s[49]++;
          break;
        case 'in_stock':
          /* istanbul ignore next */
          cov_107lepzjbt().b[19][2]++;
          cov_107lepzjbt().s[50]++;
          query = query.gt('stock', 10);
          /* istanbul ignore next */
          cov_107lepzjbt().s[51]++;
          break;
      }
    } else
    /* istanbul ignore next */
    {
      cov_107lepzjbt().b[17][1]++;
    }
    cov_107lepzjbt().s[52]++;
    if (validatedFilters.price_min) {
      /* istanbul ignore next */
      cov_107lepzjbt().b[20][0]++;
      cov_107lepzjbt().s[53]++;
      query = query.gte('price', parseFloat(validatedFilters.price_min));
    } else
    /* istanbul ignore next */
    {
      cov_107lepzjbt().b[20][1]++;
    }
    cov_107lepzjbt().s[54]++;
    if (validatedFilters.price_max) {
      /* istanbul ignore next */
      cov_107lepzjbt().b[21][0]++;
      cov_107lepzjbt().s[55]++;
      query = query.lte('price', parseFloat(validatedFilters.price_max));
    } else
    /* istanbul ignore next */
    {
      cov_107lepzjbt().b[21][1]++;
    }
    cov_107lepzjbt().s[56]++;
    if (validatedFilters.created_from) {
      /* istanbul ignore next */
      cov_107lepzjbt().b[22][0]++;
      cov_107lepzjbt().s[57]++;
      query = query.gte('created_at', validatedFilters.created_from);
    } else
    /* istanbul ignore next */
    {
      cov_107lepzjbt().b[22][1]++;
    }
    cov_107lepzjbt().s[58]++;
    if (validatedFilters.created_to) {
      /* istanbul ignore next */
      cov_107lepzjbt().b[23][0]++;
      cov_107lepzjbt().s[59]++;
      query = query.lte('created_at', validatedFilters.created_to);
    } else
    /* istanbul ignore next */
    {
      cov_107lepzjbt().b[23][1]++;
    }
    // Ordenar por fecha de creaciÃ³n (mÃ¡s recientes primero)
    cov_107lepzjbt().s[60]++;
    query = query.order('created_at', {
      ascending: false
    });
    // Limitar a 10,000 productos para evitar problemas de memoria
    /* istanbul ignore next */
    cov_107lepzjbt().s[61]++;
    query = query.limit(10000);
    // Ejecutar consulta
    const {
      data: products,
      error
    } =
    /* istanbul ignore next */
    (cov_107lepzjbt().s[62]++, await query);
    /* istanbul ignore next */
    cov_107lepzjbt().s[63]++;
    if (error) {
      /* istanbul ignore next */
      cov_107lepzjbt().b[24][0]++;
      cov_107lepzjbt().s[64]++;
      console.error('Error obteniendo productos para exportaciÃ³n:', error);
      /* istanbul ignore next */
      cov_107lepzjbt().s[65]++;
      return _server.NextResponse.json({
        error: 'Error al obtener productos'
      }, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_107lepzjbt().b[24][1]++;
    }
    cov_107lepzjbt().s[66]++;
    if (
    /* istanbul ignore next */
    (cov_107lepzjbt().b[26][0]++, !products) ||
    /* istanbul ignore next */
    (cov_107lepzjbt().b[26][1]++, products.length === 0)) {
      /* istanbul ignore next */
      cov_107lepzjbt().b[25][0]++;
      cov_107lepzjbt().s[67]++;
      return _server.NextResponse.json({
        error: 'No se encontraron productos para exportar'
      }, {
        status: 404
      });
    } else
    /* istanbul ignore next */
    {
      cov_107lepzjbt().b[25][1]++;
    }
    // Transformar datos para incluir nombre de categorÃ­a
    const transformedProducts =
    /* istanbul ignore next */
    (cov_107lepzjbt().s[68]++, products.map(product => {
      /* istanbul ignore next */
      cov_107lepzjbt().f[10]++;
      cov_107lepzjbt().s[69]++;
      return {
        ...product,
        category_name:
        /* istanbul ignore next */
        (cov_107lepzjbt().b[27][0]++, product.categories?.name) ||
        /* istanbul ignore next */
        (cov_107lepzjbt().b[27][1]++, 'Sin categorÃ­a')
      };
    }));
    // Generar CSV
    const csvContent =
    /* istanbul ignore next */
    (cov_107lepzjbt().s[70]++, generateCSV(transformedProducts));
    // Crear nombre de archivo con timestamp
    const timestamp =
    /* istanbul ignore next */
    (cov_107lepzjbt().s[71]++, new Date().toISOString().split('T')[0]);
    const filename =
    /* istanbul ignore next */
    (cov_107lepzjbt().s[72]++, `productos-pinteya-${timestamp}.csv`);
    // Log de la exportaciÃ³n
    /* istanbul ignore next */
    cov_107lepzjbt().s[73]++;
    console.log('â ExportaciÃ³n completada:', {
      products_count: products.length,
      filters: validatedFilters,
      user_id: session.user.id,
      timestamp: new Date().toISOString()
    });
    // Retornar archivo CSV
    /* istanbul ignore next */
    cov_107lepzjbt().s[74]++;
    return new _server.NextResponse(csvContent, {
      status: 200,
      headers: {
        'Content-Type': 'text/csv; charset=utf-8',
        'Content-Disposition': `attachment; filename="${filename}"`,
        'Cache-Control': 'no-cache, no-store, must-revalidate',
        'Pragma': 'no-cache',
        'Expires': '0'
      }
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_107lepzjbt().s[75]++;
    console.error('â Error en exportaciÃ³n de productos:', error);
    /* istanbul ignore next */
    cov_107lepzjbt().s[76]++;
    return _server.NextResponse.json({
      error: 'Error interno del servidor',
      details: error instanceof Error ?
      /* istanbul ignore next */
      (cov_107lepzjbt().b[28][0]++, error.message) :
      /* istanbul ignore next */
      (cov_107lepzjbt().b[28][1]++, 'Unknown error')
    }, {
      status: 500
    });
  }
}
async function POST(request) {
  /* istanbul ignore next */
  cov_107lepzjbt().f[11]++;
  cov_107lepzjbt().s[77]++;
  try {
    // Verificar autenticaciÃ³n
    const session =
    /* istanbul ignore next */
    (cov_107lepzjbt().s[78]++, await (0, _config.auth)());
    /* istanbul ignore next */
    cov_107lepzjbt().s[79]++;
    if (!session?.user) {
      /* istanbul ignore next */
      cov_107lepzjbt().b[29][0]++;
      cov_107lepzjbt().s[80]++;
      return _server.NextResponse.json({
        error: 'No autorizado'
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_107lepzjbt().b[29][1]++;
    }
    // Obtener filtros del body
    const body =
    /* istanbul ignore next */
    (cov_107lepzjbt().s[81]++, await request.json());
    const validationResult =
    /* istanbul ignore next */
    (cov_107lepzjbt().s[82]++, ExportFiltersSchema.safeParse(body));
    /* istanbul ignore next */
    cov_107lepzjbt().s[83]++;
    if (!validationResult.success) {
      /* istanbul ignore next */
      cov_107lepzjbt().b[30][0]++;
      cov_107lepzjbt().s[84]++;
      return _server.NextResponse.json({
        error: 'Filtros invÃ¡lidos',
        details: validationResult.error.errors
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_107lepzjbt().b[30][1]++;
    }
    const filters =
    /* istanbul ignore next */
    (cov_107lepzjbt().s[85]++, validationResult.data);
    // Contar productos que coinciden con los filtros
    let countQuery =
    /* istanbul ignore next */
    (cov_107lepzjbt().s[86]++, supabase.from('products').select('id', {
      count: 'exact',
      head: true
    }));
    // Aplicar los mismos filtros que en GET
    /* istanbul ignore next */
    cov_107lepzjbt().s[87]++;
    if (filters.category_id) {
      /* istanbul ignore next */
      cov_107lepzjbt().b[31][0]++;
      cov_107lepzjbt().s[88]++;
      countQuery = countQuery.eq('category_id', parseInt(filters.category_id));
    } else
    /* istanbul ignore next */
    {
      cov_107lepzjbt().b[31][1]++;
    }
    cov_107lepzjbt().s[89]++;
    if (filters.brand) {
      /* istanbul ignore next */
      cov_107lepzjbt().b[32][0]++;
      cov_107lepzjbt().s[90]++;
      countQuery = countQuery.ilike('brand', `%${filters.brand}%`);
    } else
    /* istanbul ignore next */
    {
      cov_107lepzjbt().b[32][1]++;
    }
    cov_107lepzjbt().s[91]++;
    if (
    /* istanbul ignore next */
    (cov_107lepzjbt().b[34][0]++, filters.status) &&
    /* istanbul ignore next */
    (cov_107lepzjbt().b[34][1]++, filters.status !== 'all')) {
      /* istanbul ignore next */
      cov_107lepzjbt().b[33][0]++;
      cov_107lepzjbt().s[92]++;
      countQuery = countQuery.eq('is_active', filters.status === 'active');
    } else
    /* istanbul ignore next */
    {
      cov_107lepzjbt().b[33][1]++;
    }
    cov_107lepzjbt().s[93]++;
    if (
    /* istanbul ignore next */
    (cov_107lepzjbt().b[36][0]++, filters.stock_status) &&
    /* istanbul ignore next */
    (cov_107lepzjbt().b[36][1]++, filters.stock_status !== 'all')) {
      /* istanbul ignore next */
      cov_107lepzjbt().b[35][0]++;
      cov_107lepzjbt().s[94]++;
      switch (filters.stock_status) {
        case 'out_of_stock':
          /* istanbul ignore next */
          cov_107lepzjbt().b[37][0]++;
          cov_107lepzjbt().s[95]++;
          countQuery = countQuery.eq('stock', 0);
          /* istanbul ignore next */
          cov_107lepzjbt().s[96]++;
          break;
        case 'low_stock':
          /* istanbul ignore next */
          cov_107lepzjbt().b[37][1]++;
          cov_107lepzjbt().s[97]++;
          countQuery = countQuery.gt('stock', 0).lte('stock', 10);
          /* istanbul ignore next */
          cov_107lepzjbt().s[98]++;
          break;
        case 'in_stock':
          /* istanbul ignore next */
          cov_107lepzjbt().b[37][2]++;
          cov_107lepzjbt().s[99]++;
          countQuery = countQuery.gt('stock', 10);
          /* istanbul ignore next */
          cov_107lepzjbt().s[100]++;
          break;
      }
    } else
    /* istanbul ignore next */
    {
      cov_107lepzjbt().b[35][1]++;
    }
    const {
      count,
      error
    } =
    /* istanbul ignore next */
    (cov_107lepzjbt().s[101]++, await countQuery);
    /* istanbul ignore next */
    cov_107lepzjbt().s[102]++;
    if (error) {
      /* istanbul ignore next */
      cov_107lepzjbt().b[38][0]++;
      cov_107lepzjbt().s[103]++;
      console.error('Error contando productos:', error);
      /* istanbul ignore next */
      cov_107lepzjbt().s[104]++;
      return _server.NextResponse.json({
        error: 'Error al contar productos'
      }, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_107lepzjbt().b[38][1]++;
    }
    cov_107lepzjbt().s[105]++;
    return _server.NextResponse.json({
      success: true,
      data: {
        estimated_products:
        /* istanbul ignore next */
        (cov_107lepzjbt().b[39][0]++, count) ||
        /* istanbul ignore next */
        (cov_107lepzjbt().b[39][1]++, 0),
        max_products: 10000,
        estimated_file_size: `${Math.round((
        /* istanbul ignore next */
        (cov_107lepzjbt().b[40][0]++, count) ||
        /* istanbul ignore next */
        (cov_107lepzjbt().b[40][1]++, 0)) * 0.5)}KB`,
        supported_formats: ['CSV'],
        filters_applied: filters
      }
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_107lepzjbt().s[106]++;
    console.error('â Error obteniendo informaciÃ³n de exportaciÃ³n:', error);
    /* istanbul ignore next */
    cov_107lepzjbt().s[107]++;
    return _server.NextResponse.json({
      error: 'Error interno del servidor'
    }, {
      status: 500
    });
  }
}
