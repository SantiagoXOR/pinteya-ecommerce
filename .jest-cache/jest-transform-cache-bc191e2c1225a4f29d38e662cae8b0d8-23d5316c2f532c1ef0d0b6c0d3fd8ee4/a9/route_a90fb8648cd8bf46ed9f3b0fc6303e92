    // Registrar actividad de actualización de perfil
    const requestInfo =
    /* istanbul ignore next */
    (cov_1uhfcmk0k().s[66]++, (0, _activityLogger.getRequestInfo)(request));
    /* istanbul ignore next */
    cov_1uhfcmk0k().s[67]++;
