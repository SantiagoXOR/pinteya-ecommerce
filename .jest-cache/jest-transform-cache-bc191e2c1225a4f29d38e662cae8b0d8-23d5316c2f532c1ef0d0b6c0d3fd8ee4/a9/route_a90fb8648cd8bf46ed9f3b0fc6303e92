    /* istanbul ignore next */
    cov_1uhfcmk0k().b[8][0]++;
    cov_1uhfcmk0k().s[46]++;
    securityLogger.logRateLimitExceeded(securityLogger.context, {
      endpoint: '/api/user/profile',
      method: 'GET'
