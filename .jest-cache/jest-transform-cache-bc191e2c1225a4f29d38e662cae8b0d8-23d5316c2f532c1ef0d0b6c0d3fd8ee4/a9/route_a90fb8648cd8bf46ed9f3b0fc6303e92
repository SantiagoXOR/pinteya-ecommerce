      /* istanbul ignore next */
      (cov_1uhfcmk0k().b[16][0]++, 'changed') :
      /* istanbul ignore next */
      (cov_1uhfcmk0k().b[16][1]++, 'unchanged'),
      previous_phone: updatedUser.phone !== phone ?
      /* istanbul ignore next */
      (cov_1uhfcmk0k().b[17][0]++, 'changed') :
      /* istanbul ignore next */
