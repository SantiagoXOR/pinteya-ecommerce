}
async function PUT(request) {
  /* istanbul ignore next */
  cov_1uhfcmk0k().f[6]++;
  cov_1uhfcmk0k().s[49]++;
  try {
    /* istanbul ignore next */
    cov_1uhfcmk0k().s[50]++;
    // Verificar que el cliente administrativo esté disponible
    if (!_supabase.supabaseAdmin) {
      /* istanbul ignore next */
      cov_1uhfcmk0k().b[9][0]++;
      cov_1uhfcmk0k().s[51]++;
