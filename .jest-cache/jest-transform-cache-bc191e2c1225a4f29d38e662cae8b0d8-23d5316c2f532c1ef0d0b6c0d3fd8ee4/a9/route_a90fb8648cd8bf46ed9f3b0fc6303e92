      /* istanbul ignore next */
      {
        cov_1uhfcmk0k().b[2][1]++;
      }
      // Si no existe el usuario, crear uno demo
      cov_1uhfcmk0k().s[34]++;
      if (!user) {
        /* istanbul ignore next */
        cov_1uhfcmk0k().b[4][0]++;
        const {
          data: newUser,
          error: createError
        } =
        /* istanbul ignore next */
        (cov_1uhfcmk0k().s[35]++, await _supabase.supabaseAdmin.from('users').insert([{
          clerk_id: userId,
          email: 'usuario@demo.com',
