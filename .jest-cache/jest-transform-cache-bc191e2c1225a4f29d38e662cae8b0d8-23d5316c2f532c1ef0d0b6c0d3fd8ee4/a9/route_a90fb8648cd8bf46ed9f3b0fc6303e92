        /* istanbul ignore next */
        {
          cov_1uhfcmk0k().b[5][1]++;
        }
        cov_1uhfcmk0k().s[39]++;
        return _server.NextResponse.json({
          success: true,
          user: newUser
        });
      } else
      /* istanbul ignore next */
      {
        cov_1uhfcmk0k().b[4][1]++;
      }
      // Log de operación exitosa
      cov_1uhfcmk0k().s[40]++;
      securityLogger.logEvent('user_profile_retrieved', 'low', {
        userId: userId,
        hasUser: !!user
      });
      /* istanbul ignore next */
      cov_1uhfcmk0k().s[41]++;
      return _server.NextResponse.json({
        success: true,
