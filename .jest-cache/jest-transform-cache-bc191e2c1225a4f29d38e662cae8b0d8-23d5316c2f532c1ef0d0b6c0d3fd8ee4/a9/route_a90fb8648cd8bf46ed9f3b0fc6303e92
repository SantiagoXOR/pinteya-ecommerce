          /* istanbul ignore next */
          cov_1uhfcmk0k().b[5][0]++;
          cov_1uhfcmk0k().s[37]++;
          console.error('Error al crear usuario demo:', createError);
          /* istanbul ignore next */
          cov_1uhfcmk0k().s[38]++;
          return _server.NextResponse.json({
            error: 'Error al crear perfil de usuario'
          }, {
            status: 500
          });
