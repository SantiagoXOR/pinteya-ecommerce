      /* istanbul ignore next */
      cov_1uhfcmk0k().s[65]++;
      return _server.NextResponse.json({
        error: 'Error al actualizar perfil de usuario'
      }, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_1uhfcmk0k().b[14][1]++;
