    cov_2o1nnc9fr().f[2]++;
    cov_2o1nnc9fr().s[10]++;
    try {
      /* istanbul ignore next */
      cov_2o1nnc9fr().s[11]++;
      setLoading(true);
      /* istanbul ignore next */
      cov_2o1nnc9fr().s[12]++;
      setError(null);
      const params =
      /* istanbul ignore next */
      (cov_2o1nnc9fr().s[13]++, new URLSearchParams({
        page: page.toString(),
        limit: '10'
      }));
      /* istanbul ignore next */
      cov_2o1nnc9fr().s[14]++;
      if (
      /* istanbul ignore next */
      (cov_2o1nnc9fr().b[5][0]++, status) &&
      /* istanbul ignore next */
      (cov_2o1nnc9fr().b[5][1]++, status !== 'all')) {
        /* istanbul ignore next */
        cov_2o1nnc9fr().b[4][0]++;
        cov_2o1nnc9fr().s[15]++;
        params.append('status', status);
      } else
      /* istanbul ignore next */
      {
        cov_2o1nnc9fr().b[4][1]++;
      }
      const response =
      /* istanbul ignore next */
      (cov_2o1nnc9fr().s[16]++, await fetch(`/api/user/orders?${params}`));
      const data =
      /* istanbul ignore next */
      (cov_2o1nnc9fr().s[17]++, await response.json());
      /* istanbul ignore next */
      cov_2o1nnc9fr().s[18]++;
      if (!response.ok) {
        /* istanbul ignore next */
        cov_2o1nnc9fr().b[6][0]++;
        cov_2o1nnc9fr().s[19]++;
        throw new Error(
        /* istanbul ignore next */
        (cov_2o1nnc9fr().b[7][0]++, data.error) ||
        /* istanbul ignore next */
        (cov_2o1nnc9fr().b[7][1]++, 'Error al obtener órdenes'));
      } else
      /* istanbul ignore next */
      {
        cov_2o1nnc9fr().b[6][1]++;
      }
      cov_2o1nnc9fr().s[20]++;
      if (data.success) {
        /* istanbul ignore next */
        cov_2o1nnc9fr().b[8][0]++;
        cov_2o1nnc9fr().s[21]++;
        setOrders(data.orders);
        /* istanbul ignore next */
        cov_2o1nnc9fr().s[22]++;
        setPagination(data.pagination);
        /* istanbul ignore next */
        cov_2o1nnc9fr().s[23]++;
        setStatistics(data.statistics);
      } else {
        /* istanbul ignore next */
        cov_2o1nnc9fr().b[8][1]++;
        cov_2o1nnc9fr().s[24]++;
        throw new Error('Error al obtener órdenes');
      }
    } catch (err) {
      /* istanbul ignore next */
      cov_2o1nnc9fr().s[25]++;
      console.error('Error en useUserOrders:', err);
      /* istanbul ignore next */
      cov_2o1nnc9fr().s[26]++;
      setError(err instanceof Error ?
      /* istanbul ignore next */
      (cov_2o1nnc9fr().b[9][0]++, err.message) :
      /* istanbul ignore next */
      (cov_2o1nnc9fr().b[9][1]++, 'Error desconocido'));
    } finally {
      /* istanbul ignore next */
      cov_2o1nnc9fr().s[27]++;
      setLoading(false);
    }
  };
  // Función para refrescar las órdenes
  /* istanbul ignore next */
  cov_2o1nnc9fr().s[28]++;
  const refreshOrders = () => {
    /* istanbul ignore next */
    cov_2o1nnc9fr().f[3]++;
    cov_2o1nnc9fr().s[29]++;
    fetchOrders(pagination.page, initialStatus);
  };
  // Cargar órdenes al montar el componente
  /* istanbul ignore next */
  cov_2o1nnc9fr().s[30]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_2o1nnc9fr().f[4]++;
    cov_2o1nnc9fr().s[31]++;
    fetchOrders(initialPage, initialStatus);
  }, [initialPage, initialStatus]);
  /* istanbul ignore next */
  cov_2o1nnc9fr().s[32]++;
  return {
    orders,
    loading,
    error,
    pagination,
    statistics,
    fetchOrders,
    refreshOrders
  };
}
