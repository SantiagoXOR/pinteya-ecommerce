    });
  }
  /**
  * Agrega presupuesto personalizado
  */
  addBudget(budget) {
    /* istanbul ignore next */
    cov_lqmgx0sax().f[42]++;
    const id =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[188]++, `custom_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`);
    const now =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[189]++, Date.now());
    const fullBudget =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[190]++, {
      ...budget,
      id,
      createdAt: now,
      updatedAt: now
    });
    /* istanbul ignore next */
    cov_lqmgx0sax().s[191]++;
    this.budgets.set(id, fullBudget);
    /* istanbul ignore next */
    cov_lqmgx0sax().s[192]++;
    _logger.logger.info(_logger.LogCategory.MONITORING, `Custom performance budget added: ${budget.name}`, {
      id
    });
    /* istanbul ignore next */
    cov_lqmgx0sax().s[193]++;
    return id;
  }
  /**
  * Actualiza presupuesto
  */
  updateBudget(id, updates) {
    /* istanbul ignore next */
    cov_lqmgx0sax().f[43]++;
    const budget =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[194]++, this.budgets.get(id));
    /* istanbul ignore next */
    cov_lqmgx0sax().s[195]++;
    if (!budget) {
      /* istanbul ignore next */
      cov_lqmgx0sax().b[36][0]++;
      cov_lqmgx0sax().s[196]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_lqmgx0sax().b[36][1]++;
    }
    const updatedBudget =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[197]++, {
      ...budget,
      ...updates,
      id,
      updatedAt: Date.now()
    });
    /* istanbul ignore next */
    cov_lqmgx0sax().s[198]++;
    this.budgets.set(id, updatedBudget);
    /* istanbul ignore next */
    cov_lqmgx0sax().s[199]++;
    _logger.logger.info(_logger.LogCategory.MONITORING, `Performance budget updated: ${id}`);
    /* istanbul ignore next */
    cov_lqmgx0sax().s[200]++;
    return true;
  }
  /**
  * Elimina presupuesto
  */
  removeBudget(id) {
    /* istanbul ignore next */
    cov_lqmgx0sax().f[44]++;
    const deleted =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[201]++, this.budgets.delete(id));
    /* istanbul ignore next */
    cov_lqmgx0sax().s[202]++;
    if (deleted) {
      /* istanbul ignore next */
      cov_lqmgx0sax().b[37][0]++;
      cov_lqmgx0sax().s[203]++;
      this.measurements.delete(id);
      /* istanbul ignore next */
      cov_lqmgx0sax().s[204]++;
      _logger.logger.info(_logger.LogCategory.MONITORING, `Performance budget removed: ${id}`);
    } else
    /* istanbul ignore next */
    {
      cov_lqmgx0sax().b[37][1]++;
    }
    cov_lqmgx0sax().s[205]++;
    return deleted;
  }
  /**
  * Persiste medición en Redis
  */
  async persistMeasurement(measurement) {
    /* istanbul ignore next */
    cov_lqmgx0sax().f[45]++;
    cov_lqmgx0sax().s[206]++;
    try {
      /* istanbul ignore next */
      cov_lqmgx0sax().s[207]++;
      await this.redis.setex(`measurement:${measurement.id}`, 86400 * 7, JSON.stringify(measurement));
    } catch (error) {
      /* istanbul ignore next */
      cov_lqmgx0sax().s[208]++;
      _logger.logger.error(_logger.LogCategory.MONITORING, 'Error persisting measurement', error);
    }
  }
  /**
  * Destructor
  */
  destroy() {
    /* istanbul ignore next */
    cov_lqmgx0sax().f[46]++;
    cov_lqmgx0sax().s[209]++;
    if (this.evaluationInterval) {
      /* istanbul ignore next */
      cov_lqmgx0sax().b[38][0]++;
      cov_lqmgx0sax().s[210]++;
      clearInterval(this.evaluationInterval);
    } else
    /* istanbul ignore next */
    {
      cov_lqmgx0sax().b[38][1]++;
    }
  }
}
