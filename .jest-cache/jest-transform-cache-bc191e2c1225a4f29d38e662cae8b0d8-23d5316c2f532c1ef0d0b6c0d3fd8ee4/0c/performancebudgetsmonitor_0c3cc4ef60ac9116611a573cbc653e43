    /* istanbul ignore next */
    (cov_lqmgx0sax().b[35][1]++, []));
    const cutoff =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[185]++, Date.now() - hours * 60 * 60 * 1000);
    /* istanbul ignore next */
    cov_lqmgx0sax().s[186]++;
    return measurements.filter(m => {
      /* istanbul ignore next */
