    } else
    /* istanbul ignore next */
    {
      cov_lqmgx0sax().b[6][1]++;
    }
    const evaluation =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[60]++, this.createEvaluation(budget, measurement));
    // Generar alerta si es necesario
    /* istanbul ignore next */
    cov_lqmgx0sax().s[61]++;
    if (
    /* istanbul ignore next */
    (cov_lqmgx0sax().b[8][0]++, evaluation.status === 'warning') ||
    /* istanbul ignore next */
    (cov_lqmgx0sax().b[8][1]++, evaluation.status === 'critical')) {
      /* istanbul ignore next */
