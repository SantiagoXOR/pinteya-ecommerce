      case "responseTime":
        /* istanbul ignore next */
        cov_lqmgx0sax().b[13][4]++;
        cov_lqmgx0sax().s[94]++;
        recommendations.push('Optimizar consultas de base de datos', 'Implementar cache en múltiples niveles', 'Usar connection pooling', 'Optimizar algoritmos de procesamiento');
