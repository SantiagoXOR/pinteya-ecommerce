  /**
  * Obtiene mediciones recientes
  */
  getRecentMeasurements(budgetId, hours =
  /* istanbul ignore next */
  (cov_lqmgx0sax().b[34][0]++, 1)) {
    /* istanbul ignore next */
    cov_lqmgx0sax().f[40]++;
    const measurements =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[184]++,
    /* istanbul ignore next */
