  /**
  * Evalúa un presupuesto específico
  */
  async evaluateBudget(budgetId, measurement) {
    /* istanbul ignore next */
    cov_lqmgx0sax().f[14]++;
    const budget =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[57]++, this.budgets.get(budgetId));
    /* istanbul ignore next */
    cov_lqmgx0sax().s[58]++;
