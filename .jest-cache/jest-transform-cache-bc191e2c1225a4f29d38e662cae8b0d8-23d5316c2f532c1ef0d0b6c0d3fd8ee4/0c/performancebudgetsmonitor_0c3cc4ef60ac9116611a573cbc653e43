  /**
  * Registra Core Web Vitals
  */
  recordWebVitals(vitals, context =
  /* istanbul ignore next */
  (cov_lqmgx0sax().b[39][0]++, {})) {
    /* istanbul ignore next */
    cov_lqmgx0sax().f[47]++;
    cov_lqmgx0sax().s[213]++;
