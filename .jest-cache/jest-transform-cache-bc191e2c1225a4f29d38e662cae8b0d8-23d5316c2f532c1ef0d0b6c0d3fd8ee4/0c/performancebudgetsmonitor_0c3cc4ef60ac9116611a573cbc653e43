  /**
  * Crea evaluación de presupuesto
  */
  createEvaluation(budget, measurement) {
    /* istanbul ignore next */
    cov_lqmgx0sax().f[15]++;
    let status;
    let threshold;
    let message;
    /* istanbul ignore next */
    cov_lqmgx0sax().s[64]++;
    if (measurement.value <= budget.target) {
      /* istanbul ignore next */
      cov_lqmgx0sax().b[9][0]++;
      cov_lqmgx0sax().s[65]++;
      status = 'good';
      /* istanbul ignore next */
      cov_lqmgx0sax().s[66]++;
      threshold = budget.target;
      /* istanbul ignore next */
      cov_lqmgx0sax().s[67]++;
      message = `${budget.name} dentro del objetivo: ${measurement.value}${budget.unit}`;
    } else {
      /* istanbul ignore next */
      cov_lqmgx0sax().b[9][1]++;
      cov_lqmgx0sax().s[68]++;
      if (measurement.value <= budget.warning) {
        /* istanbul ignore next */
        cov_lqmgx0sax().b[10][0]++;
        cov_lqmgx0sax().s[69]++;
        status = 'warning';
        /* istanbul ignore next */
        cov_lqmgx0sax().s[70]++;
        threshold = budget.warning;
        /* istanbul ignore next */
        cov_lqmgx0sax().s[71]++;
        message = `${budget.name} excede objetivo pero dentro de advertencia: ${measurement.value}${budget.unit}`;
      } else {
        /* istanbul ignore next */
        cov_lqmgx0sax().b[10][1]++;
        cov_lqmgx0sax().s[72]++;
        if (measurement.value <= budget.critical) {
          /* istanbul ignore next */
          cov_lqmgx0sax().b[11][0]++;
          cov_lqmgx0sax().s[73]++;
          status = 'warning';
          /* istanbul ignore next */
          cov_lqmgx0sax().s[74]++;
          threshold = budget.critical;
          /* istanbul ignore next */
          cov_lqmgx0sax().s[75]++;
          message = `${budget.name} en nivel de advertencia: ${measurement.value}${budget.unit}`;
        } else {
          /* istanbul ignore next */
          cov_lqmgx0sax().b[11][1]++;
          cov_lqmgx0sax().s[76]++;
          status = 'critical';
          /* istanbul ignore next */
          cov_lqmgx0sax().s[77]++;
          threshold = budget.critical;
          /* istanbul ignore next */
          cov_lqmgx0sax().s[78]++;
          message = `${budget.name} en nivel crítico: ${measurement.value}${budget.unit}`;
        }
      }
    }
    const deviation =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[79]++, (measurement.value - budget.target) / budget.target * 100);
    const recommendations =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[80]++, this.generateRecommendations(budget, measurement, status));
    /* istanbul ignore next */
    cov_lqmgx0sax().s[81]++;
    return {
      budgetId: budget.id,
      measurement,
      status,
      deviation,
      threshold,
      message,
      recommendations
    };
  }
  /**
  * Genera recomendaciones basadas en la evaluación
  */
  generateRecommendations(budget, measurement, status) {
    /* istanbul ignore next */
    cov_lqmgx0sax().f[16]++;
    cov_lqmgx0sax().s[82]++;
    if (status === 'good') {
      /* istanbul ignore next */
      cov_lqmgx0sax().b[12][0]++;
      cov_lqmgx0sax().s[83]++;
      return [];
    } else
    /* istanbul ignore next */
    {
      cov_lqmgx0sax().b[12][1]++;
    }
    const recommendations =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[84]++, []);
    /* istanbul ignore next */
    cov_lqmgx0sax().s[85]++;
    switch (budget.metricType) {
      case "lcp":
        /* istanbul ignore next */
        cov_lqmgx0sax().b[13][0]++;
        cov_lqmgx0sax().s[86]++;
        recommendations.push('Optimizar imágenes y usar formatos modernos (WebP, AVIF)', 'Implementar lazy loading para imágenes', 'Reducir el tamaño del bundle JavaScript crítico', 'Usar CDN para recursos estáticos');
        /* istanbul ignore next */
        cov_lqmgx0sax().s[87]++;
        break;
      case "fid":
        /* istanbul ignore next */
        cov_lqmgx0sax().b[13][1]++;
        cov_lqmgx0sax().s[88]++;
        recommendations.push('Reducir el tiempo de ejecución de JavaScript', 'Dividir tareas largas en chunks más pequeños', 'Usar Web Workers para procesamiento pesado', 'Optimizar event listeners');
        /* istanbul ignore next */
        cov_lqmgx0sax().s[89]++;
        break;
      case "cls":
        /* istanbul ignore next */
        cov_lqmgx0sax().b[13][2]++;
        cov_lqmgx0sax().s[90]++;
        recommendations.push('Especificar dimensiones para imágenes y videos', 'Reservar espacio para contenido dinámico', 'Evitar insertar contenido sobre contenido existente', 'Usar transform en lugar de cambiar propiedades de layout');
        /* istanbul ignore next */
