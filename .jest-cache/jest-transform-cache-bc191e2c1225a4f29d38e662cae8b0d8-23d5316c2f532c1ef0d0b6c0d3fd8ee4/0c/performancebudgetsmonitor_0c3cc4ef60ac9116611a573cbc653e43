  }
  /**
  * Crea alerta de presupuesto
  */
  async createBudgetAlert(evaluation) {
    /* istanbul ignore next */
    cov_lqmgx0sax().f[17]++;
    const budget =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[99]++, this.budgets.get(evaluation.budgetId));
    /* istanbul ignore next */
    cov_lqmgx0sax().s[100]++;
    if (!budget) {
      /* istanbul ignore next */
      cov_lqmgx0sax().b[14][0]++;
      cov_lqmgx0sax().s[101]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_lqmgx0sax().b[14][1]++;
    }
    const severity =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[102]++, evaluation.status === 'critical' ?
    /* istanbul ignore next */
    (cov_lqmgx0sax().b[15][0]++, _advancedalertingengine.AlertSeverity.HIGH) :
    /* istanbul ignore next */
    (cov_lqmgx0sax().b[15][1]++, _advancedalertingengine.AlertSeverity.MEDIUM));
    const title =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[103]++, `Presupuesto de Performance Excedido: ${budget.name}`);
    const message =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[104]++, evaluation.message);
    const details =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[105]++, {
      budgetId: budget.id,
      metricType: budget.metricType,
      value: evaluation.measurement.value,
      target: budget.target,
      threshold: evaluation.threshold,
      deviation: evaluation.deviation,
      unit: budget.unit,
      context: evaluation.measurement.context,
      recommendations: evaluation.recommendations
    });
    /* istanbul ignore next */
    cov_lqmgx0sax().s[106]++;
    await _advancedalertingengine.advancedAlertingEngine.createAlert(_advancedalertingengine.AlertType.PERFORMANCE, severity, title, message, details, 'performance-budgets', ['performance', 'budget', budget.metricType, ...budget.tags]);
  }
  /**
  * Evalúa todos los presupuestos
  */
  async evaluateAllBudgets() {
    /* istanbul ignore next */
    cov_lqmgx0sax().f[18]++;
    const now =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[107]++, Date.now());
    const fiveMinutesAgo =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[108]++, now - 5 * 60 * 1000);
    /* istanbul ignore next */
    cov_lqmgx0sax().s[109]++;
    for (const [budgetId, budget] of this.budgets) {
      /* istanbul ignore next */
      cov_lqmgx0sax().s[110]++;
      if (!budget.enabled) {
        /* istanbul ignore next */
        cov_lqmgx0sax().b[16][0]++;
        cov_lqmgx0sax().s[111]++;
        continue;
      } else
      /* istanbul ignore next */
      {
        cov_lqmgx0sax().b[16][1]++;
      }
      const measurements =
      /* istanbul ignore next */
      (cov_lqmgx0sax().s[112]++,
      /* istanbul ignore next */
      (cov_lqmgx0sax().b[17][0]++, this.measurements.get(budgetId)) ||
      /* istanbul ignore next */
      (cov_lqmgx0sax().b[17][1]++, []));
      const recentMeasurements =
      /* istanbul ignore next */
      (cov_lqmgx0sax().s[113]++, measurements.filter(m => {
        /* istanbul ignore next */
        cov_lqmgx0sax().f[19]++;
        cov_lqmgx0sax().s[114]++;
        return m.timestamp > fiveMinutesAgo;
      }));
      /* istanbul ignore next */
      cov_lqmgx0sax().s[115]++;
      if (recentMeasurements.length === 0) {
        /* istanbul ignore next */
        cov_lqmgx0sax().b[18][0]++;
        cov_lqmgx0sax().s[116]++;
        continue;
      } else
      /* istanbul ignore next */
      {
        cov_lqmgx0sax().b[18][1]++;
      }
      // Evaluar promedio de mediciones recientes
      const averageValue =
      /* istanbul ignore next */
      (cov_lqmgx0sax().s[117]++, recentMeasurements.reduce((sum, m) => {
        /* istanbul ignore next */
        cov_lqmgx0sax().f[20]++;
        cov_lqmgx0sax().s[118]++;
        return sum + m.value;
      }, 0) / recentMeasurements.length);
      const syntheticMeasurement =
      /* istanbul ignore next */
      (cov_lqmgx0sax().s[119]++, {
        id: `synthetic_${budgetId}_${now}`,
        budgetId,
        value: averageValue,
        timestamp: now,
        context: {
          synthetic: true
        }
      });
      /* istanbul ignore next */
      cov_lqmgx0sax().s[120]++;
      await this.evaluateBudget(budgetId, syntheticMeasurement);
    }
  }
  /**
  * Genera reporte de presupuestos
  */
  generateReport(periodHours =
  /* istanbul ignore next */
  (cov_lqmgx0sax().b[19][0]++, 24)) {
    /* istanbul ignore next */
    cov_lqmgx0sax().f[21]++;
    const now =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[121]++, Date.now());
    const periodStart =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[122]++, now - periodHours * 60 * 60 * 1000);
    const budgetResults =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[123]++, Array.from(this.budgets.values()).filter(budget => {
      /* istanbul ignore next */
      cov_lqmgx0sax().f[22]++;
      cov_lqmgx0sax().s[124]++;
      return budget.enabled;
    }).map(budget => {
      /* istanbul ignore next */
      cov_lqmgx0sax().f[23]++;
      const measurements =
      /* istanbul ignore next */
      (cov_lqmgx0sax().s[125]++, (
      /* istanbul ignore next */
      (cov_lqmgx0sax().b[20][0]++, this.measurements.get(budget.id)) ||
      /* istanbul ignore next */
      (cov_lqmgx0sax().b[20][1]++, [])).filter(m => {
        /* istanbul ignore next */
        cov_lqmgx0sax().f[24]++;
        cov_lqmgx0sax().s[126]++;
        return m.timestamp >= periodStart;
      }));
      /* istanbul ignore next */
      cov_lqmgx0sax().s[127]++;
      if (measurements.length === 0) {
        /* istanbul ignore next */
        cov_lqmgx0sax().b[21][0]++;
        cov_lqmgx0sax().s[128]++;
        return {
          budget,
          measurements: [],
          averageValue: 0,
          status: 'good',
          trend: 'stable',
          violations: 0
        };
      } else
      /* istanbul ignore next */
      {
        cov_lqmgx0sax().b[21][1]++;
      }
      const averageValue =
      /* istanbul ignore next */
      (cov_lqmgx0sax().s[129]++, measurements.reduce((sum, m) => {
        /* istanbul ignore next */
        cov_lqmgx0sax().f[25]++;
        cov_lqmgx0sax().s[130]++;
        return sum + m.value;
      }, 0) / measurements.length);
      let status;
      /* istanbul ignore next */
      cov_lqmgx0sax().s[131]++;
      if (averageValue <= budget.target) {
        /* istanbul ignore next */
        cov_lqmgx0sax().b[22][0]++;
        cov_lqmgx0sax().s[132]++;
        status = 'good';
      } else {
        /* istanbul ignore next */
        cov_lqmgx0sax().b[22][1]++;
        cov_lqmgx0sax().s[133]++;
        if (averageValue <= budget.warning) {
          /* istanbul ignore next */
          cov_lqmgx0sax().b[23][0]++;
          cov_lqmgx0sax().s[134]++;
          status = 'warning';
        } else {
          /* istanbul ignore next */
          cov_lqmgx0sax().b[23][1]++;
          cov_lqmgx0sax().s[135]++;
          status = 'critical';
        }
      }
      const violations =
      /* istanbul ignore next */
      (cov_lqmgx0sax().s[136]++, measurements.filter(m => {
        /* istanbul ignore next */
        cov_lqmgx0sax().f[26]++;
        cov_lqmgx0sax().s[137]++;
        return m.value > budget.critical;
      }).length);
      // Calcular tendencia
      const halfPeriod =
      /* istanbul ignore next */
      (cov_lqmgx0sax().s[138]++, measurements.length / 2);
      const firstHalf =
      /* istanbul ignore next */
      (cov_lqmgx0sax().s[139]++, measurements.slice(0, halfPeriod));
      const secondHalf =
      /* istanbul ignore next */
      (cov_lqmgx0sax().s[140]++, measurements.slice(halfPeriod));
      const firstAvg =
      /* istanbul ignore next */
      (cov_lqmgx0sax().s[141]++, firstHalf.reduce((sum, m) => {
        /* istanbul ignore next */
        cov_lqmgx0sax().f[27]++;
        cov_lqmgx0sax().s[142]++;
        return sum + m.value;
      }, 0) / firstHalf.length);
      const secondAvg =
      /* istanbul ignore next */
      (cov_lqmgx0sax().s[143]++, secondHalf.reduce((sum, m) => {
        /* istanbul ignore next */
        cov_lqmgx0sax().f[28]++;
        cov_lqmgx0sax().s[144]++;
        return sum + m.value;
      }, 0) / secondHalf.length);
      let trend;
      const change =
      /* istanbul ignore next */
      (cov_lqmgx0sax().s[145]++, (secondAvg - firstAvg) / firstAvg * 100);
      /* istanbul ignore next */
      cov_lqmgx0sax().s[146]++;
      if (change < -5) {
        /* istanbul ignore next */
        cov_lqmgx0sax().b[24][0]++;
        cov_lqmgx0sax().s[147]++;
        trend = 'improving';
      } else {
        /* istanbul ignore next */
        cov_lqmgx0sax().b[24][1]++;
        cov_lqmgx0sax().s[148]++;
        if (change > 5) {
          /* istanbul ignore next */
          cov_lqmgx0sax().b[25][0]++;
          cov_lqmgx0sax().s[149]++;
          trend = 'degrading';
        } else {
          /* istanbul ignore next */
          cov_lqmgx0sax().b[25][1]++;
          cov_lqmgx0sax().s[150]++;
          trend = 'stable';
        }
      }
      /* istanbul ignore next */
      cov_lqmgx0sax().s[151]++;
      return {
        budget,
        measurements,
        averageValue,
        status,
        trend,
        violations
      };
    }));
    const totalBudgets =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[152]++, budgetResults.length);
    const passingBudgets =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[153]++, budgetResults.filter(r => {
      /* istanbul ignore next */
      cov_lqmgx0sax().f[29]++;
      cov_lqmgx0sax().s[154]++;
      return r.status === 'good';
    }).length);
    const warningBudgets =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[155]++, budgetResults.filter(r => {
      /* istanbul ignore next */
      cov_lqmgx0sax().f[30]++;
      cov_lqmgx0sax().s[156]++;
      return r.status === 'warning';
    }).length);
    const failingBudgets =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[157]++, budgetResults.filter(r => {
      /* istanbul ignore next */
      cov_lqmgx0sax().f[31]++;
      cov_lqmgx0sax().s[158]++;
      return r.status === 'critical';
    }).length);
    const overallScore =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[159]++, totalBudgets > 0 ?
    /* istanbul ignore next */
    (cov_lqmgx0sax().b[26][0]++, Math.round(passingBudgets / totalBudgets * 100)) :
    /* istanbul ignore next */
    (cov_lqmgx0sax().b[26][1]++, 100));
    const recommendations =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[160]++, this.generateReportRecommendations(budgetResults));
    /* istanbul ignore next */
    cov_lqmgx0sax().s[161]++;
    return {
      period: {
        start: periodStart,
        end: now
      },
      summary: {
        totalBudgets,
        passingBudgets,
        warningBudgets,
        failingBudgets,
        overallScore
      },
      budgetResults,
      recommendations
    };
  }
  /**
  * Genera recomendaciones para el reporte
  */
  generateReportRecommendations(budgetResults) {
    /* istanbul ignore next */
    cov_lqmgx0sax().f[32]++;
    const recommendations =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[162]++, []);
    const failingBudgets =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[163]++, budgetResults.filter(r => {
      /* istanbul ignore next */
      cov_lqmgx0sax().f[33]++;
      cov_lqmgx0sax().s[164]++;
      return r.status === 'critical';
    }));
    const degradingBudgets =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[165]++, budgetResults.filter(r => {
      /* istanbul ignore next */
      cov_lqmgx0sax().f[34]++;
      cov_lqmgx0sax().s[166]++;
      return r.trend === 'degrading';
    }));
    /* istanbul ignore next */
    cov_lqmgx0sax().s[167]++;
    if (failingBudgets.length > 0) {
      /* istanbul ignore next */
      cov_lqmgx0sax().b[27][0]++;
      cov_lqmgx0sax().s[168]++;
      recommendations.push(`${failingBudgets.length} presupuestos están en estado crítico y requieren atención inmediata`);
    } else
    /* istanbul ignore next */
    {
      cov_lqmgx0sax().b[27][1]++;
    }
    cov_lqmgx0sax().s[169]++;
    if (degradingBudgets.length > 0) {
      /* istanbul ignore next */
      cov_lqmgx0sax().b[28][0]++;
      cov_lqmgx0sax().s[170]++;
      recommendations.push(`${degradingBudgets.length} presupuestos muestran tendencia de degradación`);
    } else
    /* istanbul ignore next */
    {
      cov_lqmgx0sax().b[28][1]++;
    }
    const highViolationBudgets =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[171]++, budgetResults.filter(r => {
      /* istanbul ignore next */
      cov_lqmgx0sax().f[35]++;
      cov_lqmgx0sax().s[172]++;
      return r.violations > 10;
    }));
    /* istanbul ignore next */
    cov_lqmgx0sax().s[173]++;
    if (highViolationBudgets.length > 0) {
      /* istanbul ignore next */
      cov_lqmgx0sax().b[29][0]++;
      cov_lqmgx0sax().s[174]++;
      recommendations.push('Revisar presupuestos con alta frecuencia de violaciones');
    } else
    /* istanbul ignore next */
    {
      cov_lqmgx0sax().b[29][1]++;
    }
    cov_lqmgx0sax().s[175]++;
    if (budgetResults.some(r => {
      /* istanbul ignore next */
      cov_lqmgx0sax().f[36]++;
      cov_lqmgx0sax().s[176]++;
      return /* istanbul ignore next */(cov_lqmgx0sax().b[31][0]++, r.budget.metricType === "bundleSize") &&
      /* istanbul ignore next */
      (cov_lqmgx0sax().b[31][1]++, r.status !== 'good');
    })) {
      /* istanbul ignore next */
      cov_lqmgx0sax().b[30][0]++;
      cov_lqmgx0sax().s[177]++;
      recommendations.push('Implementar estrategias de optimización de bundle size');
    } else
    /* istanbul ignore next */
    {
      cov_lqmgx0sax().b[30][1]++;
    }
    cov_lqmgx0sax().s[178]++;
    if (budgetResults.some(r => {
      /* istanbul ignore next */
      cov_lqmgx0sax().f[37]++;
      cov_lqmgx0sax().s[179]++;
      return /* istanbul ignore next */(cov_lqmgx0sax().b[33][0]++, r.budget.metricType === "lcp") &&
      /* istanbul ignore next */
      (cov_lqmgx0sax().b[33][1]++, r.status !== 'good');
    })) {
      /* istanbul ignore next */
      cov_lqmgx0sax().b[32][0]++;
      cov_lqmgx0sax().s[180]++;
      recommendations.push('Optimizar Core Web Vitals para mejorar experiencia de usuario');
    } else
    /* istanbul ignore next */
    {
      cov_lqmgx0sax().b[32][1]++;
    }
    cov_lqmgx0sax().s[181]++;
    return recommendations;
  }
  /**
