  /**
  * Registra una medición de performance
  */
  recordMeasurement(budgetId, value, context =
  /* istanbul ignore next */
  (cov_lqmgx0sax().b[1][0]++, {}), metadata) {
    /* istanbul ignore next */
    cov_lqmgx0sax().f[13]++;
    const budget =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[41]++, this.budgets.get(budgetId));
    /* istanbul ignore next */
    cov_lqmgx0sax().s[42]++;
    if (
    /* istanbul ignore next */
    (cov_lqmgx0sax().b[3][0]++, !budget) ||
    /* istanbul ignore next */
    (cov_lqmgx0sax().b[3][1]++, !budget.enabled)) {
      /* istanbul ignore next */
      cov_lqmgx0sax().b[2][0]++;
      cov_lqmgx0sax().s[43]++;
      _logger.logger.warn(_logger.LogCategory.MONITORING, `Budget not found or disabled: ${budgetId}`);
      /* istanbul ignore next */
      cov_lqmgx0sax().s[44]++;
      return '';
    } else
    /* istanbul ignore next */
    {
      cov_lqmgx0sax().b[2][1]++;
    }
    const measurementId =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[45]++, `measurement_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`);
    const measurement =
    /* istanbul ignore next */
    (cov_lqmgx0sax().s[46]++, {
      id: measurementId,
      budgetId,
      value,
      timestamp: Date.now(),
      context,
      metadata
    });
    // Almacenar medición
    /* istanbul ignore next */
    cov_lqmgx0sax().s[47]++;
    if (!this.measurements.has(budgetId)) {
      /* istanbul ignore next */
      cov_lqmgx0sax().b[4][0]++;
      cov_lqmgx0sax().s[48]++;
