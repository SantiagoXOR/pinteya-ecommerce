"use strict";
Object.defineProperty(exports, "__esModule", {
    value: true
});
const _validation = require("../../lib/utils/validation");
describe('Validation Utils', ()=>{
    describe('validateEmail', ()=>{
        it('should validate correct email formats', ()=>{
            expect((0, _validation.validateEmail)('test@example.com')).toBe(true);
            expect((0, _validation.validateEmail)('user.name@domain.co.ar')).toBe(true);
            expect((0, _validation.validateEmail)('juan.perez@test.com')).toBe(true);
        });
        it('should reject invalid email formats', ()=>{
            expect((0, _validation.validateEmail)('invalid-email')).toBe(false);
            expect((0, _validation.validateEmail)('test@')).toBe(false);
            expect((0, _validation.validateEmail)('@domain.com')).toBe(false);
        });
    });
    describe('validatePhoneNumber', ()=>{
        it('should validate Argentine phone formats', ()=>{
            // Formato que causó el error en el reporte
            expect((0, _validation.validatePhoneNumber)('+54 351 123-4567')).toBe(true);
            // Otros formatos válidos
            expect((0, _validation.validatePhoneNumber)('+54 9 351 123 4567')).toBe(true);
            expect((0, _validation.validatePhoneNumber)('+54 351 123 4567')).toBe(true);
            expect((0, _validation.validatePhoneNumber)('351 123 4567')).toBe(true);
            expect((0, _validation.validatePhoneNumber)('3511234567')).toBe(true);
            expect((0, _validation.validatePhoneNumber)('+543511234567')).toBe(true);
            expect((0, _validation.validatePhoneNumber)('+5493511234567')).toBe(true);
        });
        it('should reject invalid phone formats', ()=>{
            expect((0, _validation.validatePhoneNumber)('123')).toBe(false);
            expect((0, _validation.validatePhoneNumber)('abc123456789')).toBe(false);
            expect((0, _validation.validatePhoneNumber)('')).toBe(false);
            expect((0, _validation.validatePhoneNumber)('12345')).toBe(false);
        });
        it('should handle phone numbers with various separators', ()=>{
            expect((0, _validation.validatePhoneNumber)('+54-351-123-4567')).toBe(true);
            expect((0, _validation.validatePhoneNumber)('+54 (351) 123-4567')).toBe(true);
            expect((0, _validation.validatePhoneNumber)('351-123-4567')).toBe(true);
        });
    });
});

