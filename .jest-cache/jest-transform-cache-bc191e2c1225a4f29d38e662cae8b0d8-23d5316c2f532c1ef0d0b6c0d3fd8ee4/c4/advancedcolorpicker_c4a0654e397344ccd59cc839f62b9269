                            variant: "secondary",
                            className: "text-xs",
                            children: "Populares"
                        })
                    }),
                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                        className: "grid grid-cols-6 sm:grid-cols-8 md:grid-cols-10 lg:grid-cols-12 gap-2",
                        children: colors.filter((color)=>{
                            // Para productos lÃ¡tex, excluir colores de Madera y SintÃ©tico de los populares
                            if (productType?.allowedColorCategories?.includes('LÃ¡tex') && productType.allowedColorCategories.length === 1) {
                                return color.isPopular && color.category !== 'Madera' && color.category !== 'SintÃ©tico';
                            }
                            return color.isPopular;
                        }).map((color)=>/*#__PURE__*/ (0, _jsxruntime.jsx)(ColorSwatch, {
                                color: color,
                                isSelected: selectedColor === color.id,
                                onClick: ()=>onColorChange(color.id)
                            }, `${color.id}-${color.category.toLowerCase()}`))
                    })
                ]
            }),
            selectedCategory !== 'all' && /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                className: "space-y-2",
                children: /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                    className: "grid grid-cols-6 sm:grid-cols-8 md:grid-cols-10 lg:grid-cols-12 gap-2",
                    children: filteredColors.map((color)=>/*#__PURE__*/ (0, _jsxruntime.jsx)(ColorSwatch, {
                            color: color,
                            isSelected: selectedColor === color.id,
                            onClick: ()=>onColorChange(color.id)
                        }, `${color.id}-${color.category.toLowerCase()}`))
                })
            }),
            currentColor && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                className: "bg-gray-50 p-4 rounded-lg space-y-2",
                children: [
                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "flex items-center gap-3",
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                className: "w-12 h-12 rounded-lg border-2 border-gray-300 shadow-sm",
                                style: {
                                    backgroundColor: currentColor.hex
                                }
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                className: "flex-1",
                                children: [
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("h5", {
                                        className: "font-medium text-gray-900",
                                        children: currentColor.displayName
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("p", {
                                        className: "text-sm text-gray-600",
                                        children: [
                                            currentColor.family,
                                            " â€¢ ",
                                            currentColor.category
                                        ]
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                        className: "text-xs text-gray-500 font-mono",
                                        children: currentColor.hex
                                    })
                                ]
                            })
                        ]
                    }),
                    currentColor.description && /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                        className: "text-sm text-gray-600 italic",
                        children: currentColor.description
                    })
                ]
            })
        ]
    });
};
const ColorSwatch = ({ color, isSelected, onClick })=>{
    return /*#__PURE__*/ (0, _jsxruntime.jsxs)("button", {
        onClick: onClick,
        className: (0, _utils.cn)('relative w-10 h-10 rounded-lg border-2 transition-all duration-200 hover:scale-110 hover:shadow-md group', isSelected ? 'border-blaze-orange-500 ring-2 ring-blaze-orange-200 shadow-md' : 'border-gray-300 hover:border-gray-400'),
        style: {
            backgroundColor: color.hex,
            boxShadow: color.hex === '#FFFFFF' ? 'inset 0 0 0 1px #E5E7EB' : 'none'
        },
        title: color.displayName,
        "aria-label": `Seleccionar color ${color.displayName}`,
        children: [
            isSelected && /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                className: "absolute inset-0 flex items-center justify-center",
                children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.Check, {
                    className: "w-4 h-4 text-white drop-shadow-md",
                    style: {
                        color: color.hex === '#FFFFFF' || color.hex === '#FFFFF0' ? '#000000' : '#FFFFFF'
                    }
                })
            }),
            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                className: "absolute bottom-full left-1/2 transform -translate-x-1/2 mb-2 px-2 py-1 bg-gray-900 text-white text-xs rounded opacity-0 group-hover:opacity-100 transition-opacity duration-200 pointer-events-none whitespace-nowrap z-10",
                children: color.displayName
            })
        ]
    });
};
const _default = AdvancedColorPicker;

