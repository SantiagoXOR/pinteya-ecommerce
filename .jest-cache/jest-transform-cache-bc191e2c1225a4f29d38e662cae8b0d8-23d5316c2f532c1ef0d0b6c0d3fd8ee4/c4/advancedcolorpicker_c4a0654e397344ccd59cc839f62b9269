const getCategoryDisplayName = (category)=>{
    const categoryNames = {
        all: 'Todos',
        Madera: 'Madera',
        SintÃ©tico: 'SintÃ©tico',
        Neutros: 'Neutros',
        CÃ¡lidos: 'CÃ¡lidos',
        FrÃ­os: 'FrÃ­os',
        Tierras: 'Tierras'
    };
    return categoryNames[category] || category;
};
const AdvancedColorPicker = ({ colors = PAINT_COLORS, selectedColor, onColorChange, showSearch = true, showCategories = true, showPreview = true, maxDisplayColors = 24, className, productType })=>{
    const [searchTerm, setSearchTerm] = (0, _react.useState)('');
    const [selectedCategory, setSelectedCategory] = (0, _react.useState)('all');
    const [showAllColors, setShowAllColors] = (0, _react.useState)(false);
    // Filtrar colores segÃºn el tipo de producto
    const availableColors = (0, _react.useMemo)(()=>{
        console.log('ðŸŽ¨ AdvancedColorPicker - Debug Info:', {
            productType,
            hasProductType: !!productType,
            allowedColorCategories: productType?.allowedColorCategories,
            totalColors: colors.length
        });
        if (!productType || !productType.allowedColorCategories) {
            console.log('âš ï¸ No hay restricciones de color, mostrando todos los colores');
            return colors // Si no hay restricciones, mostrar todos los colores
            ;
        }
        let filtered = colors.filter((color)=>productType.allowedColorCategories.includes(color.category));
        // Para productos de lÃ¡tex, excluir completamente los colores de madera
        const isLatexProduct = productType.allowedColorCategories.includes('LÃ¡tex') && productType.allowedColorCategories.length === 1;
        if (isLatexProduct) {
            filtered = filtered.filter((color)=>color.category !== 'Madera');
            console.log('ðŸŽ¨ Latex product detected - excluding wood colors');
        }
        console.log('âœ… Colores filtrados:', {
            allowedCategories: productType.allowedColorCategories,
            filteredCount: filtered.length,
            originalCount: colors.length,
            filteredColors: filtered.map((c)=>({
                    name: c.displayName,
                    category: c.category
                }))
        });
        return filtered;
    }, [
        colors,
        productType
    ]);
    // Obtener categorÃ­as Ãºnicas basadas en los colores disponibles
    const categories = (0, _react.useMemo)(()=>{
        const cats = Array.from(new Set(availableColors.map((color)=>color.category)));
        // Ordenar categorÃ­as con prioridad especÃ­fica
        const categoryOrder = [
            'Madera',
            'SintÃ©tico',
            'Neutros',
            'CÃ¡lidos',
            'FrÃ­os',
            'Tierras'
        ];
        const sortedCats = cats.sort((a, b)=>{
            const indexA = categoryOrder.indexOf(a);
            const indexB = categoryOrder.indexOf(b);
            if (indexA === -1 && indexB === -1) return a.localeCompare(b);
            if (indexA === -1) return 1;
            if (indexB === -1) return -1;
            return indexA - indexB;
        });
        // Para productos sintÃ©ticos, solo mostrar la categorÃ­a SintÃ©tico (sin "Todos")
        if (productType?.allowedColorCategories?.includes('SintÃ©tico') && productType.allowedColorCategories.length === 1) {
            return [
                'SintÃ©tico'
            ];
        }
        // Para productos de madera, solo mostrar la categorÃ­a Madera (sin "Todos")
        if (productType?.allowedColorCategories?.includes('Madera') && productType.allowedColorCategories.length === 1) {
            return [
                'Madera'
            ];
        }
        return [
            'all',
            ...sortedCats
        ];
    }, [
        availableColors,
        productType
    ]);
    // Inicializar selectedCategory correctamente para productos sintÃ©ticos y de madera
    (0, _react.useEffect)(()=>{
        if (productType?.allowedColorCategories?.includes('SintÃ©tico') && productType.allowedColorCategories.length === 1) {
            setSelectedCategory('SintÃ©tico');
        } else if (productType?.allowedColorCategories?.includes('Madera') && productType.allowedColorCategories.length === 1) {
            setSelectedCategory('Madera');
        }
    }, [
        productType
    ]);
    // Filtrar colores
    const filteredColors = (0, _react.useMemo)(()=>{
        let filtered = availableColors;
        // Filtrar por categorÃ­a
        if (selectedCategory !== 'all') {
            filtered = filtered.filter((color)=>color.category === selectedCategory);
        }
        // Filtrar por bÃºsqueda
        if (searchTerm) {
            filtered = filtered.filter((color)=>color.displayName.toLowerCase().includes(searchTerm.toLowerCase()) || color.name.toLowerCase().includes(searchTerm.toLowerCase()) || color.family.toLowerCase().includes(searchTerm.toLowerCase()));
        }
        return filtered;
    }, [
        availableColors,
        selectedCategory,
        searchTerm
    ]);
    // Colores a mostrar (con lÃ­mite)
    const displayColors = (0, _react.useMemo)(()=>{
        if (showAllColors) return filteredColors;
        return filteredColors.slice(0, maxDisplayColors);
    }, [
        filteredColors,
        showAllColors,
        maxDisplayColors
    ]);
