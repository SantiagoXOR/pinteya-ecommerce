function LogisticsAlertsSummary({
  alerts
}) {
  /* istanbul ignore next */
  cov_ql2jftyom().f[19]++;
  const unreadAlerts =
  /* istanbul ignore next */
  (cov_ql2jftyom().s[48]++, alerts.filter(alert => {
    /* istanbul ignore next */
    cov_ql2jftyom().f[20]++;
    cov_ql2jftyom().s[49]++;
    return !alert.is_read;
  }));
  const criticalCount =
  /* istanbul ignore next */
  (cov_ql2jftyom().s[50]++, unreadAlerts.filter(alert => {
    /* istanbul ignore next */
    cov_ql2jftyom().f[21]++;
    cov_ql2jftyom().s[51]++;
    return alert.type === 'error';
  }).length);
  const warningCount =
  /* istanbul ignore next */
  (cov_ql2jftyom().s[52]++, unreadAlerts.filter(alert => {
    /* istanbul ignore next */
    cov_ql2jftyom().f[22]++;
    cov_ql2jftyom().s[53]++;
    return alert.type === 'warning';
  }).length);
  /* istanbul ignore next */
  cov_ql2jftyom().s[54]++;
  if (unreadAlerts.length === 0) {
    /* istanbul ignore next */
    cov_ql2jftyom().b[27][0]++;
    cov_ql2jftyom().s[55]++;
    return /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
      className: "flex items-center gap-2 text-sm text-green-600",
      children: [/*#__PURE__*/(0, _jsxruntime.jsx)(CheckCircle, {
        className: "w-4 h-4"
      }), "Sin alertas pendientes"]
    });
  } else
  /* istanbul ignore next */
  {
    cov_ql2jftyom().b[27][1]++;
  }
  cov_ql2jftyom().s[56]++;
  return /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
    className: "flex items-center gap-3",
    children: [
    /* istanbul ignore next */
    (cov_ql2jftyom().b[28][0]++, criticalCount > 0) &&
