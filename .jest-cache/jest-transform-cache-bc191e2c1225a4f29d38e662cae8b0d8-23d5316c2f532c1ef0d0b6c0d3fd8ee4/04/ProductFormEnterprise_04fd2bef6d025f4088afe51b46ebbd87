    (cov_hhz6pgq3l().b[2][0]++, !data) ||
    /* istanbul ignore next */
    (cov_hhz6pgq3l().b[2][1]++, !onSave)) {
      /* istanbul ignore next */
      cov_hhz6pgq3l().b[1][0]++;
      cov_hhz6pgq3l().s[23]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_hhz6pgq3l().b[1][1]++;
    }
    const timer =
    /* istanbul ignore next */
    (cov_hhz6pgq3l().s[24]++, setTimeout(async () => {
      /* istanbul ignore next */
      cov_hhz6pgq3l().f[3]++;
      cov_hhz6pgq3l().s[25]++;
      try {
        /* istanbul ignore next */
        cov_hhz6pgq3l().s[26]++;
        setIsSaving(true);
        /* istanbul ignore next */
        cov_hhz6pgq3l().s[27]++;
        await onSave(data);
        /* istanbul ignore next */
        cov_hhz6pgq3l().s[28]++;
        setLastSaved(new Date());
        /* istanbul ignore next */
        cov_hhz6pgq3l().s[29]++;
        _reacthottoast.toast.success('Guardado automático completado', {
          duration: 2000
        });
      } catch (error) {
        /* istanbul ignore next */
        cov_hhz6pgq3l().s[30]++;
        console.error('Auto-save failed:', error);
        /* istanbul ignore next */
        cov_hhz6pgq3l().s[31]++;
        _reacthottoast.toast.error('Error en guardado automático');
      } finally {
        /* istanbul ignore next */
        cov_hhz6pgq3l().s[32]++;
        setIsSaving(false);
      }
    }, delay));
    /* istanbul ignore next */
    cov_hhz6pgq3l().s[33]++;
    return () => {
      /* istanbul ignore next */
      cov_hhz6pgq3l().f[4]++;
      cov_hhz6pgq3l().s[34]++;
      return clearTimeout(timer);
    };
  }, [data, onSave, delay]);
  /* istanbul ignore next */
  cov_hhz6pgq3l().s[35]++;
  return {
    lastSaved,
    isSaving
  };
}
// Slug validation API
async function validateSlug(slug, productId) {
  /* istanbul ignore next */
  cov_hhz6pgq3l().f[5]++;
  cov_hhz6pgq3l().s[36]++;
  if (!slug) {
    /* istanbul ignore next */
    cov_hhz6pgq3l().b[3][0]++;
    cov_hhz6pgq3l().s[37]++;
    return true;
  } else
  /* istanbul ignore next */
  {
    cov_hhz6pgq3l().b[3][1]++;
  }
  const response =
  /* istanbul ignore next */
  (cov_hhz6pgq3l().s[38]++, await fetch(`/api/admin/products/validate-slug`, {
    method: 'POST',
    headers: {
      'Content-Type': 'application/json'
    },
    body: JSON.stringify({
      slug,
      productId
    })
  }));
  const result =
  /* istanbul ignore next */
  (cov_hhz6pgq3l().s[39]++, await response.json());
  /* istanbul ignore next */
  cov_hhz6pgq3l().s[40]++;
  return result.available;
}
