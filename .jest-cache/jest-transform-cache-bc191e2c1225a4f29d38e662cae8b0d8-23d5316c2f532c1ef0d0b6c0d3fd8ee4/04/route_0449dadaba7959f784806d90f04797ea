          error: `Tipo de anÃ¡lisis no soportado: ${type}`
        }, {
          status: 400
        });
    }
    /* istanbul ignore next */
    cov_2dbo0ctxbp().s[43]++;
    console.log(`â AnÃ¡lisis ${type} completado`);
    /* istanbul ignore next */
    cov_2dbo0ctxbp().s[44]++;
    return _server.NextResponse.json({
      success: true,
      type,
      url,
      timestamp: new Date().toISOString(),
      ...result
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_2dbo0ctxbp().s[45]++;
    console.error('â Error en anÃ¡lisis SEO:', error);
    /* istanbul ignore next */
    cov_2dbo0ctxbp().s[46]++;
    return _server.NextResponse.json({
      success: false,
      error: 'Error interno del servidor',
      message: error instanceof Error ?
      /* istanbul ignore next */
      (cov_2dbo0ctxbp().b[8][0]++, error.message) :
      /* istanbul ignore next */
      (cov_2dbo0ctxbp().b[8][1]++, 'Error desconocido')
    }, {
      status: 500
    });
  }
}
