const getDefaultColor = productType => {
  /* istanbul ignore next */
  cov_c5lqdjo38().f[24]++;
  cov_c5lqdjo38().s[75]++;
  if (!productType.hasColorSelector) {
    /* istanbul ignore next */
    cov_c5lqdjo38().b[35][0]++;
    cov_c5lqdjo38().s[76]++;
    // Productos sin selector de color tienen un color fijo
    if (productType.id === 'poximix') {
      /* istanbul ignore next */
      cov_c5lqdjo38().b[36][0]++;
      cov_c5lqdjo38().s[77]++;
      return 'gris';
    } else
    /* istanbul ignore next */
    {
      cov_c5lqdjo38().b[36][1]++;
    }
    cov_c5lqdjo38().s[78]++;
    return 'natural';
  } else
  /* istanbul ignore next */
  {
    cov_c5lqdjo38().b[35][1]++;
  }
  cov_c5lqdjo38().s[79]++;
  return 'blanco-puro'; // Color por defecto para productos con selector
};
