const formatCapacity = (capacity, unit) => {
  /* istanbul ignore next */
  cov_c5lqdjo38().f[23]++;
  cov_c5lqdjo38().s[64]++;
  // Si la capacidad es "Sin especificar", devolverla tal como est
  if (capacity === 'Sin especificar') {
    /* istanbul ignore next */
    cov_c5lqdjo38().b[26][0]++;
    cov_c5lqdjo38().s[65]++;
    return capacity;
  } else
  /* istanbul ignore next */
  {
    cov_c5lqdjo38().b[26][1]++;
  }
  cov_c5lqdjo38().s[66]++;
  switch (unit) {
    case 'litros':
      /* istanbul ignore next */
      cov_c5lqdjo38().b[27][0]++;
      cov_c5lqdjo38().s[67]++;
      // Verificar si ya termina con 'L' (mayscula) o 'l' (minscula)
      if (
      /* istanbul ignore next */
      (cov_c5lqdjo38().b[29][0]++, capacity.endsWith('L')) ||
      /* istanbul ignore next */
      (cov_c5lqdjo38().b[29][1]++, capacity.endsWith('l'))) {
        /* istanbul ignore next */
        cov_c5lqdjo38().b[28][0]++;
        cov_c5lqdjo38().s[68]++;
        // Si termina con 'l' minscula, convertir a 'L' mayscula
        return capacity.endsWith('l') ?
        /* istanbul ignore next */
        (cov_c5lqdjo38().b[30][0]++, capacity.slice(0, -1) + 'L') :
        /* istanbul ignore next */
        (cov_c5lqdjo38().b[30][1]++, capacity);
      } else
      /* istanbul ignore next */
      {
        cov_c5lqdjo38().b[28][1]++;
      }
      cov_c5lqdjo38().s[69]++;
      return `${capacity}L`;
    case 'kg':
      /* istanbul ignore next */
      cov_c5lqdjo38().b[27][1]++;
      cov_c5lqdjo38().s[70]++;
      return capacity.endsWith('kg') ?
      /* istanbul ignore next */
      (cov_c5lqdjo38().b[31][0]++, capacity) :
      /* istanbul ignore next */
      (cov_c5lqdjo38().b[31][1]++, `${capacity}kg`);
    case 'metros':
      /* istanbul ignore next */
      cov_c5lqdjo38().b[27][2]++;
      cov_c5lqdjo38().s[71]++;
      return capacity.endsWith('m') ?
      /* istanbul ignore next */
      (cov_c5lqdjo38().b[32][0]++, capacity) :
      /* istanbul ignore next */
      (cov_c5lqdjo38().b[32][1]++, `${capacity}m`);
    case 'unidades':
      /* istanbul ignore next */
      cov_c5lqdjo38().b[27][3]++;
      cov_c5lqdjo38().s[72]++;
      return capacity.includes('unidad') ?
      /* istanbul ignore next */
      (cov_c5lqdjo38().b[33][0]++, capacity) :
      /* istanbul ignore next */
      (cov_c5lqdjo38().b[33][1]++, `${capacity} unidad${capacity !== '1' ?
      /* istanbul ignore next */
      (cov_c5lqdjo38().b[34][0]++, 'es') :
      /* istanbul ignore next */
      (cov_c5lqdjo38().b[34][1]++, '')}`);
    default:
      /* istanbul ignore next */
      cov_c5lqdjo38().b[27][4]++;
      cov_c5lqdjo38().s[73]++;
      return capacity;
  }
};
