      _sonner.toast.success('Configuracin de sesiones actualizada');
    } else
    /* istanbul ignore next */
    {
      cov_10ofbem7in().b[13][1]++;
    }
    cov_10ofbem7in().s[67]++;
    return success;
  }, [updateSettings]));
  // Cargar configuracin al montar el componente
  /* istanbul ignore next */
  cov_10ofbem7in().s[68]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_10ofbem7in().f[11]++;
    cov_10ofbem7in().s[69]++;
    fetchSettings();
  }, [fetchSettings]);
  /* istanbul ignore next */
  cov_10ofbem7in().s[70]++;
  return {
