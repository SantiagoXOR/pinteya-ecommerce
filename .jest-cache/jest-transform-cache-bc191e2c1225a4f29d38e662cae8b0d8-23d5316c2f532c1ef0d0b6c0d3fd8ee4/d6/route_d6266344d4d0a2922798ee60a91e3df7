  }
  cov_1wu7m4dra0().s[40]++;
  if (filters.reserved_only) {
    /* istanbul ignore next */
    cov_1wu7m4dra0().b[7][0]++;
    cov_1wu7m4dra0().s[41]++;
    query = query.gt('reserved_stock', 0);
  } else
  /* istanbul ignore next */
  {
    cov_1wu7m4dra0().b[7][1]++;
  }
  cov_1wu7m4dra0().s[42]++;
  if (filters.search) {
    /* istanbul ignore next */
    cov_1wu7m4dra0().b[8][0]++;
    cov_1wu7m4dra0().s[43]++;
    query = query.or(`product.name.ilike.%${filters.search}%,product.sku.ilike.%${filters.search}%,variant.name.ilike.%${filters.search}%,variant.sku.ilike.%${filters.search}%`);
  } else
  /* istanbul ignore next */
  {
    cov_1wu7m4dra0().b[8][1]++;
  }
  // Contar total
  const {
    count
  } =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[44]++, await query.select('*', {
    count: 'exact',
    head: true
  }));
  // Aplicar paginaciÃ³n y ordenamiento
  const offset =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[45]++, (filters.page - 1) * filters.limit);
  /* istanbul ignore next */
  cov_1wu7m4dra0().s[46]++;
  query = query.order(filters.sort_by, {
    ascending: filters.sort_order === 'asc'
  }).range(offset, offset + filters.limit - 1);
  const {
    data,
    error
  } =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[47]++, await query);
  /* istanbul ignore next */
  cov_1wu7m4dra0().s[48]++;
  if (error) {
    /* istanbul ignore next */
    cov_1wu7m4dra0().b[9][0]++;
    cov_1wu7m4dra0().s[49]++;
    throw new Error(`Error al obtener inventario: ${error.message}`);
  } else
  /* istanbul ignore next */
  {
    cov_1wu7m4dra0().b[9][1]++;
  }
  // Procesar datos para incluir campos calculados
  const processedData =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[50]++, (
  /* istanbul ignore next */
  (cov_1wu7m4dra0().b[10][0]++, data) ||
  /* istanbul ignore next */
  (cov_1wu7m4dra0().b[10][1]++, [])).map(item => {
    /* istanbul ignore next */
    cov_1wu7m4dra0().f[6]++;
    cov_1wu7m4dra0().s[51]++;
    return {
      ...item,
      available_stock: item.current_stock - item.reserved_stock,
      is_low_stock: item.current_stock <= item.reorder_point,
      is_out_of_stock: item.current_stock === 0
    };
  }));
  /* istanbul ignore next */
  cov_1wu7m4dra0().s[52]++;
  return {
    items: processedData,
    total:
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[11][0]++, count) ||
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[11][1]++, 0),
    totalPages: Math.ceil((
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[12][0]++, count) ||
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[12][1]++, 0)) / filters.limit)
  };
}
async function adjustStock(adjustment, userId) {
  /* istanbul ignore next */
  cov_1wu7m4dra0().f[7]++;
  const {
    data: currentInventory,
    error: inventoryError
  } =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[53]++, await supabase.from('inventory').select('current_stock').eq('product_id', adjustment.product_id).eq('location', adjustment.location).eq('variant_id',
  /* istanbul ignore next */
  (cov_1wu7m4dra0().b[13][0]++, adjustment.variant_id) ||
  /* istanbul ignore next */
  (cov_1wu7m4dra0().b[13][1]++, null)).single());
  /* istanbul ignore next */
  cov_1wu7m4dra0().s[54]++;
  if (inventoryError) {
    /* istanbul ignore next */
    cov_1wu7m4dra0().b[14][0]++;
    cov_1wu7m4dra0().s[55]++;
    throw new Error(`Error al obtener inventario actual: ${inventoryError.message}`);
  } else
  /* istanbul ignore next */
  {
    cov_1wu7m4dra0().b[14][1]++;
  }
  const currentStock =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[56]++, currentInventory.current_stock);
  let newStock;
  /* istanbul ignore next */
  cov_1wu7m4dra0().s[57]++;
  switch (adjustment.adjustment_type) {
    case 'increase':
      /* istanbul ignore next */
      cov_1wu7m4dra0().b[15][0]++;
      cov_1wu7m4dra0().s[58]++;
      newStock = currentStock + adjustment.quantity;
      /* istanbul ignore next */
      cov_1wu7m4dra0().s[59]++;
      break;
    case 'decrease':
      /* istanbul ignore next */
      cov_1wu7m4dra0().b[15][1]++;
      cov_1wu7m4dra0().s[60]++;
      newStock = Math.max(0, currentStock - adjustment.quantity);
      /* istanbul ignore next */
      cov_1wu7m4dra0().s[61]++;
      break;
    case 'set':
      /* istanbul ignore next */
      cov_1wu7m4dra0().b[15][2]++;
      cov_1wu7m4dra0().s[62]++;
      newStock = adjustment.quantity;
      /* istanbul ignore next */
      cov_1wu7m4dra0().s[63]++;
      break;
    default:
      /* istanbul ignore next */
      cov_1wu7m4dra0().b[15][3]++;
      cov_1wu7m4dra0().s[64]++;
      throw new Error('Tipo de ajuste invÃ¡lido');
  }
  // Actualizar inventario
  const {
    error: updateError
  } =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[65]++, await supabase.from('inventory').update({
    current_stock: newStock,
    updated_at: new Date().toISOString()
  }).eq('product_id', adjustment.product_id).eq('location', adjustment.location).eq('variant_id',
  /* istanbul ignore next */
  (cov_1wu7m4dra0().b[16][0]++, adjustment.variant_id) ||
  /* istanbul ignore next */
  (cov_1wu7m4dra0().b[16][1]++, null)));
  /* istanbul ignore next */
  cov_1wu7m4dra0().s[66]++;
  if (updateError) {
    /* istanbul ignore next */
    cov_1wu7m4dra0().b[17][0]++;
    cov_1wu7m4dra0().s[67]++;
    throw new Error(`Error al actualizar inventario: ${updateError.message}`);
  } else
  /* istanbul ignore next */
  {
    cov_1wu7m4dra0().b[17][1]++;
  }
  // Crear movimiento de stock
  const {
    error: movementError
  } =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[68]++, await supabase.from('stock_movements').insert({
    product_id: adjustment.product_id,
    variant_id: adjustment.variant_id,
    location: adjustment.location,
    movement_type: 'adjustment',
    quantity: adjustment.adjustment_type === 'decrease' ?
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[18][0]++, -adjustment.quantity) :
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[18][1]++, adjustment.quantity),
    previous_stock: currentStock,
    new_stock: newStock,
    reason: adjustment.reason,
    notes: adjustment.notes,
    user_id: userId,
    created_at: new Date().toISOString()
  }));
  /* istanbul ignore next */
  cov_1wu7m4dra0().s[69]++;
  if (movementError) {
    /* istanbul ignore next */
    cov_1wu7m4dra0().b[19][0]++;
    cov_1wu7m4dra0().s[70]++;
    throw new Error(`Error al crear movimiento de stock: ${movementError.message}`);
  } else
  /* istanbul ignore next */
  {
    cov_1wu7m4dra0().b[19][1]++;
  }
  cov_1wu7m4dra0().s[71]++;
  return {
    previous_stock: currentStock,
    new_stock: newStock,
    adjustment_quantity: adjustment.quantity
  };
}
async function createReservation(reservation, userId) {
  /* istanbul ignore next */
  cov_1wu7m4dra0().f[8]++;
  // Verificar stock disponible
  const {
    data: inventory,
    error: inventoryError
  } =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[72]++, await supabase.from('inventory').select('current_stock, reserved_stock').eq('product_id', reservation.product_id).eq('location', reservation.location).eq('variant_id',
  /* istanbul ignore next */
  (cov_1wu7m4dra0().b[20][0]++, reservation.variant_id) ||
  /* istanbul ignore next */
  (cov_1wu7m4dra0().b[20][1]++, null)).single());
  /* istanbul ignore next */
  cov_1wu7m4dra0().s[73]++;
  if (inventoryError) {
    /* istanbul ignore next */
    cov_1wu7m4dra0().b[21][0]++;
    cov_1wu7m4dra0().s[74]++;
    throw new Error(`Error al obtener inventario: ${inventoryError.message}`);
  } else
  /* istanbul ignore next */
  {
    cov_1wu7m4dra0().b[21][1]++;
  }
  const availableStock =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[75]++, inventory.current_stock - inventory.reserved_stock);
  /* istanbul ignore next */
  cov_1wu7m4dra0().s[76]++;
  if (availableStock < reservation.quantity) {
    /* istanbul ignore next */
    cov_1wu7m4dra0().b[22][0]++;
    cov_1wu7m4dra0().s[77]++;
    throw new Error(`Stock insuficiente. Disponible: ${availableStock}, Solicitado: ${reservation.quantity}`);
  } else
  /* istanbul ignore next */
  {
    cov_1wu7m4dra0().b[22][1]++;
  }
  // Crear reserva
  const {
    data: newReservation,
    error: reservationError
  } =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[78]++, await supabase.from('stock_reservations').insert({
    product_id: reservation.product_id,
    variant_id: reservation.variant_id,
    location: reservation.location,
    quantity: reservation.quantity,
    reason: reservation.reason,
    notes: reservation.notes,
    expires_at: reservation.expires_at,
    status: 'active',
    user_id: userId,
    created_at: new Date().toISOString(),
    updated_at: new Date().toISOString()
  }).select().single());
  /* istanbul ignore next */
  cov_1wu7m4dra0().s[79]++;
  if (reservationError) {
    /* istanbul ignore next */
    cov_1wu7m4dra0().b[23][0]++;
    cov_1wu7m4dra0().s[80]++;
    throw new Error(`Error al crear reserva: ${reservationError.message}`);
  } else
  /* istanbul ignore next */
  {
    cov_1wu7m4dra0().b[23][1]++;
  }
  // Actualizar stock reservado
  const {
    error: updateError
  } =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[81]++, await supabase.from('inventory').update({
    reserved_stock: inventory.reserved_stock + reservation.quantity,
    updated_at: new Date().toISOString()
  }).eq('product_id', reservation.product_id).eq('location', reservation.location).eq('variant_id',
  /* istanbul ignore next */
  (cov_1wu7m4dra0().b[24][0]++, reservation.variant_id) ||
  /* istanbul ignore next */
  (cov_1wu7m4dra0().b[24][1]++, null)));
  /* istanbul ignore next */
  cov_1wu7m4dra0().s[82]++;
  if (updateError) {
    /* istanbul ignore next */
    cov_1wu7m4dra0().b[25][0]++;
    cov_1wu7m4dra0().s[83]++;
    throw new Error(`Error al actualizar stock reservado: ${updateError.message}`);
  } else
  /* istanbul ignore next */
  {
    cov_1wu7m4dra0().b[25][1]++;
  }
  // Crear movimiento de stock
  cov_1wu7m4dra0().s[84]++;
  await supabase.from('stock_movements').insert({
    product_id: reservation.product_id,
    variant_id: reservation.variant_id,
    location: reservation.location,
    movement_type: 'reservation',
    quantity: -reservation.quantity,
    previous_stock: inventory.current_stock,
    new_stock: inventory.current_stock,
    reason: reservation.reason,
    notes: reservation.notes,
    reference_id: newReservation.id,
    user_id: userId,
    created_at: new Date().toISOString()
  });
  /* istanbul ignore next */
  cov_1wu7m4dra0().s[85]++;
  return newReservation;
}
async function getStockMovements(filters) {
  /* istanbul ignore next */
  cov_1wu7m4dra0().f[9]++;
  let query =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[86]++, supabase.from('stock_movements').select(`
      *,
      product:products!stock_movements_product_id_fkey(
        name,
        sku
      ),
      variant:product_variants!stock_movements_variant_id_fkey(
        name,
        sku
      ),
      user:profiles!stock_movements_user_id_fkey(
        full_name,
        email
      )
    `));
  // Aplicar filtros
  /* istanbul ignore next */
  cov_1wu7m4dra0().s[87]++;
  if (filters.product_id) {
    /* istanbul ignore next */
    cov_1wu7m4dra0().b[26][0]++;
    cov_1wu7m4dra0().s[88]++;
    query = query.eq('product_id', filters.product_id);
  } else
  /* istanbul ignore next */
  {
    cov_1wu7m4dra0().b[26][1]++;
  }
  cov_1wu7m4dra0().s[89]++;
  if (filters.variant_id) {
    /* istanbul ignore next */
    cov_1wu7m4dra0().b[27][0]++;
    cov_1wu7m4dra0().s[90]++;
    query = query.eq('variant_id', filters.variant_id);
  } else
  /* istanbul ignore next */
  {
    cov_1wu7m4dra0().b[27][1]++;
  }
  cov_1wu7m4dra0().s[91]++;
  if (filters.location) {
    /* istanbul ignore next */
    cov_1wu7m4dra0().b[28][0]++;
    cov_1wu7m4dra0().s[92]++;
    query = query.eq('location', filters.location);
  } else
  /* istanbul ignore next */
  {
    cov_1wu7m4dra0().b[28][1]++;
  }
  cov_1wu7m4dra0().s[93]++;
  if (filters.movement_type) {
    /* istanbul ignore next */
    cov_1wu7m4dra0().b[29][0]++;
    cov_1wu7m4dra0().s[94]++;
    query = query.eq('movement_type', filters.movement_type);
  } else
  /* istanbul ignore next */
  {
    cov_1wu7m4dra0().b[29][1]++;
  }
  cov_1wu7m4dra0().s[95]++;
  if (filters.date_from) {
    /* istanbul ignore next */
    cov_1wu7m4dra0().b[30][0]++;
    cov_1wu7m4dra0().s[96]++;
    query = query.gte('created_at', filters.date_from);
  } else
  /* istanbul ignore next */
  {
    cov_1wu7m4dra0().b[30][1]++;
  }
  cov_1wu7m4dra0().s[97]++;
  if (filters.date_to) {
    /* istanbul ignore next */
    cov_1wu7m4dra0().b[31][0]++;
    cov_1wu7m4dra0().s[98]++;
    query = query.lte('created_at', filters.date_to);
  } else
  /* istanbul ignore next */
  {
    cov_1wu7m4dra0().b[31][1]++;
  }
  // Contar total
  const {
    count
  } =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[99]++, await query.select('*', {
    count: 'exact',
    head: true
  }));
  // Aplicar paginaciÃ³n y ordenamiento
  const offset =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[100]++, (filters.page - 1) * filters.limit);
  /* istanbul ignore next */
  cov_1wu7m4dra0().s[101]++;
  query = query.order(filters.sort_by, {
    ascending: filters.sort_order === 'asc'
  }).range(offset, offset + filters.limit - 1);
  const {
    data,
    error
  } =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[102]++, await query);
  /* istanbul ignore next */
  cov_1wu7m4dra0().s[103]++;
  if (error) {
    /* istanbul ignore next */
    cov_1wu7m4dra0().b[32][0]++;
    cov_1wu7m4dra0().s[104]++;
    throw new Error(`Error al obtener movimientos de stock: ${error.message}`);
  } else
  /* istanbul ignore next */
  {
    cov_1wu7m4dra0().b[32][1]++;
  }
  cov_1wu7m4dra0().s[105]++;
  return {
    movements:
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[33][0]++, data) ||
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[33][1]++, []),
    total:
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[34][0]++, count) ||
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[34][1]++, 0),
    totalPages: Math.ceil((
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[35][0]++, count) ||
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[35][1]++, 0)) / filters.limit)
  };
}
async function getInventoryStats() {
  /* istanbul ignore next */
  cov_1wu7m4dra0().f[10]++;
  // Obtener estadÃ­sticas bÃ¡sicas
  const {
    data: inventoryData,
    error
  } =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[106]++, await supabase.from('inventory').select(`
      *,
      product:products!inventory_product_id_fkey(
        name,
        sku
      )
    `));
  /* istanbul ignore next */
  cov_1wu7m4dra0().s[107]++;
  if (error) {
    /* istanbul ignore next */
    cov_1wu7m4dra0().b[36][0]++;
    cov_1wu7m4dra0().s[108]++;
    throw new Error(`Error al obtener estadÃ­sticas de inventario: ${error.message}`);
  } else
  /* istanbul ignore next */
  {
    cov_1wu7m4dra0().b[36][1]++;
  }
  const inventory =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[109]++,
  /* istanbul ignore next */
  (cov_1wu7m4dra0().b[37][0]++, inventoryData) ||
  /* istanbul ignore next */
  (cov_1wu7m4dra0().b[37][1]++, []));
  const now =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[110]++, new Date());
  const last24h =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[111]++, new Date(now.getTime() - 24 * 60 * 60 * 1000));
  const last7d =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[112]++, new Date(now.getTime() - 7 * 24 * 60 * 60 * 1000));
  const last30d =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[113]++, new Date(now.getTime() - 30 * 24 * 60 * 60 * 1000));
  // Obtener movimientos recientes
  const {
    data: movements
  } =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[114]++, await supabase.from('stock_movements').select('created_at, movement_type'));
  const recentMovements =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[115]++, {
    last_24h: (
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[38][0]++, movements) ||
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[38][1]++, [])).filter(m => {
      /* istanbul ignore next */
      cov_1wu7m4dra0().f[11]++;
      cov_1wu7m4dra0().s[116]++;
      return new Date(m.created_at) >= last24h;
    }).length,
    last_7d: (
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[39][0]++, movements) ||
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[39][1]++, [])).filter(m => {
      /* istanbul ignore next */
      cov_1wu7m4dra0().f[12]++;
      cov_1wu7m4dra0().s[117]++;
      return new Date(m.created_at) >= last7d;
    }).length,
    last_30d: (
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[40][0]++, movements) ||
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[40][1]++, [])).filter(m => {
      /* istanbul ignore next */
      cov_1wu7m4dra0().f[13]++;
      cov_1wu7m4dra0().s[118]++;
      return new Date(m.created_at) >= last30d;
    }).length
  });
  // Resumen de movimientos
  const movementSummary =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[119]++, (
  /* istanbul ignore next */
  (cov_1wu7m4dra0().b[41][0]++, movements) ||
  /* istanbul ignore next */
  (cov_1wu7m4dra0().b[41][1]++, [])).reduce((acc, movement) => {
    /* istanbul ignore next */
    cov_1wu7m4dra0().f[14]++;
    cov_1wu7m4dra0().s[120]++;
    acc[movement.movement_type] = (
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[42][0]++, acc[movement.movement_type]) ||
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[42][1]++, 0)) + 1;
    /* istanbul ignore next */
    cov_1wu7m4dra0().s[121]++;
    return acc;
  }, {}));
  // Productos Ãºnicos y variantes
  const uniqueProducts =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[122]++, new Set(inventory.map(item => {
    /* istanbul ignore next */
    cov_1wu7m4dra0().f[15]++;
    cov_1wu7m4dra0().s[123]++;
    return item.product_id;
  })));
  const uniqueVariants =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[124]++, inventory.filter(item => {
    /* istanbul ignore next */
    cov_1wu7m4dra0().f[16]++;
    cov_1wu7m4dra0().s[125]++;
    return item.variant_id;
  }).length);
  // Valor total del stock
  const totalStockValue =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[126]++, inventory.reduce((sum, item) => {
    /* istanbul ignore next */
    cov_1wu7m4dra0().f[17]++;
    cov_1wu7m4dra0().s[127]++;
    return sum + item.current_stock * (
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[43][0]++, item.cost_per_unit) ||
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[43][1]++, 0));
  }, 0));
  // Stock reservado
  const reservedStockValue =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[128]++, inventory.reduce((sum, item) => {
    /* istanbul ignore next */
    cov_1wu7m4dra0().f[18]++;
    cov_1wu7m4dra0().s[129]++;
    return sum + item.reserved_stock * (
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[44][0]++, item.cost_per_unit) ||
    /* istanbul ignore next */
    (cov_1wu7m4dra0().b[44][1]++, 0));
  }, 0));
  // Items con stock bajo y sin stock
  const lowStockItems =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[130]++, inventory.filter(item => {
    /* istanbul ignore next */
    cov_1wu7m4dra0().f[19]++;
    cov_1wu7m4dra0().s[131]++;
    return item.current_stock <= item.reorder_point;
  }).length);
  const outOfStockItems =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[132]++, inventory.filter(item => {
    /* istanbul ignore next */
    cov_1wu7m4dra0().f[20]++;
    cov_1wu7m4dra0().s[133]++;
    return item.current_stock === 0;
  }).length);
  // Ubicaciones Ãºnicas
  const locations =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[134]++, [...new Set(inventory.map(item => {
    /* istanbul ignore next */
    cov_1wu7m4dra0().f[21]++;
    cov_1wu7m4dra0().s[135]++;
    return item.location;
  }))]);
  // Top productos por valor
  const topProductsByValue =
  /* istanbul ignore next */
  (cov_1wu7m4dra0().s[136]++, inventory.map(item => {
    /* istanbul ignore next */
    cov_1wu7m4dra0().f[22]++;
    cov_1wu7m4dra0().s[137]++;
    return {
      product_id: item.product_id,
      product_name: item.product.name,
      stock_value: item.current_stock * (
      /* istanbul ignore next */
      (cov_1wu7m4dra0().b[45][0]++, item.cost_per_unit) ||
      /* istanbul ignore next */
      (cov_1wu7m4dra0().b[45][1]++, 0)),
      current_stock: item.current_stock
    };
  }).sort((a, b) => {
    /* istanbul ignore next */
    cov_1wu7m4dra0().f[23]++;
    cov_1wu7m4dra0().s[138]++;
    return b.stock_value - a.stock_value;
  }).slice(0, 10));
  /* istanbul ignore next */
  cov_1wu7m4dra0().s[139]++;
  return {
    total_products: uniqueProducts.size,
    total_variants: uniqueVariants,
    total_stock_value: totalStockValue,
    low_stock_items: lowStockItems,
    out_of_stock_items: outOfStockItems,
    reserved_stock_value: reservedStockValue,
    locations,
    recent_movements: recentMovements,
    top_products_by_value: topProductsByValue,
    movement_summary: movementSummary
  };
}
