const debounce = (func, wait) => {
  /* istanbul ignore next */
  cov_29sl8ia230().f[25]++;
  let timeout;
  /* istanbul ignore next */
  cov_29sl8ia230().s[47]++;
  return (...args) => {
    /* istanbul ignore next */
    cov_29sl8ia230().f[26]++;
    cov_29sl8ia230().s[48]++;
    clearTimeout(timeout);
    /* istanbul ignore next */
    cov_29sl8ia230().s[49]++;
    timeout = setTimeout(() => {
      /* istanbul ignore next */
      cov_29sl8ia230().f[27]++;
      cov_29sl8ia230().s[50]++;
      return func(...args);
    }, wait);
  };
};
/* istanbul ignore next */
cov_29sl8ia230().s[51]++;
const throttle = (func, limit) => {
  /* istanbul ignore next */
  cov_29sl8ia230().f[28]++;
  let inThrottle;
  /* istanbul ignore next */
  cov_29sl8ia230().s[52]++;
  return (...args) => {
    /* istanbul ignore next */
    cov_29sl8ia230().f[29]++;
    cov_29sl8ia230().s[53]++;
    if (!inThrottle) {
      /* istanbul ignore next */
      cov_29sl8ia230().b[54][0]++;
      cov_29sl8ia230().s[54]++;
      func(...args);
      /* istanbul ignore next */
      cov_29sl8ia230().s[55]++;
      inThrottle = true;
      /* istanbul ignore next */
      cov_29sl8ia230().s[56]++;
      setTimeout(() => {
        /* istanbul ignore next */
        cov_29sl8ia230().f[30]++;
        cov_29sl8ia230().s[57]++;
        return inThrottle = false;
      }, limit);
    } else
    /* istanbul ignore next */
    {
      cov_29sl8ia230().b[54][1]++;
    }
  };
};
