                    const errorResponse = {
                        data: null,
                        success: false,
                        error: `Acci贸n no v谩lida: ${action}`
                    };
                    return _server.NextResponse.json(errorResponse, {
                        status: 400
                    });
            }
        } catch (error) {
            // Log del error de seguridad
            securityLogger.logApiError(securityLogger.context, error, {
                endpoint: '/api/auth/security',
                method: 'GET',
                action: request.url
            });
