function productFormReducer(state, action) {
  /* istanbul ignore next */
  cov_2pld42bkgj().f[3]++;
  cov_2pld42bkgj().s[8]++;
  switch (action.type) {
    case 'SET_FIELD':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][0]++;
      cov_2pld42bkgj().s[9]++;
      return {
        ...state,
        [action.field]: action.value,
        isDirty: true,
        // Limpiar error del campo si existe
        errors: {
          ...state.errors,
          [action.field]: undefined
        }
      };
    case 'SET_MULTIPLE_FIELDS':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][1]++;
      cov_2pld42bkgj().s[10]++;
      return {
        ...state,
        ...action.fields,
        isDirty: true
      };
    case 'SET_LOADING':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][2]++;
      cov_2pld42bkgj().s[11]++;
      return {
        ...state,
        isLoading: action.payload
      };
    case 'SET_SAVING':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][3]++;
      cov_2pld42bkgj().s[12]++;
      return {
        ...state,
        isSaving: action.payload
      };
    case 'SET_ERROR':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][4]++;
      cov_2pld42bkgj().s[13]++;
      return {
        ...state,
        errors: {
          ...state.errors,
          [action.field]: action.error
        }
      };
    case 'CLEAR_ERROR':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][5]++;
      const {
        [action.field]: _,
        ...restErrors
      } =
      /* istanbul ignore next */
      (cov_2pld42bkgj().s[14]++, state.errors);
      /* istanbul ignore next */
      cov_2pld42bkgj().s[15]++;
      return {
        ...state,
        errors: restErrors
      };
    case 'CLEAR_ALL_ERRORS':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][6]++;
      cov_2pld42bkgj().s[16]++;
      return {
        ...state,
        errors: {},
        validationErrors: {}
      };
    case 'SET_VALIDATION_ERRORS':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][7]++;
      cov_2pld42bkgj().s[17]++;
      return {
        ...state,
        validationErrors: action.errors
      };
    case 'SET_DIRTY':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][8]++;
      cov_2pld42bkgj().s[18]++;
      return {
        ...state,
        isDirty: action.payload
      };
    case 'SET_STEP':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][9]++;
      cov_2pld42bkgj().s[19]++;
      return {
        ...state,
        currentStep: Math.max(0, Math.min(4, action.step))
      };
    case 'NEXT_STEP':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][10]++;
      cov_2pld42bkgj().s[20]++;
      return {
        ...state,
        currentStep: Math.min(4, state.currentStep + 1)
      };
    case 'PREV_STEP':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][11]++;
      cov_2pld42bkgj().s[21]++;
      return {
        ...state,
        currentStep: Math.max(0, state.currentStep - 1)
      };
    case 'ADD_IMAGE':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][12]++;
      cov_2pld42bkgj().s[22]++;
      return {
        ...state,
        images: [...state.images, action.image],
        isDirty: true
      };
    case 'REMOVE_IMAGE':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][13]++;
      cov_2pld42bkgj().s[23]++;
      return {
        ...state,
        images: state.images.filter((_, index) => {
          /* istanbul ignore next */
          cov_2pld42bkgj().f[4]++;
          cov_2pld42bkgj().s[24]++;
          return index !== action.index;
        }),
        isDirty: true
      };
    case 'ADD_VARIANT':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][14]++;
      cov_2pld42bkgj().s[25]++;
      return {
        ...state,
        variants: [...state.variants, action.variant],
        isDirty: true
      };
    case 'REMOVE_VARIANT':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][15]++;
      cov_2pld42bkgj().s[26]++;
      return {
        ...state,
