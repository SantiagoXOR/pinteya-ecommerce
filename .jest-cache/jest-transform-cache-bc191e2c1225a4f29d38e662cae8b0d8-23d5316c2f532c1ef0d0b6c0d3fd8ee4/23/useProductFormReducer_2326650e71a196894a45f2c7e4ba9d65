          /* istanbul ignore next */
          cov_2pld42bkgj().f[5]++;
          cov_2pld42bkgj().s[27]++;
          return index !== action.index;
        }),
        isDirty: true
      };
    case 'UPDATE_VARIANT':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][16]++;
      cov_2pld42bkgj().s[28]++;
      return {
        ...state,
        variants: state.variants.map((variant, index) => {
