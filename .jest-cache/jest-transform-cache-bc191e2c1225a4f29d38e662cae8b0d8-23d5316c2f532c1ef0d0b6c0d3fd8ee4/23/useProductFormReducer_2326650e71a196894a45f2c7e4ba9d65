      (cov_2pld42bkgj().b[8][0]++, state.dimensions.length > 0) &&
      /* istanbul ignore next */
      (cov_2pld42bkgj().b[8][1]++, state.dimensions.width > 0) &&
      /* istanbul ignore next */
      (cov_2pld42bkgj().b[8][2]++, state.dimensions.height > 0),
      // Contadores
      imageCount: state.images.length,
      variantCount: state.variants.length,
      keywordCount: state.seoKeywords.length,
      // Obtener errores por paso
      getStepErrors: step => {
        /* istanbul ignore next */
        cov_2pld42bkgj().f[32]++;
