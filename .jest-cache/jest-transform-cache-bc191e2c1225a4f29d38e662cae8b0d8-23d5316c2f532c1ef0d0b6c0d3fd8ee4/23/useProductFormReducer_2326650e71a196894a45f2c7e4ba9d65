          /* istanbul ignore next */
          (cov_2pld42bkgj().b[1][0]++, action.variant) :
          /* istanbul ignore next */
          (cov_2pld42bkgj().b[1][1]++, variant);
        }),
        isDirty: true
      };
    case 'ADD_SEO_KEYWORD':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][17]++;
      cov_2pld42bkgj().s[30]++;
      if (!state.seoKeywords.includes(action.keyword)) {
        /* istanbul ignore next */
        cov_2pld42bkgj().b[2][0]++;
        cov_2pld42bkgj().s[31]++;
        return {
          ...state,
          seoKeywords: [...state.seoKeywords, action.keyword],
          isDirty: true
        };
      } else
      /* istanbul ignore next */
      {
        cov_2pld42bkgj().b[2][1]++;
      }
      cov_2pld42bkgj().s[32]++;
      return state;
    case 'REMOVE_SEO_KEYWORD':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][18]++;
      cov_2pld42bkgj().s[33]++;
      return {
        ...state,
        seoKeywords: state.seoKeywords.filter((_, index) => {
          /* istanbul ignore next */
          cov_2pld42bkgj().f[7]++;
          cov_2pld42bkgj().s[34]++;
          return index !== action.index;
        }),
        isDirty: true
      };
    case 'RESET_FORM':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][19]++;
      cov_2pld42bkgj().s[35]++;
      return initialState;
    case 'LOAD_PRODUCT':
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][20]++;
      cov_2pld42bkgj().s[36]++;
      return {
        ...state,
        ...action.product,
        isDirty: false
      };
    default:
      /* istanbul ignore next */
      cov_2pld42bkgj().b[0][21]++;
      cov_2pld42bkgj().s[37]++;
      return state;
  }
}
