      }
    };
  }, [state]));
  /* istanbul ignore next */
  cov_2pld42bkgj().s[68]++;
  return {
    state,
    actions,
    selectors
  };
}
function useProductFormValidation() {
  /* istanbul ignore next */
  cov_2pld42bkgj().f[34]++;
  const {
    state,
    actions,
    selectors
  } =
  /* istanbul ignore next */
  (cov_2pld42bkgj().s[69]++, useProductFormReducer());
  const validateField =
  /* istanbul ignore next */
  (cov_2pld42bkgj().s[70]++, (0, _react.useCallback)((field, value) => {
    /* istanbul ignore next */
    cov_2pld42bkgj().f[35]++;
    let error =
    /* istanbul ignore next */
    (cov_2pld42bkgj().s[71]++, '');
    /* istanbul ignore next */
    cov_2pld42bkgj().s[72]++;
    switch (field) {
      case 'name':
        /* istanbul ignore next */
        cov_2pld42bkgj().b[10][0]++;
        cov_2pld42bkgj().s[73]++;
        if (
        /* istanbul ignore next */
        (cov_2pld42bkgj().b[12][0]++, !value) ||
        /* istanbul ignore next */
        (cov_2pld42bkgj().b[12][1]++, value.trim().length === 0)) {
          /* istanbul ignore next */
          cov_2pld42bkgj().b[11][0]++;
          cov_2pld42bkgj().s[74]++;
          error = 'El nombre es requerido';
        } else {
          /* istanbul ignore next */
          cov_2pld42bkgj().b[11][1]++;
          cov_2pld42bkgj().s[75]++;
          if (value.length > 255) {
            /* istanbul ignore next */
            cov_2pld42bkgj().b[13][0]++;
            cov_2pld42bkgj().s[76]++;
            error = 'Mximo 255 caracteres';
          } else
          /* istanbul ignore next */
          {
            cov_2pld42bkgj().b[13][1]++;
          }
        }
        /* istanbul ignore next */
        cov_2pld42bkgj().s[77]++;
        break;
      case 'price':
        /* istanbul ignore next */
        cov_2pld42bkgj().b[10][1]++;
        cov_2pld42bkgj().s[78]++;
        if (value <= 0) {
          /* istanbul ignore next */
          cov_2pld42bkgj().b[14][0]++;
          cov_2pld42bkgj().s[79]++;
          error = 'El precio debe ser mayor a 0';
        } else {
          /* istanbul ignore next */
          cov_2pld42bkgj().b[14][1]++;
          cov_2pld42bkgj().s[80]++;
          if (value > 999999.99) {
            /* istanbul ignore next */
            cov_2pld42bkgj().b[15][0]++;
            cov_2pld42bkgj().s[81]++;
            error = 'Precio mximo excedido';
          } else
          /* istanbul ignore next */
          {
            cov_2pld42bkgj().b[15][1]++;
          }
        }
        /* istanbul ignore next */
        cov_2pld42bkgj().s[82]++;
        break;
      case 'comparePrice':
        /* istanbul ignore next */
        cov_2pld42bkgj().b[10][2]++;
        cov_2pld42bkgj().s[83]++;
        if (
        /* istanbul ignore next */
        (cov_2pld42bkgj().b[17][0]++, value > 0) &&
        /* istanbul ignore next */
        (cov_2pld42bkgj().b[17][1]++, value < state.price)) {
          /* istanbul ignore next */
          cov_2pld42bkgj().b[16][0]++;
          cov_2pld42bkgj().s[84]++;
          error = 'El precio de comparacin debe ser mayor al precio base';
        } else
        /* istanbul ignore next */
        {
          cov_2pld42bkgj().b[16][1]++;
        }
        cov_2pld42bkgj().s[85]++;
        break;
      case 'costPrice':
        /* istanbul ignore next */
        cov_2pld42bkgj().b[10][3]++;
        cov_2pld42bkgj().s[86]++;
        if (
        /* istanbul ignore next */
        (cov_2pld42bkgj().b[19][0]++, value > 0) &&
        /* istanbul ignore next */
        (cov_2pld42bkgj().b[19][1]++, value > state.price)) {
          /* istanbul ignore next */
          cov_2pld42bkgj().b[18][0]++;
          cov_2pld42bkgj().s[87]++;
          error = 'El precio de costo debe ser menor al precio de venta';
        } else
        /* istanbul ignore next */
        {
          cov_2pld42bkgj().b[18][1]++;
        }
        cov_2pld42bkgj().s[88]++;
        break;
      case 'stock':
        /* istanbul ignore next */
        cov_2pld42bkgj().b[10][4]++;
        cov_2pld42bkgj().s[89]++;
        if (value < 0) {
          /* istanbul ignore next */
          cov_2pld42bkgj().b[20][0]++;
          cov_2pld42bkgj().s[90]++;
          error = 'El stock debe ser mayor o igual a 0';
        } else {
          /* istanbul ignore next */
          cov_2pld42bkgj().b[20][1]++;
          cov_2pld42bkgj().s[91]++;
          if (value > 999999) {
            /* istanbul ignore next */
            cov_2pld42bkgj().b[21][0]++;
            cov_2pld42bkgj().s[92]++;
            error = 'Stock mximo excedido';
          } else
          /* istanbul ignore next */
          {
            cov_2pld42bkgj().b[21][1]++;
          }
        }
        /* istanbul ignore next */
        cov_2pld42bkgj().s[93]++;
        break;
      case 'sku':
        /* istanbul ignore next */
        cov_2pld42bkgj().b[10][5]++;
        cov_2pld42bkgj().s[94]++;
        if (
        /* istanbul ignore next */
        (cov_2pld42bkgj().b[23][0]++, value) &&
        /* istanbul ignore next */
        (cov_2pld42bkgj().b[23][1]++, !/^[A-Z0-9\-_]+$/.test(value))) {
          /* istanbul ignore next */
          cov_2pld42bkgj().b[22][0]++;
          cov_2pld42bkgj().s[95]++;
          error = 'SKU debe contener solo letras maysculas, nmeros, guiones y guiones bajos';
        } else
        /* istanbul ignore next */
        {
          cov_2pld42bkgj().b[22][1]++;
        }
        cov_2pld42bkgj().s[96]++;
        break;
    }
    /* istanbul ignore next */
    cov_2pld42bkgj().s[97]++;
    if (error) {
      /* istanbul ignore next */
      cov_2pld42bkgj().b[24][0]++;
      cov_2pld42bkgj().s[98]++;
      actions.setError(field, error);
    } else {
      /* istanbul ignore next */
      cov_2pld42bkgj().b[24][1]++;
      cov_2pld42bkgj().s[99]++;
      actions.clearError(field);
    }
    /* istanbul ignore next */
    cov_2pld42bkgj().s[100]++;
    return error === '';
  }, [state.price, actions]));
  const validateForm =
  /* istanbul ignore next */
  (cov_2pld42bkgj().s[101]++, (0, _react.useCallback)(() => {
    /* istanbul ignore next */
    cov_2pld42bkgj().f[36]++;
    const fields =
    /* istanbul ignore next */
    (cov_2pld42bkgj().s[102]++, ['name', 'price', 'stock', 'categoryId']);
    let isValid =
    /* istanbul ignore next */
    (cov_2pld42bkgj().s[103]++, true);
    /* istanbul ignore next */
    cov_2pld42bkgj().s[104]++;
    fields.forEach(field => {
      /* istanbul ignore next */
      cov_2pld42bkgj().f[37]++;
      const fieldValid =
      /* istanbul ignore next */
      (cov_2pld42bkgj().s[105]++, validateField(field, state[field]));
      /* istanbul ignore next */
      cov_2pld42bkgj().s[106]++;
      if (!fieldValid) {
        /* istanbul ignore next */
        cov_2pld42bkgj().b[25][0]++;
        cov_2pld42bkgj().s[107]++;
        isValid = false;
      } else
      /* istanbul ignore next */
      {
        cov_2pld42bkgj().b[25][1]++;
      }
    });
    /* istanbul ignore next */
    cov_2pld42bkgj().s[108]++;
    return isValid;
  }, [state, validateField]));
  /* istanbul ignore next */
  cov_2pld42bkgj().s[109]++;
  return {
    state,
    actions,
    selectors,
    validateField,
    validateForm
  };
}
