    } else
    /* istanbul ignore next */
    {
      cov_29g3pif7wi().b[14][1]++;
    }
    const shipmentData =
    /* istanbul ignore next */
    (cov_29g3pif7wi().s[72]++, {
      carrier_id: carrierId,
      shipping_service: shippingService,
