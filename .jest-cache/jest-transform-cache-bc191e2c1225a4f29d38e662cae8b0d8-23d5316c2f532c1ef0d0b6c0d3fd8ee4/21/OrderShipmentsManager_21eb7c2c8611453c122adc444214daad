    className: "max-w-md",
    children: [/*#__PURE__*/(0, _jsxruntime.jsxs)(_dialog.DialogHeader, {
      children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_dialog.DialogTitle, {
        children: "Crear Nuevo Env\xedo"
