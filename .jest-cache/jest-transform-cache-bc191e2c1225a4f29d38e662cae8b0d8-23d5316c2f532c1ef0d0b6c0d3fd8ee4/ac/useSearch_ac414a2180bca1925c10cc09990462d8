const defaultTrendingSearches = [
    {
        id: 'trending-1',
        type: 'trending',
        title: 'Pintura l√°tex',
        href: '/search?q=pintura+latex'
    },
    {
        id: 'trending-2',
        type: 'trending',
        title: 'Sherwin Williams',
        href: '/search?q=sherwin+williams'
    },
    {
        id: 'trending-3',
        type: 'trending',
        title: 'Rodillos premium',
        href: '/search?q=rodillos+premium'
    },
    {
        id: 'trending-4',
        type: 'trending',
        title: 'Pinceles',
        href: '/search?q=pinceles'
