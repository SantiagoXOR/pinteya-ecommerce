    cov_156w7mk75h().s[12]++;
    try {
      /* istanbul ignore next */
      cov_156w7mk75h().s[13]++;
      await (0, _react.signIn)(provider, {
        callbackUrl:
        /* istanbul ignore next */
        (cov_156w7mk75h().b[1][0]++, options?.callbackUrl) ||
        /* istanbul ignore next */
        (cov_156w7mk75h().b[1][1]++, "/admin"),
        redirect: true
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_156w7mk75h().s[14]++;
      console.error("Error al iniciar sesiÃ³n:", error);
    }
  }, []));
  const handleSignOut =
  /* istanbul ignore next */
  (cov_156w7mk75h().s[15]++, useCallback(async options => {
    /* istanbul ignore next */
    cov_156w7mk75h().f[6]++;
    cov_156w7mk75h().s[16]++;
    try {
      /* istanbul ignore next */
      cov_156w7mk75h().s[17]++;
      await (0, _react.signOut)({
        callbackUrl:
        /* istanbul ignore next */
        (cov_156w7mk75h().b[2][0]++, options?.callbackUrl) ||
        /* istanbul ignore next */
        (cov_156w7mk75h().b[2][1]++, "/"),
        redirect: true
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_156w7mk75h().s[18]++;
      console.error("Error al cerrar sesiÃ³n:", error);
    }
  }, []));
  // Mapear usuario de NextAuth a nuestro formato
  const user =
  /* istanbul ignore next */
  (cov_156w7mk75h().s[19]++, session?.user ?
  /* istanbul ignore next */
  (cov_156w7mk75h().b[3][0]++, {
    id:
    /* istanbul ignore next */
    (cov_156w7mk75h().b[4][0]++, session.user.id) ||
    /* istanbul ignore next */
    (cov_156w7mk75h().b[4][1]++, session.user.email) ||
    /* istanbul ignore next */
    (cov_156w7mk75h().b[4][2]++, ''),
    name: session.user.name,
    email: session.user.email,
    image: session.user.image
  }) :
  /* istanbul ignore next */
  (cov_156w7mk75h().b[3][1]++, null));
  /* istanbul ignore next */
  cov_156w7mk75h().s[20]++;
  return {
    user,
    isLoaded: status !== "loading",
    isSignedIn: status === "authenticated",
    signIn: handleSignIn,
    signOut: handleSignOut,
    session,
    status
  };
}
function useIsAdmin() {
