                                                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                            className: "flex-1",
                                                            children: [
                                                                /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                                    className: "font-medium",
                                                                    children: alert.message
                                                                }),
                                                                /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                                                    className: "text-sm text-muted-foreground",
                                                                    children: (0, _consolidatedutils.formatDate)(alert.timestamp)
                                                                })
                                                            ]
                                                        }),
                                                        /*#__PURE__*/ (0, _jsxruntime.jsx)(_badge.Badge, {
                                                            variant: "outline",
                                                            children: alert.type
                                                        })
                                                    ]
                                                }, index)),
                                            alerts.length === 0 && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                className: "text-center py-8",
                                                children: [
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.Clock, {
                                                        className: "w-8 h-8 text-gray-400 mx-auto mb-2"
                                                    }),
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                                        className: "text-gray-500",
                                                        children: "No hay eventos recientes"
                                                    })
                                                ]
                                            })
                                        ]
                                    })
                                })
                            ]
                        })
                    })
                ]
            })
        ]
    });
}
