async function validateAdminAuth() {
  /* istanbul ignore next */
  cov_2d2wy0gjnr().f[4]++;
  const session =
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().s[21]++, await (0, _config.auth)());
  /* istanbul ignore next */
  cov_2d2wy0gjnr().s[22]++;
  if (!session?.user) {
    /* istanbul ignore next */
    cov_2d2wy0gjnr().b[0][0]++;
    cov_2d2wy0gjnr().s[23]++;
    return {
      error: 'No autorizado',
      status: 401
    };
  } else
  /* istanbul ignore next */
  {
    cov_2d2wy0gjnr().b[0][1]++;
  }
  // Verificar rol de administrador o manager
  const {
    data: profile
  } =
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().s[24]++, await supabase.from('profiles').select('role').eq('id', session.user.id).single());
  /* istanbul ignore next */
  cov_2d2wy0gjnr().s[25]++;
  if (!['admin', 'manager'].includes(profile?.role)) {
    /* istanbul ignore next */
    cov_2d2wy0gjnr().b[1][0]++;
    cov_2d2wy0gjnr().s[26]++;
    return {
      error: 'Acceso denegado',
      status: 403
    };
  } else
  /* istanbul ignore next */
  {
    cov_2d2wy0gjnr().b[1][1]++;
  }
  cov_2d2wy0gjnr().s[27]++;
  return {
    userId: session.user.id,
    role: profile.role
  };
}
async function getPromotions(filters) {
  /* istanbul ignore next */
  cov_2d2wy0gjnr().f[5]++;
  let query =
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().s[28]++, supabase.from('promotions').select(`
      *,
      categories:promotion_categories!promotion_categories_promotion_id_fkey(
        category:categories!promotion_categories_category_id_fkey(
          id,
          name
        )
      ),
      products:promotion_products!promotion_products_promotion_id_fkey(
        product:products!promotion_products_product_id_fkey(
          id,
          name,
          sku
        )
      ),
      brands:promotion_brands!promotion_brands_promotion_id_fkey(
        brand:brands!promotion_brands_brand_id_fkey(
          id,
          name
        )
      ),
      creator:profiles!promotions_created_by_fkey(
        full_name,
        email
      )
    `));
  // Aplicar filtros
  /* istanbul ignore next */
  cov_2d2wy0gjnr().s[29]++;
  if (filters.status) {
    /* istanbul ignore next */
    cov_2d2wy0gjnr().b[2][0]++;
    const now =
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().s[30]++, new Date().toISOString());
    /* istanbul ignore next */
    cov_2d2wy0gjnr().s[31]++;
    switch (filters.status) {
      case 'active':
        /* istanbul ignore next */
        cov_2d2wy0gjnr().b[3][0]++;
        cov_2d2wy0gjnr().s[32]++;
        query = query.eq('is_active', true).lte('starts_at', now).or(`ends_at.is.null,ends_at.gt.${now}`);
        /* istanbul ignore next */
        cov_2d2wy0gjnr().s[33]++;
        break;
      case 'inactive':
        /* istanbul ignore next */
        cov_2d2wy0gjnr().b[3][1]++;
        cov_2d2wy0gjnr().s[34]++;
        query = query.eq('is_active', false);
        /* istanbul ignore next */
        cov_2d2wy0gjnr().s[35]++;
        break;
      case 'scheduled':
        /* istanbul ignore next */
        cov_2d2wy0gjnr().b[3][2]++;
        cov_2d2wy0gjnr().s[36]++;
        query = query.eq('is_active', true).gt('starts_at', now);
        /* istanbul ignore next */
        cov_2d2wy0gjnr().s[37]++;
        break;
      case 'expired':
        /* istanbul ignore next */
        cov_2d2wy0gjnr().b[3][3]++;
        cov_2d2wy0gjnr().s[38]++;
        query = query.eq('is_active', true).not('ends_at', 'is', null).lt('ends_at', now);
        /* istanbul ignore next */
        cov_2d2wy0gjnr().s[39]++;
        break;
      case 'paused':
        /* istanbul ignore next */
        cov_2d2wy0gjnr().b[3][4]++;
        cov_2d2wy0gjnr().s[40]++;
        query = query.eq('is_paused', true);
        /* istanbul ignore next */
        cov_2d2wy0gjnr().s[41]++;
        break;
    }
  } else
  /* istanbul ignore next */
  {
    cov_2d2wy0gjnr().b[2][1]++;
  }
  cov_2d2wy0gjnr().s[42]++;
  if (filters.type) {
    /* istanbul ignore next */
    cov_2d2wy0gjnr().b[4][0]++;
    cov_2d2wy0gjnr().s[43]++;
    query = query.eq('type', filters.type);
  } else
  /* istanbul ignore next */
  {
    cov_2d2wy0gjnr().b[4][1]++;
  }
  cov_2d2wy0gjnr().s[44]++;
  if (filters.search) {
    /* istanbul ignore next */
    cov_2d2wy0gjnr().b[5][0]++;
    cov_2d2wy0gjnr().s[45]++;
    query = query.or(`name.ilike.%${filters.search}%,description.ilike.%${filters.search}%,banner_text.ilike.%${filters.search}%`);
  } else
  /* istanbul ignore next */
  {
    cov_2d2wy0gjnr().b[5][1]++;
  }
  cov_2d2wy0gjnr().s[46]++;
  if (filters.date_from) {
    /* istanbul ignore next */
    cov_2d2wy0gjnr().b[6][0]++;
    cov_2d2wy0gjnr().s[47]++;
    query = query.gte('created_at', filters.date_from);
  } else
  /* istanbul ignore next */
  {
    cov_2d2wy0gjnr().b[6][1]++;
  }
  cov_2d2wy0gjnr().s[48]++;
  if (filters.date_to) {
    /* istanbul ignore next */
    cov_2d2wy0gjnr().b[7][0]++;
    cov_2d2wy0gjnr().s[49]++;
    query = query.lte('created_at', filters.date_to);
  } else
  /* istanbul ignore next */
  {
    cov_2d2wy0gjnr().b[7][1]++;
  }
  // Contar total
  const {
    count
  } =
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().s[50]++, await query.select('*', {
    count: 'exact',
    head: true
  }));
  // Aplicar paginaciÃ³n y ordenamiento
  const offset =
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().s[51]++, (filters.page - 1) * filters.limit);
  /* istanbul ignore next */
  cov_2d2wy0gjnr().s[52]++;
  query = query.order(filters.sort_by, {
    ascending: filters.sort_order === 'asc'
  }).range(offset, offset + filters.limit - 1);
  const {
    data,
    error
  } =
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().s[53]++, await query);
  /* istanbul ignore next */
  cov_2d2wy0gjnr().s[54]++;
  if (error) {
    /* istanbul ignore next */
    cov_2d2wy0gjnr().b[8][0]++;
    cov_2d2wy0gjnr().s[55]++;
    throw new Error(`Error al obtener promociones: ${error.message}`);
  } else
  /* istanbul ignore next */
  {
    cov_2d2wy0gjnr().b[8][1]++;
  }
  // Procesar datos para incluir estado calculado
  const processedData =
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().s[56]++, (
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().b[9][0]++, data) ||
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().b[9][1]++, [])).map(promotion => {
    /* istanbul ignore next */
    cov_2d2wy0gjnr().f[6]++;
    const now =
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().s[57]++, new Date());
    const startsAt =
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().s[58]++, new Date(promotion.starts_at));
    const endsAt =
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().s[59]++, promotion.ends_at ?
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().b[10][0]++, new Date(promotion.ends_at)) :
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().b[10][1]++, null));
    let status;
    /* istanbul ignore next */
    cov_2d2wy0gjnr().s[60]++;
    if (promotion.is_paused) {
      /* istanbul ignore next */
      cov_2d2wy0gjnr().b[11][0]++;
      cov_2d2wy0gjnr().s[61]++;
      status = 'paused';
    } else {
      /* istanbul ignore next */
      cov_2d2wy0gjnr().b[11][1]++;
      cov_2d2wy0gjnr().s[62]++;
      if (!promotion.is_active) {
        /* istanbul ignore next */
        cov_2d2wy0gjnr().b[12][0]++;
        cov_2d2wy0gjnr().s[63]++;
        status = 'inactive';
      } else {
        /* istanbul ignore next */
        cov_2d2wy0gjnr().b[12][1]++;
        cov_2d2wy0gjnr().s[64]++;
        if (now < startsAt) {
          /* istanbul ignore next */
          cov_2d2wy0gjnr().b[13][0]++;
          cov_2d2wy0gjnr().s[65]++;
          status = 'scheduled';
        } else {
          /* istanbul ignore next */
          cov_2d2wy0gjnr().b[13][1]++;
          cov_2d2wy0gjnr().s[66]++;
          if (
          /* istanbul ignore next */
          (cov_2d2wy0gjnr().b[15][0]++, endsAt) &&
          /* istanbul ignore next */
          (cov_2d2wy0gjnr().b[15][1]++, now > endsAt)) {
            /* istanbul ignore next */
            cov_2d2wy0gjnr().b[14][0]++;
            cov_2d2wy0gjnr().s[67]++;
            status = 'expired';
          } else {
            /* istanbul ignore next */
            cov_2d2wy0gjnr().b[14][1]++;
            cov_2d2wy0gjnr().s[68]++;
            status = 'active';
          }
        }
      }
    }
    /* istanbul ignore next */
    cov_2d2wy0gjnr().s[69]++;
    return {
      ...promotion,
      status,
      categories:
      /* istanbul ignore next */
      (cov_2d2wy0gjnr().b[16][0]++, promotion.categories?.map(pc => {
        /* istanbul ignore next */
        cov_2d2wy0gjnr().f[7]++;
        cov_2d2wy0gjnr().s[70]++;
        return pc.category;
      })) ||
      /* istanbul ignore next */
      (cov_2d2wy0gjnr().b[16][1]++, []),
      products:
      /* istanbul ignore next */
      (cov_2d2wy0gjnr().b[17][0]++, promotion.products?.map(pp => {
        /* istanbul ignore next */
        cov_2d2wy0gjnr().f[8]++;
        cov_2d2wy0gjnr().s[71]++;
        return pp.product;
      })) ||
      /* istanbul ignore next */
      (cov_2d2wy0gjnr().b[17][1]++, []),
      brands:
      /* istanbul ignore next */
      (cov_2d2wy0gjnr().b[18][0]++, promotion.brands?.map(pb => {
        /* istanbul ignore next */
        cov_2d2wy0gjnr().f[9]++;
        cov_2d2wy0gjnr().s[72]++;
        return pb.brand;
      })) ||
      /* istanbul ignore next */
      (cov_2d2wy0gjnr().b[18][1]++, [])
    };
  }));
  /* istanbul ignore next */
  cov_2d2wy0gjnr().s[73]++;
  return {
    promotions: processedData,
    total:
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().b[19][0]++, count) ||
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().b[19][1]++, 0),
    totalPages: Math.ceil((
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().b[20][0]++, count) ||
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().b[20][1]++, 0)) / filters.limit)
  };
}
async function createPromotion(promotionData, userId) {
  /* istanbul ignore next */
  cov_2d2wy0gjnr().f[10]++;
  // Validar fechas
  const startsAt =
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().s[74]++, new Date(promotionData.starts_at));
  const endsAt =
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().s[75]++, promotionData.ends_at ?
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().b[21][0]++, new Date(promotionData.ends_at)) :
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().b[21][1]++, null));
  /* istanbul ignore next */
  cov_2d2wy0gjnr().s[76]++;
  if (
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().b[23][0]++, endsAt) &&
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().b[23][1]++, startsAt >= endsAt)) {
    /* istanbul ignore next */
    cov_2d2wy0gjnr().b[22][0]++;
    cov_2d2wy0gjnr().s[77]++;
    throw new Error('La fecha de inicio debe ser anterior a la fecha de finalizaciÃ³n');
  } else
  /* istanbul ignore next */
  {
    cov_2d2wy0gjnr().b[22][1]++;
  }
  // Validar configuraciÃ³n segÃºn el tipo
  cov_2d2wy0gjnr().s[78]++;
  switch (promotionData.type) {
    case 'percentage_discount':
      /* istanbul ignore next */
      cov_2d2wy0gjnr().b[24][0]++;
      cov_2d2wy0gjnr().s[79]++;
      if (!promotionData.discount_percentage) {
        /* istanbul ignore next */
        cov_2d2wy0gjnr().b[25][0]++;
        cov_2d2wy0gjnr().s[80]++;
        throw new Error('El porcentaje de descuento es requerido para este tipo de promociÃ³n');
      } else
      /* istanbul ignore next */
      {
        cov_2d2wy0gjnr().b[25][1]++;
      }
      cov_2d2wy0gjnr().s[81]++;
      break;
    case 'fixed_discount':
      /* istanbul ignore next */
      cov_2d2wy0gjnr().b[24][1]++;
      cov_2d2wy0gjnr().s[82]++;
      if (!promotionData.discount_amount) {
        /* istanbul ignore next */
        cov_2d2wy0gjnr().b[26][0]++;
        cov_2d2wy0gjnr().s[83]++;
        throw new Error('El monto de descuento es requerido para este tipo de promociÃ³n');
      } else
      /* istanbul ignore next */
      {
        cov_2d2wy0gjnr().b[26][1]++;
      }
      cov_2d2wy0gjnr().s[84]++;
      break;
    case 'buy_x_get_y':
      /* istanbul ignore next */
      cov_2d2wy0gjnr().b[24][2]++;
      cov_2d2wy0gjnr().s[85]++;
      if (
      /* istanbul ignore next */
      (cov_2d2wy0gjnr().b[28][0]++, !promotionData.buy_quantity) ||
      /* istanbul ignore next */
      (cov_2d2wy0gjnr().b[28][1]++, !promotionData.get_quantity)) {
        /* istanbul ignore next */
        cov_2d2wy0gjnr().b[27][0]++;
        cov_2d2wy0gjnr().s[86]++;
        throw new Error('Las cantidades de compra y obsequio son requeridas para este tipo de promociÃ³n');
      } else
      /* istanbul ignore next */
      {
        cov_2d2wy0gjnr().b[27][1]++;
      }
      cov_2d2wy0gjnr().s[87]++;
      break;
    case 'bundle_deal':
      /* istanbul ignore next */
      cov_2d2wy0gjnr().b[24][3]++;
      cov_2d2wy0gjnr().s[88]++;
      if (
      /* istanbul ignore next */
      (cov_2d2wy0gjnr().b[30][0]++, !promotionData.bundle_products?.length) ||
      /* istanbul ignore next */
      (cov_2d2wy0gjnr().b[30][1]++, !promotionData.bundle_price)) {
        /* istanbul ignore next */
        cov_2d2wy0gjnr().b[29][0]++;
        cov_2d2wy0gjnr().s[89]++;
        throw new Error('Los productos del bundle y el precio son requeridos para este tipo de promociÃ³n');
      } else
      /* istanbul ignore next */
      {
        cov_2d2wy0gjnr().b[29][1]++;
      }
      cov_2d2wy0gjnr().s[90]++;
      break;
  }
  // Validar cÃ³digo de cupÃ³n si es requerido
  /* istanbul ignore next */
  cov_2d2wy0gjnr().s[91]++;
  if (promotionData.requires_coupon_code) {
    /* istanbul ignore next */
    cov_2d2wy0gjnr().b[31][0]++;
    cov_2d2wy0gjnr().s[92]++;
    if (!promotionData.coupon_code) {
      /* istanbul ignore next */
      cov_2d2wy0gjnr().b[32][0]++;
      cov_2d2wy0gjnr().s[93]++;
      throw new Error('El cÃ³digo de cupÃ³n es requerido cuando se habilita esta opciÃ³n');
    } else
    /* istanbul ignore next */
    {
      cov_2d2wy0gjnr().b[32][1]++;
    }
