}
async function GET(request) {
  /* istanbul ignore next */
  cov_2d2wy0gjnr().f[39]++;
  const startTime =
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().s[190]++, Date.now());
  /* istanbul ignore next */
  cov_2d2wy0gjnr().s[191]++;
  try {
    // Rate limiting
    const rateLimitResult =
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().s[192]++, await (0, _ratelimiter.checkRateLimit)(request, {
      windowMs: RATE_LIMIT_CONFIGS.admin.windowMs,
      maxRequests: RATE_LIMIT_CONFIGS.admin.maxRequests,
      message: RATE_LIMIT_CONFIGS.admin.message
    }, 'admin-promotions'));
    /* istanbul ignore next */
    cov_2d2wy0gjnr().s[193]++;
    if (!rateLimitResult.success) {
      /* istanbul ignore next */
      cov_2d2wy0gjnr().b[82][0]++;
      const response =
      /* istanbul ignore next */
      (cov_2d2wy0gjnr().s[194]++, _server.NextResponse.json({
        error: rateLimitResult.message
      }, {
        status: 429
