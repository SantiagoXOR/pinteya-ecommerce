    const {
      data: existingPromotion
    } =
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().s[94]++, await supabase.from('promotions').select('id').eq('coupon_code', promotionData.coupon_code).single());
    /* istanbul ignore next */
    cov_2d2wy0gjnr().s[95]++;
    if (existingPromotion) {
      /* istanbul ignore next */
      cov_2d2wy0gjnr().b[33][0]++;
      cov_2d2wy0gjnr().s[96]++;
      throw new Error('Ya existe una promociÃ³n con este cÃ³digo de cupÃ³n');
    } else
    /* istanbul ignore next */
    {
      cov_2d2wy0gjnr().b[33][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_2d2wy0gjnr().b[31][1]++;
  }
  // Preparar datos para inserciÃ³n
  const {
    category_ids,
    product_ids,
    brand_ids,
    bundle_products,
    ...promotionInsertData
  } =
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().s[97]++, promotionData);
  // Crear promociÃ³n
  const {
    data: newPromotion,
    error: promotionError
  } =
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().s[98]++, await supabase.from('promotions').insert({
    ...promotionInsertData,
    usage_count: 0,
    created_by: userId,
    created_at: new Date().toISOString(),
    updated_at: new Date().toISOString()
  }).select().single());
  /* istanbul ignore next */
  cov_2d2wy0gjnr().s[99]++;
  if (promotionError) {
    /* istanbul ignore next */
    cov_2d2wy0gjnr().b[34][0]++;
    cov_2d2wy0gjnr().s[100]++;
    throw new Error(`Error al crear promociÃ³n: ${promotionError.message}`);
  } else
  /* istanbul ignore next */
  {
    cov_2d2wy0gjnr().b[34][1]++;
  }
  // Asociar categorÃ­as si aplica
  cov_2d2wy0gjnr().s[101]++;
  if (
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().b[36][0]++, promotionData.applicable_to === 'categories') &&
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().b[36][1]++, category_ids?.length)) {
    /* istanbul ignore next */
    cov_2d2wy0gjnr().b[35][0]++;
    const categoryInserts =
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().s[102]++, category_ids.map(categoryId => {
      /* istanbul ignore next */
      cov_2d2wy0gjnr().f[11]++;
      cov_2d2wy0gjnr().s[103]++;
      return {
        promotion_id: newPromotion.id,
        category_id: categoryId
      };
    }));
    const {
      error: categoryError
    } =
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().s[104]++, await supabase.from('promotion_categories').insert(categoryInserts));
    /* istanbul ignore next */
    cov_2d2wy0gjnr().s[105]++;
    if (categoryError) {
      /* istanbul ignore next */
      cov_2d2wy0gjnr().b[37][0]++;
      cov_2d2wy0gjnr().s[106]++;
      throw new Error(`Error al asociar categorÃ­as: ${categoryError.message}`);
    } else
    /* istanbul ignore next */
    {
      cov_2d2wy0gjnr().b[37][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_2d2wy0gjnr().b[35][1]++;
  }
  // Asociar productos si aplica
  cov_2d2wy0gjnr().s[107]++;
  if (
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().b[39][0]++, promotionData.applicable_to === 'products') &&
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().b[39][1]++, product_ids?.length)) {
    /* istanbul ignore next */
    cov_2d2wy0gjnr().b[38][0]++;
    const productInserts =
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().s[108]++, product_ids.map(productId => {
      /* istanbul ignore next */
      cov_2d2wy0gjnr().f[12]++;
      cov_2d2wy0gjnr().s[109]++;
      return {
        promotion_id: newPromotion.id,
        product_id: productId
      };
    }));
    const {
      error: productError
    } =
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().s[110]++, await supabase.from('promotion_products').insert(productInserts));
    /* istanbul ignore next */
    cov_2d2wy0gjnr().s[111]++;
    if (productError) {
      /* istanbul ignore next */
      cov_2d2wy0gjnr().b[40][0]++;
      cov_2d2wy0gjnr().s[112]++;
      throw new Error(`Error al asociar productos: ${productError.message}`);
    } else
    /* istanbul ignore next */
    {
      cov_2d2wy0gjnr().b[40][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_2d2wy0gjnr().b[38][1]++;
  }
  // Asociar marcas si aplica
  cov_2d2wy0gjnr().s[113]++;
  if (
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().b[42][0]++, promotionData.applicable_to === 'brands') &&
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().b[42][1]++, brand_ids?.length)) {
    /* istanbul ignore next */
    cov_2d2wy0gjnr().b[41][0]++;
    const brandInserts =
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().s[114]++, brand_ids.map(brandId => {
      /* istanbul ignore next */
      cov_2d2wy0gjnr().f[13]++;
      cov_2d2wy0gjnr().s[115]++;
      return {
        promotion_id: newPromotion.id,
        brand_id: brandId
      };
    }));
    const {
      error: brandError
    } =
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().s[116]++, await supabase.from('promotion_brands').insert(brandInserts));
    /* istanbul ignore next */
    cov_2d2wy0gjnr().s[117]++;
    if (brandError) {
      /* istanbul ignore next */
      cov_2d2wy0gjnr().b[43][0]++;
      cov_2d2wy0gjnr().s[118]++;
      throw new Error(`Error al asociar marcas: ${brandError.message}`);
    } else
    /* istanbul ignore next */
    {
      cov_2d2wy0gjnr().b[43][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_2d2wy0gjnr().b[41][1]++;
  }
  // Crear productos del bundle si aplica
  cov_2d2wy0gjnr().s[119]++;
  if (
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().b[45][0]++, promotionData.type === 'bundle_deal') &&
  /* istanbul ignore next */
  (cov_2d2wy0gjnr().b[45][1]++, bundle_products?.length)) {
    /* istanbul ignore next */
    cov_2d2wy0gjnr().b[44][0]++;
    const bundleInserts =
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().s[120]++, bundle_products.map(item => {
      /* istanbul ignore next */
      cov_2d2wy0gjnr().f[14]++;
      cov_2d2wy0gjnr().s[121]++;
      return {
        promotion_id: newPromotion.id,
        product_id: item.product_id,
        quantity: item.quantity
      };
    }));
    const {
      error: bundleError
    } =
    /* istanbul ignore next */
    (cov_2d2wy0gjnr().s[122]++, await supabase.from('promotion_bundle_products').insert(bundleInserts));
    /* istanbul ignore next */
    cov_2d2wy0gjnr().s[123]++;
    if (bundleError) {
      /* istanbul ignore next */
      cov_2d2wy0gjnr().b[46][0]++;
      cov_2d2wy0gjnr().s[124]++;
      throw new Error(`Error al crear bundle de productos: ${bundleError.message}`);
    } else
    /* istanbul ignore next */
    {
      cov_2d2wy0gjnr().b[46][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_2d2wy0gjnr().b[44][1]++;
  }
  cov_2d2wy0gjnr().s[125]++;
