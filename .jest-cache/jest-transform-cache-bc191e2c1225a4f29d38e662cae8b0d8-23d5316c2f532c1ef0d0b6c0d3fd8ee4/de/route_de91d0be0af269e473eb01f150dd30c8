        /* istanbul ignore next */
        {
          cov_267vgkv1vh().b[16][1]++;
        }
        const destination =
        /* istanbul ignore next */
        (cov_267vgkv1vh().s[60]++, shipment.destination?.coordinates);
        /* istanbul ignore next */
        cov_267vgkv1vh().s[61]++;
        if (!destination) {
          /* istanbul ignore next */
          cov_267vgkv1vh().b[17][0]++;
