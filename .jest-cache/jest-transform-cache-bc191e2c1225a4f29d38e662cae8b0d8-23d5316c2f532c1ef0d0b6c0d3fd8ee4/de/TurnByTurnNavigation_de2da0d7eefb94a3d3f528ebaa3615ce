        (/*#__PURE__*/cov_1jfi5dl7u4().b[35][1]++, (0, _jsxruntime.jsx)(_lucidereact.VolumeX, {
          className: "w-4 h-4"
        }))
      })]
    }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
      className: "p-4",
      children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "flex items-start space-x-4",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "flex-shrink-0",
          children: getManeuverIcon(currentStep.maneuver)
        }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          className: "flex-1",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
            className: "text-lg font-semibold text-gray-900 mb-2",
            children: currentStep.instruction
          }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "flex items-center space-x-4 text-sm text-gray-600",
            children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
              className: "flex items-center",
              children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.MapPin, {
                className: "w-4 h-4 mr-1"
              }), /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
                children: currentStep.distance
              })]
            }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
              className: "flex items-center",
              children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.Clock, {
                className: "w-4 h-4 mr-1"
              }), /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
                children: currentStep.duration
              })]
            })]
          })]
        })]
      }),
      /* istanbul ignore next */
      (cov_1jfi5dl7u4().b[36][0]++, nextStepPreview) &&
      /* istanbul ignore next */
      (cov_1jfi5dl7u4().b[36][1]++, /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "mt-4 pt-4 border-t border-gray-100",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "text-sm text-gray-500 mb-2",
          children: "Siguiente:"
        }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          className: "flex items-center space-x-3",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
            className: "flex-shrink-0",
            children: getManeuverIcon(nextStepPreview.maneuver)
          }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
            className: "text-sm text-gray-700",
            children: nextStepPreview.instruction
          })]
        })]
      })), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "mt-4 flex justify-between",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("button", {
          onClick: previousStep,
          disabled: currentStepIndex === 0,
          className: (0, _utils.cn)("px-4 py-2 rounded-lg text-sm font-medium transition-colors", currentStepIndex === 0 ?
          /* istanbul ignore next */
          (cov_1jfi5dl7u4().b[37][0]++, "bg-gray-100 text-gray-400 cursor-not-allowed") :
          /* istanbul ignore next */
          (cov_1jfi5dl7u4().b[37][1]++, "bg-gray-100 text-gray-700 hover:bg-gray-200")),
          children: "Anterior"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("button", {
          onClick: nextStep,
          disabled: currentStepIndex === navigationSteps.length - 1,
          className: (0, _utils.cn)("px-4 py-2 rounded-lg text-sm font-medium transition-colors", currentStepIndex === navigationSteps.length - 1 ?
          /* istanbul ignore next */
          (cov_1jfi5dl7u4().b[38][0]++, "bg-gray-100 text-gray-400 cursor-not-allowed") :
          /* istanbul ignore next */
          (cov_1jfi5dl7u4().b[38][1]++, "bg-blue-600 text-white hover:bg-blue-700")),
          children: "Siguiente"
        })]
      }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "mt-4",
        children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          className: "flex justify-between text-xs text-gray-500 mb-1",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)("span", {
            children: "Progreso"
          }), /*#__PURE__*/(0, _jsxruntime.jsxs)("span", {
            children: [Math.round((currentStepIndex + 1) / navigationSteps.length * 100), "%"]
          })]
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "w-full bg-gray-200 rounded-full h-2",
          children: /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
            className: "bg-blue-600 h-2 rounded-full transition-all duration-300",
            style: {
              width: `${(currentStepIndex + 1) / navigationSteps.length * 100}%`
            }
          })
        })]
      })]
    })]
  });
}
