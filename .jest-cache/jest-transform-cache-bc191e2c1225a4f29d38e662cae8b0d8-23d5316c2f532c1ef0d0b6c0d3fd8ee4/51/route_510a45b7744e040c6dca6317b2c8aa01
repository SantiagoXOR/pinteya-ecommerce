    const {
      testId,
      variantId,
      metrics
    } =
    /* istanbul ignore next */
    (cov_19xv1lwg91().s[43]++, body);
    // Validar parámetros requeridos
    /* istanbul ignore next */
    cov_19xv1lwg91().s[44]++;
    if (
    /* istanbul ignore next */
    (cov_19xv1lwg91().b[10][0]++, !testId) ||
