function calculateDistance(lat1, lng1, lat2, lng2) {
  /* istanbul ignore next */
  cov_262mwjmmmz().f[1]++;
  const R =
  /* istanbul ignore next */
  (cov_262mwjmmmz().s[5]++, 6371); // Radio de la Tierra en km
  const dLat =
  /* istanbul ignore next */
  (cov_262mwjmmmz().s[6]++, (lat2 - lat1) * Math.PI / 180);
  const dLng =
  /* istanbul ignore next */
  (cov_262mwjmmmz().s[7]++, (lng2 - lng1) * Math.PI / 180);
  const a =
  /* istanbul ignore next */
  (cov_262mwjmmmz().s[8]++, Math.sin(dLat / 2) * Math.sin(dLat / 2) + Math.cos(lat1 * Math.PI / 180) * Math.cos(lat2 * Math.PI / 180) * Math.sin(dLng / 2) * Math.sin(dLng / 2));
  const c =
  /* istanbul ignore next */
  (cov_262mwjmmmz().s[9]++, 2 * Math.atan2(Math.sqrt(a), Math.sqrt(1 - a)));
  /* istanbul ignore next */
  cov_262mwjmmmz().s[10]++;
  return R * c;
}
