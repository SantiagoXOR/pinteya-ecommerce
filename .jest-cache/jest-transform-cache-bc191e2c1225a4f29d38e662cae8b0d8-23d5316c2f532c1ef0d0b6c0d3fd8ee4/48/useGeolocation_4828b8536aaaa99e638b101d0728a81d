        /* istanbul ignore next */
        cov_262mwjmmmz().s[62]++;
        if (result.state === 'granted') {
          /* istanbul ignore next */
          cov_262mwjmmmz().b[7][0]++;
        } // No solicitar automáticamente para evitar bucles infinitos
        else {
          /* istanbul ignore next */
          cov_262mwjmmmz().b[7][1]++;
          cov_262mwjmmmz().s[63]++;
          if (result.state === 'prompt') {
            /* istanbul ignore next */
            cov_262mwjmmmz().b[8][0]++;
          } // No solicitar automáticamente si es 'prompt' para evitar popup inesperado
          else {
            /* istanbul ignore next */
            cov_262mwjmmmz().b[8][1]++;
          }
        }
      }).catch(error => {
        /* istanbul ignore next */
        cov_262mwjmmmz().f[19]++;
        cov_262mwjmmmz().s[64]++;
        setState(prev => {
          /* istanbul ignore next */
          cov_262mwjmmmz().f[20]++;
          cov_262mwjmmmz().s[65]++;
          return {
            ...prev,
            permissionStatus: 'unknown'
          };
        });
      });
    } else {
      /* istanbul ignore next */
      cov_262mwjmmmz().b[6][1]++;
      cov_262mwjmmmz().s[66]++;
      setState(prev => {
        /* istanbul ignore next */
        cov_262mwjmmmz().f[21]++;
        cov_262mwjmmmz().s[67]++;
        return {
          ...prev,
          permissionStatus: 'unknown'
        };
      });
    }
  }, []); // Sin dependencias para evitar bucles
  // Función para obtener todas las zonas disponibles
  const getAvailableZones =
  /* istanbul ignore next */
  (cov_262mwjmmmz().s[68]++, (0, _react.useCallback)(() => {
    /* istanbul ignore next */
    cov_262mwjmmmz().f[22]++;
    cov_262mwjmmmz().s[69]++;
    return DELIVERY_ZONES;
  }, []));
  // Función para seleccionar zona manualmente
  const selectZone =
  /* istanbul ignore next */
  (cov_262mwjmmmz().s[70]++, (0, _react.useCallback)(zoneId => {
    /* istanbul ignore next */
    cov_262mwjmmmz().f[23]++;
    const zone =
    /* istanbul ignore next */
    (cov_262mwjmmmz().s[71]++, DELIVERY_ZONES.find(z => {
      /* istanbul ignore next */
      cov_262mwjmmmz().f[24]++;
      cov_262mwjmmmz().s[72]++;
      return z.id === zoneId;
    }));
    /* istanbul ignore next */
    cov_262mwjmmmz().s[73]++;
    if (zone) {
      /* istanbul ignore next */
      cov_262mwjmmmz().b[9][0]++;
      cov_262mwjmmmz().s[74]++;
      setState(prev => {
        /* istanbul ignore next */
        cov_262mwjmmmz().f[25]++;
        cov_262mwjmmmz().s[75]++;
        return {
          ...prev,
          detectedZone: zone
        };
      });
    } else
    /* istanbul ignore next */
    {
      cov_262mwjmmmz().b[9][1]++;
    }
  }, []));
  // Función de test para simular geolocalización exitosa
  const testLocation =
  /* istanbul ignore next */
  (cov_262mwjmmmz().s[76]++, (0, _react.useCallback)((lat =
  /* istanbul ignore next */
  (cov_262mwjmmmz().b[10][0]++, -31.4201), lng =
  /* istanbul ignore next */
  (cov_262mwjmmmz().b[11][0]++, -64.1888)) => {
    /* istanbul ignore next */
    cov_262mwjmmmz().f[26]++;
    const detectedZone =
    /* istanbul ignore next */
    (cov_262mwjmmmz().s[77]++, detectNearestZone(lat, lng));
    /* istanbul ignore next */
    cov_262mwjmmmz().s[78]++;
    setState(prev => {
      /* istanbul ignore next */
      cov_262mwjmmmz().f[27]++;
      const newState =
      /* istanbul ignore next */
      (cov_262mwjmmmz().s[79]++, {
        ...prev,
        location: {
          lat,
          lng
        },
        detectedZone,
        isLoading: false,
        permissionStatus: 'granted',
        error: null
      });
      /* istanbul ignore next */
      cov_262mwjmmmz().s[80]++;
      return newState;
    });
  }, []));
  /* istanbul ignore next */
  cov_262mwjmmmz().s[81]++;
  return {
    ...state,
    requestLocation,
    getAvailableZones,
    selectZone,
    deliveryZones: DELIVERY_ZONES,
    testLocation
  };
};
