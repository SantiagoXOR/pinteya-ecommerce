  }, [colors, productType]));
  // Obtener categorías únicas basadas en los colores disponibles
  const categories =
  /* istanbul ignore next */
  (cov_2b8mhamdr2().s[61]++, (0, _react.useMemo)(() => {
    /* istanbul ignore next */
    cov_2b8mhamdr2().f[13]++;
