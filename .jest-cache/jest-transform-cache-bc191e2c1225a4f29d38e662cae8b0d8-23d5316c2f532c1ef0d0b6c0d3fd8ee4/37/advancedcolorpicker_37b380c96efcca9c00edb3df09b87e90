const getCategoryDisplayName = category => {
  /* istanbul ignore next */
  cov_2b8mhamdr2().f[7]++;
  const categoryNames =
  /* istanbul ignore next */
  (cov_2b8mhamdr2().s[40]++, {
    'all': 'Todos',
    'Madera': 'Madera',
    'Sintético': 'Sintético',
    'Neutros': 'Neutros',
    'Cálidos': 'Cálidos',
    'Fríos': 'Fríos',
    'Tierras': 'Tierras'
  });
  /* istanbul ignore next */
  cov_2b8mhamdr2().s[41]++;
  return /* istanbul ignore next */(cov_2b8mhamdr2().b[15][0]++, categoryNames[category]) ||
  /* istanbul ignore next */
  (cov_2b8mhamdr2().b[15][1]++, category);
};
