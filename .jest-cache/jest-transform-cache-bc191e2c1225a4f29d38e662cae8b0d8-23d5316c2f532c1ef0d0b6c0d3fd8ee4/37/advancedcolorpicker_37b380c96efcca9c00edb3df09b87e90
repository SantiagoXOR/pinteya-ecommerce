  const [showAllColors, setShowAllColors] =
  /* istanbul ignore next */
  (cov_2b8mhamdr2().s[45]++, (0, _react.useState)(false));
  // Filtrar colores según el tipo de producto
  const availableColors =
  /* istanbul ignore next */
  (cov_2b8mhamdr2().s[46]++, (0, _react.useMemo)(() => {
    /* istanbul ignore next */
    cov_2b8mhamdr2().f[9]++;
    cov_2b8mhamdr2().s[47]++;
    console.log('🎨 AdvancedColorPicker - Debug Info:', {
      productType,
      hasProductType: !!productType,
      allowedColorCategories: productType?.allowedColorCategories,
      totalColors: colors.length
    });
    /* istanbul ignore next */
    cov_2b8mhamdr2().s[48]++;
    if (
    /* istanbul ignore next */
    (cov_2b8mhamdr2().b[22][0]++, !productType) ||
    /* istanbul ignore next */
    (cov_2b8mhamdr2().b[22][1]++, !productType.allowedColorCategories)) {
      /* istanbul ignore next */
      cov_2b8mhamdr2().b[21][0]++;
      cov_2b8mhamdr2().s[49]++;
      console.log('⚠️ No hay restricciones de color, mostrando todos los colores');
      /* istanbul ignore next */
      cov_2b8mhamdr2().s[50]++;
      return colors; // Si no hay restricciones, mostrar todos los colores
    } else
    /* istanbul ignore next */
    {
      cov_2b8mhamdr2().b[21][1]++;
    }
    let filtered =
    /* istanbul ignore next */
    (cov_2b8mhamdr2().s[51]++, colors.filter(color => {
      /* istanbul ignore next */
      cov_2b8mhamdr2().f[10]++;
      cov_2b8mhamdr2().s[52]++;
      return productType.allowedColorCategories.includes(color.category);
    }));
    // Para productos de látex, excluir completamente los colores de madera
    const isLatexProduct =
    /* istanbul ignore next */
    (cov_2b8mhamdr2().s[53]++,
    /* istanbul ignore next */
    (cov_2b8mhamdr2().b[23][0]++, productType.allowedColorCategories.includes('Látex')) &&
    /* istanbul ignore next */
    (cov_2b8mhamdr2().b[23][1]++, productType.allowedColorCategories.length === 1));
    /* istanbul ignore next */
    cov_2b8mhamdr2().s[54]++;
    if (isLatexProduct) {
      /* istanbul ignore next */
      cov_2b8mhamdr2().b[24][0]++;
      cov_2b8mhamdr2().s[55]++;
      filtered = filtered.filter(color => {
        /* istanbul ignore next */
        cov_2b8mhamdr2().f[11]++;
        cov_2b8mhamdr2().s[56]++;
        return color.category !== 'Madera';
      });
      /* istanbul ignore next */
      cov_2b8mhamdr2().s[57]++;
      console.log('🎨 Latex product detected - excluding wood colors');
    } else
    /* istanbul ignore next */
    {
      cov_2b8mhamdr2().b[24][1]++;
    }
    cov_2b8mhamdr2().s[58]++;
    console.log('✅ Colores filtrados:', {
      allowedCategories: productType.allowedColorCategories,
      filteredCount: filtered.length,
      originalCount: colors.length,
      filteredColors: filtered.map(c => {
        /* istanbul ignore next */
        cov_2b8mhamdr2().f[12]++;
        cov_2b8mhamdr2().s[59]++;
        return {
          name: c.displayName,
          category: c.category
        };
      })
    });
    /* istanbul ignore next */
    cov_2b8mhamdr2().s[60]++;
