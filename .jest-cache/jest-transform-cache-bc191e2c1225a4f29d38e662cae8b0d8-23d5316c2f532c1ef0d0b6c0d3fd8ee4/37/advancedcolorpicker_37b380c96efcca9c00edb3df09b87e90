const AdvancedColorPicker = ({
  colors =
  /* istanbul ignore next */
  (cov_2b8mhamdr2().b[16][0]++, PAINT_COLORS),
  selectedColor,
  onColorChange,
  showSearch =
  /* istanbul ignore next */
  (cov_2b8mhamdr2().b[17][0]++, true),
  showCategories =
  /* istanbul ignore next */
  (cov_2b8mhamdr2().b[18][0]++, true),
