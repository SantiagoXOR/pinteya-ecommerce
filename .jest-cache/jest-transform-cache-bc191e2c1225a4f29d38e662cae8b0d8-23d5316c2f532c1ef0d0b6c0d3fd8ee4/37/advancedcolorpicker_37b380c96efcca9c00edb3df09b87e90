    const sortedCats =
    /* istanbul ignore next */
    (cov_2b8mhamdr2().s[65]++, cats.sort((a, b) => {
      /* istanbul ignore next */
      cov_2b8mhamdr2().f[15]++;
      const indexA =
      /* istanbul ignore next */
      (cov_2b8mhamdr2().s[66]++, categoryOrder.indexOf(a));
      const indexB =
      /* istanbul ignore next */
      (cov_2b8mhamdr2().s[67]++, categoryOrder.indexOf(b));
      /* istanbul ignore next */
      cov_2b8mhamdr2().s[68]++;
      if (
      /* istanbul ignore next */
      (cov_2b8mhamdr2().b[26][0]++, indexA === -1) &&
      /* istanbul ignore next */
      (cov_2b8mhamdr2().b[26][1]++, indexB === -1)) {
        /* istanbul ignore next */
        cov_2b8mhamdr2().b[25][0]++;
        cov_2b8mhamdr2().s[69]++;
        return a.localeCompare(b);
      } else
      /* istanbul ignore next */
      {
        cov_2b8mhamdr2().b[25][1]++;
      }
      cov_2b8mhamdr2().s[70]++;
      if (indexA === -1) {
        /* istanbul ignore next */
        cov_2b8mhamdr2().b[27][0]++;
        cov_2b8mhamdr2().s[71]++;
        return 1;
      } else
      /* istanbul ignore next */
      {
        cov_2b8mhamdr2().b[27][1]++;
      }
      cov_2b8mhamdr2().s[72]++;
      if (indexB === -1) {
        /* istanbul ignore next */
        cov_2b8mhamdr2().b[28][0]++;
        cov_2b8mhamdr2().s[73]++;
        return -1;
      } else
      /* istanbul ignore next */
      {
        cov_2b8mhamdr2().b[28][1]++;
      }
      cov_2b8mhamdr2().s[74]++;
      return indexA - indexB;
    }));
    // Para productos sintéticos, solo mostrar la categoría Sintético (sin "Todos")
    /* istanbul ignore next */
    cov_2b8mhamdr2().s[75]++;
    if (
    /* istanbul ignore next */
    (cov_2b8mhamdr2().b[30][0]++, productType?.allowedColorCategories?.includes('Sintético')) &&
    /* istanbul ignore next */
    (cov_2b8mhamdr2().b[30][1]++, productType.allowedColorCategories.length === 1)) {
      /* istanbul ignore next */
      cov_2b8mhamdr2().b[29][0]++;
      cov_2b8mhamdr2().s[76]++;
