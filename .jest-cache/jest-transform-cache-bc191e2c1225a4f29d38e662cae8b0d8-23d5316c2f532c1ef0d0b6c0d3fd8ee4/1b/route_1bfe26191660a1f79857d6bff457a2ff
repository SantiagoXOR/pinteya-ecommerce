        (cov_i46hn4sbr().b[14][1]++, 'Checkout con errores')
      });
      // Log de validaciÃ³n exitosa
      /* istanbul ignore next */
      cov_i46hn4sbr().s[51]++;
      securityLogger.logEvent('checkout_validation', 'low', {
        isValid: validationResults.isValid,
        errorsCount: validationResults.errors.length,
        warningsCount: validationResults.warnings.length,
        total: validatedData.totals.total,
        paymentMethod: validatedData.paymentMethod
      });
      /* istanbul ignore next */
      cov_i46hn4sbr().s[52]++;
      return _server.NextResponse.json(response, {
        status: validationResults.isValid ?
        /* istanbul ignore next */
        (cov_i46hn4sbr().b[15][0]++, 200) :
        /* istanbul ignore next */
        (cov_i46hn4sbr().b[15][1]++, 400)
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_i46hn4sbr().s[53]++;
      console.error('â Error validando checkout:', error);
      // Log de error de seguridad
      /* istanbul ignore next */
      cov_i46hn4sbr().s[54]++;
      securityLogger.logEvent('checkout_validation_error', 'medium', {
        error: error instanceof Error ?
        /* istanbul ignore next */
        (cov_i46hn4sbr().b[16][0]++, error.message) :
        /* istanbul ignore next */
        (cov_i46hn4sbr().b[16][1]++, 'Unknown error'),
        endpoint: '/api/checkout/validate'
      });
      /* istanbul ignore next */
      cov_i46hn4sbr().s[55]++;
      if (error instanceof _zod.z.ZodError) {
        /* istanbul ignore next */
        cov_i46hn4sbr().b[17][0]++;
        cov_i46hn4sbr().s[56]++;
        return _server.NextResponse.json({
          success: false,
          error: 'Datos de entrada invÃ¡lidos',
          details: error.errors.map(err => {
            /* istanbul ignore next */
            cov_i46hn4sbr().f[7]++;
            cov_i46hn4sbr().s[57]++;
            return {
              field: err.path.join('.'),
              message: err.message
            };
          })
        }, {
          status: 400
        });
      } else
      /* istanbul ignore next */
      {
        cov_i46hn4sbr().b[17][1]++;
      }
      cov_i46hn4sbr().s[58]++;
      return _server.NextResponse.json({
        success: false,
        error: 'Error interno del servidor'
      }, {
        status: 500
      });
    }
  }));
  // Manejar rate limit excedido
  /* istanbul ignore next */
  cov_i46hn4sbr().s[59]++;
  if (rateLimitResult instanceof _server.NextResponse) {
    /* istanbul ignore next */
    cov_i46hn4sbr().b[18][0]++;
    cov_i46hn4sbr().s[60]++;
    securityLogger.logRateLimitExceeded(securityLogger.context, {
      endpoint: '/api/checkout/validate',
      method: 'POST'
    });
    /* istanbul ignore next */
    cov_i46hn4sbr().s[61]++;
    return rateLimitResult;
  } else
  /* istanbul ignore next */
  {
    cov_i46hn4sbr().b[18][1]++;
  }
  cov_i46hn4sbr().s[62]++;
  return rateLimitResult;
}
// FunciÃ³n auxiliar para calcular costo de envÃ­o
function calculateShippingCost(method, subtotal, state) {
  /* istanbul ignore next */
  cov_i46hn4sbr().f[8]++;
  cov_i46hn4sbr().s[63]++;
  if (method === 'pickup') {
    /* istanbul ignore next */
    cov_i46hn4sbr().b[19][0]++;
    cov_i46hn4sbr().s[64]++;
    return 0;
  } else
  /* istanbul ignore next */
  {
    cov_i46hn4sbr().b[19][1]++;
  }
  cov_i46hn4sbr().s[65]++;
  if (
  /* istanbul ignore next */
  (cov_i46hn4sbr().b[21][0]++, method === 'free') &&
  /* istanbul ignore next */
  (cov_i46hn4sbr().b[21][1]++, subtotal >= 25000)) {
    /* istanbul ignore next */
    cov_i46hn4sbr().b[20][0]++;
    cov_i46hn4sbr().s[66]++;
    return 0;
  } else
  /* istanbul ignore next */
  {
    cov_i46hn4sbr().b[20][1]++;
  }
  const baseCost =
  /* istanbul ignore next */
  (cov_i46hn4sbr().s[67]++, method === 'express' ?
  /* istanbul ignore next */
  (cov_i46hn4sbr().b[22][0]++, 5000) :
  /* istanbul ignore next */
  (cov_i46hn4sbr().b[22][1]++, 2500));
  // Costo adicional por provincia
  const stateSurcharge =
  /* istanbul ignore next */
  (cov_i46hn4sbr().s[68]++, state === 'CABA' ?
  /* istanbul ignore next */
  (cov_i46hn4sbr().b[23][0]++, 0) :
  /* istanbul ignore next */
  (cov_i46hn4sbr().b[23][1]++, 500));
  /* istanbul ignore next */
  cov_i46hn4sbr().s[69]++;
  return baseCost + stateSurcharge;
}
// FunciÃ³n auxiliar para calcular fecha estimada de entrega
function calculateEstimatedDelivery(shippingMethod) {
  /* istanbul ignore next */
  cov_i46hn4sbr().f[9]++;
  const now =
  /* istanbul ignore next */
  (cov_i46hn4sbr().s[70]++, new Date());
  let daysToAdd =
  /* istanbul ignore next */
  (cov_i46hn4sbr().s[71]++, 7);
  /* istanbul ignore next */
  cov_i46hn4sbr().s[72]++;
  switch (shippingMethod) {
    case 'express':
      /* istanbul ignore next */
      cov_i46hn4sbr().b[24][0]++;
      cov_i46hn4sbr().s[73]++;
      daysToAdd = 2;
      /* istanbul ignore next */
      cov_i46hn4sbr().s[74]++;
      break;
    case 'free':
      /* istanbul ignore next */
      cov_i46hn4sbr().b[24][1]++;
      cov_i46hn4sbr().s[75]++;
      daysToAdd = 7;
      /* istanbul ignore next */
      cov_i46hn4sbr().s[76]++;
      break;
    case 'pickup':
      /* istanbul ignore next */
      cov_i46hn4sbr().b[24][2]++;
      cov_i46hn4sbr().s[77]++;
      daysToAdd = 1;
      /* istanbul ignore next */
      cov_i46hn4sbr().s[78]++;
      break;
  }
  const estimatedDate =
  /* istanbul ignore next */
  (cov_i46hn4sbr().s[79]++, new Date(now.getTime() + daysToAdd * 24 * 60 * 60 * 1000));
  /* istanbul ignore next */
  cov_i46hn4sbr().s[80]++;
  return estimatedDate.toISOString().split('T')[0]; // Solo fecha
}
