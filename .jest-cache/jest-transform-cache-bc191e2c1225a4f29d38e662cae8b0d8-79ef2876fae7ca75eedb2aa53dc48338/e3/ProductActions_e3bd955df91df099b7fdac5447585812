                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("label", {
                                            className: "block text-sm font-medium text-gray-700 mb-2",
                                            children: "Ajuste de precio"
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                            className: "flex space-x-2",
                                            children: [
                                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("select", {
                                                    value: bulkEditData.priceChange.type,
                                                    onChange: (e)=>setBulkEditData((prev)=>({
                                                                ...prev,
                                                                priceChange: {
                                                                    ...prev.priceChange,
                                                                    type: e.target.value
                                                                }
                                                            })),
                                                    className: "px-3 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-blaze-orange-500 focus:border-transparent",
                                                    children: [
                                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("option", {
                                                            value: "percentage",
                                                            children: "Porcentaje"
                                                        }),
                                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("option", {
                                                            value: "fixed",
                                                            children: "Cantidad fija"
                                                        })
                                                    ]
                                                }),
                                                /*#__PURE__*/ (0, _jsxruntime.jsx)("input", {
                                                    type: "number",
                                                    value: bulkEditData.priceChange.value,
                                                    onChange: (e)=>setBulkEditData((prev)=>({
                                                                ...prev,
                                                                priceChange: {
                                                                    ...prev.priceChange,
                                                                    value: parseFloat(e.target.value) || 0
                                                                }
                                                            })),
                                                    className: "flex-1 px-3 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-blaze-orange-500 focus:border-transparent",
                                                    placeholder: "0"
                                                })
                                            ]
                                        })
                                    ]
                                })
                            ]
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                            className: "flex justify-end space-x-3 mt-6",
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                                    onClick: ()=>setShowBulkEditModal(false),
                                    className: "px-4 py-2 text-sm text-gray-700 bg-white border border-gray-300 hover:bg-gray-50 rounded-lg transition-colors",
                                    children: "Cancelar"
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                                    onClick: ()=>{
                                        if (onBulkEdit && selectedProducts.length > 0) {
                                            const updates = {};
                                            if (bulkEditData.status) updates.status = bulkEditData.status;
                                            if (bulkEditData.categoryId) {
                                                onBulkCategoryChange?.(selectedProducts.map((p)=>p.id), parseInt(bulkEditData.categoryId));
                                            }
                                            if (bulkEditData.priceChange.value !== 0) {
                                                onBulkPriceUpdate?.(selectedProducts.map((p)=>p.id), bulkEditData.priceChange);
                                            }
                                            onBulkEdit(selectedProducts.map((p)=>p.id), updates);
                                        }
                                        setShowBulkEditModal(false);
                                        setBulkEditData({
                                            status: '',
                                            categoryId: '',
                                            priceChange: {
                                                type: 'percentage',
                                                value: 0
                                            }
                                        });
                                    },
                                    className: "px-4 py-2 text-sm text-white bg-blaze-orange-600 hover:bg-blaze-orange-700 rounded-lg transition-colors",
                                    children: "Aplicar cambios"
                                })
                            ]
                        })
                    ]
                })
            })
        ]
    });
}
function ProductRowActions({ product, onEdit, onView, onDelete, onDuplicate, onToggleStatus, onManageInventory, isLoading = false }) {
    const notifications = (0, _useProductNotifications.useProductNotifications)();
    const [showActions, setShowActions] = (0, _react.useState)(false);
    const handleEdit = async ()=>{
        if (onEdit) {
            try {
                await onEdit(product.id);
                notifications.updateSuccess(product.name);
                setShowActions(false);
            } catch (error) {
                notifications.updateError(error);
            }
        }
    };
    const handleDelete = async ()=>{
        if (onDelete) {
            try {
                await onDelete(product.id);
                notifications.deleteSuccess(product.name);
                setShowActions(false);
            } catch (error) {
                notifications.deleteError(error);
            }
        }
    };
    const handleDuplicate = async ()=>{
        if (onDuplicate) {
            try {
                await onDuplicate(product.id);
                notifications.duplicateSuccess(product.name);
                setShowActions(false);
            } catch (error) {
                notifications.duplicateError(error);
            }
        }
    };
    const handleToggleStatus = async ()=>{
        if (onToggleStatus) {
            try {
                await onToggleStatus(product.id);
                const newStatus = product.status === 'active' ? 'inactive' : 'active';
                notifications.statusChangeSuccess(product.name, newStatus);
                setShowActions(false);
            } catch (error) {
                notifications.statusChangeError(error);
            }
        }
    };
    const handleManageInventory = async ()=>{
        if (onManageInventory) {
            try {
                await onManageInventory(product.id);
                notifications.inventoryUpdateSuccess(product.name);
                setShowActions(false);
            } catch (error) {
                notifications.inventoryUpdateError(error);
            }
        }
    };
    return /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
        className: "relative",
        children: [
            /*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                onClick: ()=>setShowActions(!showActions),
                className: "p-1 rounded hover:bg-gray-100 transition-colors",
                children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.MoreHorizontal, {
                    className: "w-4 h-4 text-gray-500"
                })
            }),
            showActions && /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                className: "absolute right-0 top-full mt-1 w-48 bg-white border border-gray-200 rounded-lg shadow-lg z-50",
                children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                    className: "py-1",
                    children: [
                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("button", {
                            onClick: ()=>{
                                onView?.(product.id);
                                setShowActions(false);
                            },
                            className: "flex items-center space-x-2 w-full px-4 py-2 text-sm text-gray-700 hover:bg-gray-50",
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.Eye, {
                                    className: "w-4 h-4"
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                    children: "Ver detalles"
                                })
                            ]
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("button", {
                            onClick: handleEdit,
                            className: "flex items-center space-x-2 w-full px-4 py-2 text-sm text-gray-700 hover:bg-gray-50",
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.Edit, {
                                    className: "w-4 h-4"
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                    children: "Editar"
                                })
                            ]
                        }),
                        onToggleStatus && /*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                            onClick: handleToggleStatus,
                            className: "flex items-center space-x-2 w-full px-4 py-2 text-sm text-gray-700 hover:bg-gray-50",
                            children: product.status === 'active' ? /*#__PURE__*/ (0, _jsxruntime.jsxs)(_jsxruntime.Fragment, {
                                children: [
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.XCircle, {
                                        className: "w-4 h-4 text-red-500"
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                        children: "Desactivar"
                                    })
                                ]
                            }) : /*#__PURE__*/ (0, _jsxruntime.jsxs)(_jsxruntime.Fragment, {
                                children: [
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.CheckCircle, {
                                        className: "w-4 h-4 text-green-500"
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                        children: "Activar"
                                    })
                                ]
                            })
                        }),
                        onManageInventory && /*#__PURE__*/ (0, _jsxruntime.jsxs)("button", {
                            onClick: handleManageInventory,
                            className: "flex items-center space-x-2 w-full px-4 py-2 text-sm text-gray-700 hover:bg-gray-50",
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.Package, {
                                    className: "w-4 h-4 text-blue-500"
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                    children: "Gestionar stock"
                                })
                            ]
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("button", {
                            onClick: handleDuplicate,
                            className: "flex items-center space-x-2 w-full px-4 py-2 text-sm text-gray-700 hover:bg-gray-50",
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.Copy, {
                                    className: "w-4 h-4"
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                    children: "Duplicar"
                                })
                            ]
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                            className: "border-t border-gray-200 my-1"
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("button", {
                            onClick: handleDelete,
                            disabled: isLoading,
                            className: "flex items-center space-x-2 w-full px-4 py-2 text-sm text-red-700 hover:bg-red-50 disabled:opacity-50",
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.Trash2, {
                                    className: "w-4 h-4"
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                    children: "Eliminar"
                                })
                            ]
                        })
                    ]
                })
            })
        ]
    });
}

