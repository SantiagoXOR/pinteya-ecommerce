                className: "w-4 h-4"
            });
        case "outOfStock":
            return /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.XCircle, {
                className: "w-4 h-4"
            });
        case "preOrder":
            return /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.Package, {
