        // // An치lisis de patrones
        // const patternInterval = setInterval(async () => {
        //   try {
        //     await analyzeSecurityPatterns();
        //   } catch (error) {
        //     console.error('[ENTERPRISE_AUDIT] Error en an치lisis de patrones:', error);
