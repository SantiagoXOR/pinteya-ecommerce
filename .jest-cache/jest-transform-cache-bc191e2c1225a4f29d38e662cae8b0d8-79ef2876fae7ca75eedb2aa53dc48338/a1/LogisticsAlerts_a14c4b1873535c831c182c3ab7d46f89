function LogisticsAlertsSummary({ alerts }) {
    const unreadAlerts = alerts.filter((alert)=>!alert.is_read);
    const criticalCount = unreadAlerts.filter((alert)=>alert.type === 'error').length;
    const warningCount = unreadAlerts.filter((alert)=>alert.type === 'warning').length;
    if (unreadAlerts.length === 0) {
        return /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
            className: "flex items-center gap-2 text-sm text-green-600",
            children: [
                /*#__PURE__*/ (0, _jsxruntime.jsx)(CheckCircle, {
                    className: "w-4 h-4"
                }),
                "Sin alertas pendientes"
            ]
        });
    }
    return /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
        className: "flex items-center gap-3",
        children: [
            criticalCount > 0 && /*#__PURE__*/ (0, _jsxruntime.jsxs)(_badge.Badge, {
                variant: "destructive",
                className: "flex items-center gap-1",
                children: [
                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.AlertTriangle, {
                        className: "w-3 h-3"
                    }),
                    criticalCount
                ]
            }),
            warningCount > 0 && /*#__PURE__*/ (0, _jsxruntime.jsxs)(_badge.Badge, {
                variant: "secondary",
                className: "flex items-center gap-1",
                children: [
                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.AlertCircle, {
                        className: "w-3 h-3"
                    }),
                    warningCount
                ]
            }),
            /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                className: "text-sm text-muted-foreground",
                children: [
                    unreadAlerts.length,
                    " alertas pendientes"
                ]
            })
        ]
    });
}

