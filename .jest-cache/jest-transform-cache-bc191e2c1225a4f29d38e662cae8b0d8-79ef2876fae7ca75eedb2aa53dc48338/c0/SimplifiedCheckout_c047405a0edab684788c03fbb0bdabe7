                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                    className: (0, _utils.cn)("flex items-center gap-1", currentStep === 'confirmation' && "text-primary font-medium"),
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                            className: (0, _utils.cn)("w-6 h-6 rounded-full flex items-center justify-center text-xs", currentStep === 'confirmation' ? "bg-primary text-white" : "bg-gray-200"),
                                            children: "2"
                                        }),
                                        "Confirmaci\xf3n"
                                    ]
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                    className: "w-8 h-px bg-gray-300"
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                    className: (0, _utils.cn)("flex items-center gap-1", currentStep === 'processing' && "text-primary font-medium"),
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                            className: (0, _utils.cn)("w-6 h-6 rounded-full flex items-center justify-center text-xs", currentStep === 'processing' ? "bg-primary text-white" : "bg-gray-200"),
                                            children: "3"
                                        }),
                                        "Pago"
                                    ]
                                })
                            ]
                        })
                    ]
                }),
                currentStep === 'form' && renderFormStep(),
                currentStep === 'confirmation' && renderConfirmationStep(),
                currentStep === 'processing' && renderProcessingStep()
            ]
        })
    });
};
