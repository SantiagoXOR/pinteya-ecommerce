                                        discount: item.discountedPrice < item.price ? `${Math.round((item.price - item.discountedPrice) / item.price * 100)}%` : undefined,
                                        stock: item.quantity,
                                        productId: item.id,
                                        cta: `Total: $${(item.discountedPrice * item.quantity).toLocaleString()}`,
                                        onAddToCart: ()=>{
                                        // En checkout, no necesitamos agregar más al carrito
                                        },
                                        showCartAnimation: false,
                                        freeShipping: item.discountedPrice >= 15000,
                                        shippingText: item.discountedPrice >= 15000 ? "Envío gratis" : "En carrito"
                                    })
                                }, index))
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsx)(_separator.Separator, {}),
                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                            className: "space-y-2",
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                    className: "flex justify-between text-sm",
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                            children: "Subtotal"
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                            children: [
                                                "$",
                                                totalPrice.toLocaleString()
                                            ]
                                        })
                                    ]
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                    className: "flex justify-between text-sm",
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                            children: "Env\xedo"
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                            children: shippingCost === 0 ? /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                className: "text-green-600 font-medium",
                                                children: "Gratis"
                                            }) : `$${shippingCost.toLocaleString()}`
                                        })
                                    ]
                                }),
                                discount > 0 && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                    className: "flex justify-between text-sm text-green-600",
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                            children: "Descuento"
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                            children: [
                                                "-$",
                                                discount.toLocaleString()
                                            ]
                                        })
                                    ]
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsx)(_separator.Separator, {}),
                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                    className: "flex justify-between font-semibold text-lg",
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                            children: "Total"
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                            className: "text-primary",
                                            children: [
                                                "$",
                                                finalTotal ? finalTotal.toLocaleString() : '0'
                                            ]
                                        })
                                    ]
                                })
                            ]
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                            className: "bg-green-50 p-3 rounded-lg",
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                    className: "flex items-center gap-2 text-green-700 text-sm",
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.CheckCircle, {
                                            className: "w-4 h-4"
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                            className: "font-medium",
                                            children: "Compra protegida"
                                        })
                                    ]
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                    className: "text-xs text-green-600 mt-1",
                                    children: "Tu dinero est\xe1 protegido con MercadoPago"
                                })
                            ]
                        })
                    ]
                })
            ]
        });
    // Renderizar paso de confirmación
    const renderConfirmationStep = ()=>/*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
            className: "max-w-4xl mx-auto",
            children: [
                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                    className: "text-center mb-8",
                    children: [
                        /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.CheckCircle, {
                            className: "w-16 h-16 text-green-500 mx-auto mb-4"
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsx)("h2", {
                            className: "text-2xl font-bold text-gray-900 mb-2",
                            children: "\xa1Confirma tu Pedido!"
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                            className: "text-gray-600",
                            children: "Revisa todos los datos antes de proceder al pago"
                        })
                    ]
                }),
                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                    className: "grid grid-cols-1 lg:grid-cols-2 gap-8",
                    children: [
                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                            className: "space-y-6",
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsxs)(_card.Card, {
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)(_card.CardHeader, {
                                            children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_card.CardTitle, {
                                                className: "text-lg",
                                                children: "Informaci\xf3n Personal"
                                            })
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsxs)(_card.CardContent, {
                                            className: "space-y-2",
                                            children: [
                                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("p", {
                                                    children: [
                                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("strong", {
                                                            children: "Nombre:"
                                                        }),
                                                        " ",
                                                        formData.billing.firstName,
                                                        " ",
                                                        formData.billing.lastName
                                                    ]
                                                }),
                                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("p", {
                                                    children: [
                                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("strong", {
