                                                children: "M\xe9todos Seleccionados"
                                            })
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsxs)(_card.CardContent, {
                                            className: "space-y-3",
                                            children: [
                                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                    className: "flex items-center gap-2",
                                                    children: [
                                                        /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.Truck, {
                                                            className: "w-4 h-4 text-primary"
                                                        }),
                                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                            children: formData.shippingMethod === 'free' ? 'Envío Estándar (5-7 días)' : 'Envío Express (1-2 días)'
                                                        })
                                                    ]
                                                }),
                                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                    className: "flex items-center gap-2",
                                                    children: [
                                                        /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.CreditCard, {
                                                            className: "w-4 h-4 text-primary"
