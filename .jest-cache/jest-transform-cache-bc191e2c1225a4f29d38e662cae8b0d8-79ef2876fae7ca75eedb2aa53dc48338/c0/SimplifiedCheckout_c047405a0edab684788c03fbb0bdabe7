                    children: "Procesando tu Pedido"
                }),
                /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                    className: "text-gray-600 mb-4",
                    children: "Te estamos redirigiendo a MercadoPago para completar el pago..."
                }),
                /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                    className: "max-w-md mx-auto bg-blue-50 p-4 rounded-lg",
                    children: /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                        className: "text-sm text-blue-700",
                        children: "No cierres esta ventana. Ser\xe1s redirigido autom\xe1ticamente."
                    })
                })
            ]
        });
    // Mostrar errores generales
    if (errors.general) {
        return /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
            className: "max-w-2xl mx-auto text-center py-12",
            children: [
                /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.AlertCircle, {
                    className: "w-16 h-16 text-red-500 mx-auto mb-4"
                }),
                /*#__PURE__*/ (0, _jsxruntime.jsx)("h2", {
                    className: "text-2xl font-bold text-gray-900 mb-2",
                    children: "Error en el Checkout"
                }),
                /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                    className: "text-red-600 mb-4",
                    children: errors.general
                }),
                /*#__PURE__*/ (0, _jsxruntime.jsx)(_button.Button, {
                    onClick: ()=>window.location.reload(),
                    children: "Intentar Nuevamente"
                })
            ]
        });
    }
    return /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
        className: "min-h-screen bg-gray-50 py-8",
        children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
            className: "max-w-7xl mx-auto px-4 sm:px-6 lg:px-8",
            children: [
                /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                    className: "text-center mb-8",
                    children: [
                        /*#__PURE__*/ (0, _jsxruntime.jsx)("h1", {
                            className: "text-3xl font-bold text-gray-900 mb-2",
                            children: "Finalizar Compra"
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                            className: "flex items-center justify-center gap-4 text-sm text-gray-600",
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                    className: (0, _utils.cn)("flex items-center gap-1", currentStep === 'form' && "text-primary font-medium"),
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                            className: (0, _utils.cn)("w-6 h-6 rounded-full flex items-center justify-center text-xs", currentStep === 'form' ? "bg-primary text-white" : "bg-gray-200"),
                                            children: "1"
                                        }),
                                        "Datos"
                                    ]
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
