        /* istanbul ignore next */
        cov_ibg8xeq28().f[14]++;
        cov_ibg8xeq28().s[21]++;
        if (
        /* istanbul ignore next */
        (cov_ibg8xeq28().b[2][0]++, value !== undefined) &&
        /* istanbul ignore next */
        (cov_ibg8xeq28().b[2][1]++, value !== null)) {
          /* istanbul ignore next */
          cov_ibg8xeq28().b[1][0]++;
          cov_ibg8xeq28().s[22]++;
          searchParams.append(key, value.toString());
        } else
        /* istanbul ignore next */
        {
          cov_ibg8xeq28().b[1][1]++;
        }
      });
    } else
    /* istanbul ignore next */
    {
      cov_ibg8xeq28().b[0][1]++;
    }
    const url =
    /* istanbul ignore next */
    (cov_ibg8xeq28().s[23]++, `/api/products?${searchParams.toString()}`);
    const response =
    /* istanbul ignore next */
    (cov_ibg8xeq28().s[24]++, await fetch(url, {
      method: 'GET',
      headers: {
        'Content-Type': 'application/json'
      }
    }));
    // Usar parsing seguro de JSON
    const result =
    /* istanbul ignore next */
    (cov_ibg8xeq28().s[25]++, await (0, _jsonutils.safeApiResponseJson)(response));
    /* istanbul ignore next */
    cov_ibg8xeq28().s[26]++;
    if (!result.success) {
      /* istanbul ignore next */
      cov_ibg8xeq28().b[3][0]++;
      cov_ibg8xeq28().s[27]++;
      console.error(' JSON parsing failed:', result.error);
      // Return a fallback response instead of throwing
      /* istanbul ignore next */
      cov_ibg8xeq28().s[28]++;
      return {
        data: [],
        pagination: {
          page: 1,
          limit: 12,
          total: 0,
          totalPages: 0
        },
        success: false,
        message:
        /* istanbul ignore next */
        (cov_ibg8xeq28().b[4][0]++, result.error) ||
        /* istanbul ignore next */
        (cov_ibg8xeq28().b[4][1]++, 'Error loading products')
      };
    } else
    /* istanbul ignore next */
    {
      cov_ibg8xeq28().b[3][1]++;
    }
    cov_ibg8xeq28().s[29]++;
    if (!result.data) {
      /* istanbul ignore next */
      cov_ibg8xeq28().b[5][0]++;
      cov_ibg8xeq28().s[30]++;
      console.error(' Result data is null');
      // Return a fallback response instead of throwing
      /* istanbul ignore next */
      cov_ibg8xeq28().s[31]++;
      return {
        data: [],
        pagination: {
          page: 1,
          limit: 12,
          total: 0,
          totalPages: 0
        },
        success: false,
        message: 'Error: respuesta nula del servidor'
      };
    } else
    /* istanbul ignore next */
    {
      cov_ibg8xeq28().b[5][1]++;
    }
    cov_ibg8xeq28().s[32]++;
    return result.data;
  } catch (error) {
    /* istanbul ignore next */
    cov_ibg8xeq28().s[33]++;
    console.error(' Error obteniendo productos:', error);
    // Return a fallback response instead of throwing
    /* istanbul ignore next */
    cov_ibg8xeq28().s[34]++;
    return {
      data: [],
      pagination: {
        page: 1,
        limit: 12,
        total: 0,
        totalPages: 0
      },
      success: false,
      message: error instanceof Error ?
      /* istanbul ignore next */
      (cov_ibg8xeq28().b[6][0]++, error.message) :
      /* istanbul ignore next */
      (cov_ibg8xeq28().b[6][1]++, 'Error inesperado')
    };
  }
}
