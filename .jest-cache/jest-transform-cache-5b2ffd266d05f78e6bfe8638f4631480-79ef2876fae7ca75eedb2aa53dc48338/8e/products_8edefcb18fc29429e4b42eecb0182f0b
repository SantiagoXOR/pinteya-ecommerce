async function getDiscountedProducts(page =
/* istanbul ignore next */
(cov_ibg8xeq28().b[13][0]++, 1), limit =
/* istanbul ignore next */
(cov_ibg8xeq28().b[14][0]++, 12)) {
  /* istanbul ignore next */
  cov_ibg8xeq28().f[18]++;
  // Nota: Esto requerira un filtro adicional en la API
  // Por ahora, obtenemos todos los productos y filtramos en el frontend
  const response =
  /* istanbul ignore next */
  (cov_ibg8xeq28().s[45]++, await getProducts({
    page,
    limit
  }));
  // Filtrar productos con descuento
  const discountedProducts =
  /* istanbul ignore next */
  (cov_ibg8xeq28().s[46]++, response.data.filter(product => {
    /* istanbul ignore next */
    cov_ibg8xeq28().f[19]++;
    cov_ibg8xeq28().s[47]++;
    return /* istanbul ignore next */(cov_ibg8xeq28().b[15][0]++, product.discounted_price) &&
    /* istanbul ignore next */
    (cov_ibg8xeq28().b[15][1]++, product.discounted_price < product.price);
  }));
  /* istanbul ignore next */
  cov_ibg8xeq28().s[48]++;
  return {
    ...response,
    data: discountedProducts
  };
}
