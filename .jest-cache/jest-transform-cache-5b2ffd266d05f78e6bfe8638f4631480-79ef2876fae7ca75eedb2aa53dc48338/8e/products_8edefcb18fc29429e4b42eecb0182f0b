  }
}
function convertLegacyProduct(oldProduct) {
  /* istanbul ignore next */
  cov_ibg8xeq28().f[23]++;
  cov_ibg8xeq28().s[60]++;
  return {
    id: oldProduct.id,
    name: oldProduct.title,
    brand:
    /* istanbul ignore next */
    (cov_ibg8xeq28().b[22][0]++, oldProduct.brand) ||
    /* istanbul ignore next */
    (cov_ibg8xeq28().b[22][1]++, null),
    slug: oldProduct.title.toLowerCase().replace(/[^a-z0-9]+/g, '-'),
    description: `Producto ${oldProduct.title}`,
    price: oldProduct.price,
    discounted_price: oldProduct.discountedPrice,
    stock: 50,
    category_id: 1,
    images: oldProduct.imgs,
    created_at: new Date().toISOString(),
    updated_at: null
  };
}
function calculateDiscountPercentage(originalPrice, discountedPrice) {
  /* istanbul ignore next */
  cov_ibg8xeq28().f[24]++;
  cov_ibg8xeq28().s[61]++;
  if (
  /* istanbul ignore next */
  (cov_ibg8xeq28().b[24][0]++, !discountedPrice) ||
  /* istanbul ignore next */
  (cov_ibg8xeq28().b[24][1]++, discountedPrice >= originalPrice)) {
    /* istanbul ignore next */
    cov_ibg8xeq28().b[23][0]++;
    cov_ibg8xeq28().s[62]++;
    return 0;
  } else
  /* istanbul ignore next */
  {
    cov_ibg8xeq28().b[23][1]++;
  }
  cov_ibg8xeq28().s[63]++;
  return Math.round((originalPrice - discountedPrice) / originalPrice * 100);
}
function formatPrice(price, currency =
/* istanbul ignore next */
(cov_ibg8xeq28().b[25][0]++, 'ARS')) {
  /* istanbul ignore next */
  cov_ibg8xeq28().f[25]++;
  cov_ibg8xeq28().s[64]++;
  return new Intl.NumberFormat('es-AR', {
    style: 'currency',
    currency,
    minimumFractionDigits: 0,
    maximumFractionDigits: 2
  }).format(price);
}
