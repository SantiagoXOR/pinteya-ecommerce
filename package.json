    "test:admin:report": "playwright show-report",
    "test:auth": "node scripts/run-auth-tests.js",
    "test:auth:headed": "playwright test tests/e2e/auth/ --headed",
    "test:auth:report": "playwright show-report playwright-report/auth",
    "test:security": "npm run test:auth",
    "test:all": "npm run test && npm run test:admin && npm run test:auth",
    "test:api": "jest --testPathPattern=api",
    "test:components": "jest --testPathPattern=components",
    "test:hooks": "jest --testPathPattern=hooks",
