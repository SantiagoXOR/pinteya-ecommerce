    "security:auth-logs": "node scripts/auth-log-analyzer.js",
    "security:audit": "node scripts/run-security-audit.js",
    "security:monitor": "curl -X GET http://localhost:3000/api/security/monitor",
    "analyze-bundle-optimization": "node scripts/performance/analyze-bundle-optimization.js",
    "bundle-optimization:analyze": "node scripts/performance/analyze-bundle-optimization.js --verbose",
    "bundle-optimization:report": "node scripts/performance/analyze-bundle-optimization.js --output bundle-reports",
    "bundle-optimization:check": "node scripts/performance/analyze-bundle-optimization.js --no-report",
    "analyze:recharts": "node scripts/analyze-recharts-chunk.js",
    "analyze:chunks": "node scripts/analyze-chunks.js",
    "verify:recharts-imports": "node scripts/verify-recharts-imports.js",
    "verify:recharts-async": "node scripts/verify-recharts-async-loading.js",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "ANALYZE=true BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "ANALYZE=true BUNDLE_ANALYZE=browser npm run build",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:css": "node scripts/verify-css-optimization.js",
    "optimize:hero": "node scripts/compress-hero-images.js",
    "analyze:legacy": "ANALYZE=true npm run build && echo 'Revisa el reporte del bundle analyzer para identificar c√≥digo heredado'",
    "lighthouse": "lighthouse https://www.pinteya.com --view --throttling-method=simulate --throttling.cpuSlowdownMultiplier=4 --throttling.rttMs=150 --throttling.throughputKbps=1600 --screenEmulation.mobile=true --screenEmulation.width=412 --screenEmulation.height=915",
    "lighthouse:desktop": "lighthouse https://www.pinteya.com --view --throttling-method=simulate --throttling.cpuSlowdownMultiplier=1 --throttling.rttMs=40 --throttling.throughputKbps=10240",
    "lighthouse:json": "lighthouse https://www.pinteya.com --output=json --output-path=./lighthouse-report.json --throttling-method=simulate --throttling.cpuSlowdownMultiplier=4 --throttling.rttMs=150 --throttling.throughputKbps=1600 --screenEmulation.mobile=true --screenEmulation.width=412 --screenEmulation.height=915",
    "lighthouse:analyze": "node scripts/performance/analyze-lighthouse-results.js",
    "lighthouse:diagnostic": "node scripts/performance/lighthouse-diagnostic.js",
    "lighthouse:diagnostic:local": "node scripts/performance/lighthouse-diagnostic.js --local",
    "lighthouse:ci": "lhci autorun",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006"
  },
