    "chromatic": "npx chromatic --exit-zero-on-changes",
    "test:visual": "npm run chromatic",
    "test:a11y": "npm run storybook:test",
    "test:performance": "node scripts/lighthouse-performance.js",
    "test:quality": "node scripts/run-quality-tests.js",
    "test:animations": "node scripts/test-animations.js",
    "test:animations:unit": "node scripts/test-animations.js unit",
    "test:animations:integration": "node scripts/test-animations.js integration",
